urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms401# الخد الشريق فحبذا % % ياقوت ذاك الخط أو ريحانه % % 9(@)(@@) % ما مثله في الحسن إلا خط ذي % % نظم تأنق في البيان بنانه % % 10(@)(@@) % حسان بيت قريضه حتى إذا % % ذكر الولاء فانه سلمانه % % PageV01P2026 11(@)(@@) % نعم الفريد زهت لدي فنونه % % في الناظمين وأزهرت أفنانه % % 12(@)(@@) % إن قيل إن سمنديار لشخصه % % نسب فللعرب الخلاص لسانه % % 13(@)(@@) % مستبدع الألفاظ قد حصلت على % % رجحانها وعلوها أوزانه % % 14(@)(@@) % قل يا محمد فيه يسمع فنه % % قولا يطول إلى السهى كيوانه % % 15(@)(@@) % ها قد أجزتك طوع أمرك إن تجز % % إن الرفيع تجيزه أدوانه % % 16(@)(@@) % ان كنت سلطان القريض فانه % % لولاك لم ينفذ اذا سلطانه % % 17(@)(@@) % أعلام طرسك حيث سار وقصره % % من بيتك المعمور أو بستانه % % 18(@)(@@) % أمرت في الأشعار شعرك حاكما % متصرفا في أمرها ديوانه % % PageV01P2027 البحر : - 1(@)(@@) % هجرت بديع القول هجر المباين % % فلا بالمعالي لا ولا بالمعاين % % 2(@)(@@) % وكيف أعاني سجعه أو قرينة % % وقد فقدت مني أجل القرائن % % 3(@)(@@) % ثوب في مهاوي الترب كالتبر خالصا % فحققت أن الترب بعض المعادن % % 4(@)(@@) % فوالله ما أدري لحسن خلائق % تسح جفوني أم لخلق محاسن % % 5(@)(@@) % دفنتك يا شخص الحبيب وقد بدا % % لعينك حالي قلت إنك دافني % % 6(@)(@@) % كلانا على الأيام باك وانما % % أشد البلابين الحشا كل كامن % % 7(@)(@@) % إلى الله أشكو يوم فقدك انه % % علي ليوم الحشر يوم التغابن % % 8(@)(@@) % وكنت أخاف البين قبلك والنوى % % فأصبحت لا آسي على أثر بائن % % 9(@)(@@) % كأنك بادرت الرحيل تخوفا % علي من الحسن الذي هو فاتني % % 10(@)(@@) % فديتك من لي من سناك بلمحة % % وينزل بي من بعدها كل كائن % % PageV01P2028 11(@)(@@) % أأنسى قواما أثقف الحسن رمحه % % فما فيه من عيب يعد لطاعن % % 12(@)(@@) % ووجها حكى عن حسنه كل مقمر % % ولحظا روى عن طرفه كل شادن % % 13(@)(@@) % فوا أسفا حتى أوسد في الثرى % % ويدني الردى منا مقيما لظاعن % % 14(@)(@@) % وياليت شعري في القيامة هل أرى % % محاسنها ما بين تلك المواطن % % 15(@)(@@) % رشاقة ذاك القد فوق صراطه % % ودينار ذاك الخد بين الموازن % % 16(@)(@@) % سقتك غوادي المزن أني ظامىء % % إلى الترب طوعا للزمان المحارن % % 17(@)(@@) % شكوت زمانا خان بعد أحبتي % % وبالغ في العدوى وبث الضغائن % % 18(@)(@@) % فلو طاب لي طابت حياتي بعدهم % % وكنت ألاقيهم بطلعة خائن % % PageV01P2029 البحر : - 1(@)(@@) % وعد 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms402# الفتى بلسانه % % دين على إحسانه % % 2(@)(@@) % حق عليه وفاؤه % % في وقته ومكانه % % 3(@)(@@) % مطل الفتى عار وحا % % شاك من تبيانه % % 4(@)(@@) % سيما اذا ما كان في المعه % % ود من إمكانه % % 5(@)(@@) % والسعد من خدامه % % والنجح من أعوانه % % 6(@)(@@) % واليمن تابع قصده % % والجود طي بنانه % % 7(@)(@@) % والمستحق الزي لا % % يعني الكريم بشأنه % % 8(@)(@@) % يشكو له ظمأ ولا % % يلوي على ظمآنه % % 9(@)(@@) % حتى يقول بغيظه % % أواه من عدوانه % % 10(@)(@@) % هذا الصغار بعينه % % وبعونه وعيانه % % PageV01P2030 البحر : - 1(@)(@@) % لنجم هلال الدولة الحسن عسكر % % حوى كل قاص في الجمال وداني % % 2(@)(@@) % فيا جفنه الماضي وأحمر خده % % رفيقك قيسي وأنت يماني % % 3(@)(@@) % و يا حسنه الغازي نصرت على العدى % % و لو كان من أعدائك القمران % % 4(@)(@@) % و يا خصره من دون ردفيه إنما % % عن البعد ترمي دونه الثقلان % % 5(@)(@@) % ألا ليت شعري إذ حكى الخصر ضمه % % و كانا على العلات يصطلحان % % 6(@)(@@) % و كافور جسم فيه للحسن تروة % % فليس الغواني عنده بغواني % % 7(@)(@@) % قضى الله يا كافور انك أول % % و ليس بقاض أن يرى لك ثاني % % 8(@)(@@) % وكم عاشق يا ظبي خلفت قلبه % % معار جناح محسن الطيران % % 9(@)(@@) % دليل الحشا لما نظرت قتلته % % بأضعف قرن في أذل مكان % % 10(@)(@@) % فيالك من قلبي وطرفي تنتحي % % علي غير منصور وغير معان % % PageV01P2031 11(@)(@@) % ومالك تعني بالصوارم والقنا % % و قدك طعان بغير سنان % % PageV01P2032 البحر : - 1(@)(@@) % حويت ذرا المجد لما حويت % % فنون العلوم وأفنانها % % 2(@)(@@) % و صغت المعاني كزهر النجوم % % فبوأك السعد كيوانها % % 3(@)(@@) % و مرتبة الدست أقررتها % % ولا زلت ياعين إنسانها % % 4(@)(@@) % اذ ما مددت لحاظ اليراع % % أنمت السيوف وأجفانها % % 5(@)(@@) % وأذكرت مصر الثنا الفاضلي % وأخفت دمشقك نيسانها % % 6(@)(@@) % وأرضيت في الخلق خلآقهم % % وفي دولة الملك سلطانها % % 7(@)(@@) % فأعظم بها دولة قد مت % % ك فآنست في الفضل أعيانها % % 8(@)(@@) % وأعطى لك الشعر ديوانه % % وحملت بالنثر ديوانها % % 9(@)(@@) % أقول مع الإختصار الذي % % تحسن للنفس بنيانها % % 10(@)(@@) % تهن السيادة يا صدرها % % وملك العلا يا سليمانها % % PageV01P2033 البحر : - 1(@)(@@) % أقسمت ما رزءك مما يهون % % يا غارقا حتى بدمع العيون % % 2(@)(@@) % وواجب يا فرع نوح الورى % % عليك من قبل حمام الوكون % % 3(@)(@@) % وإنما قومك شهب الهدى % % في الأجر من صبرهم يرغبون % % 4(@)(@@) % صبرا بني الانصار عن كوكب % % قد سهرت 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms403# شوقا اليه الجفون % % 5(@)(@@) % وغصن علم في ربى سودد % % قد مات بالماء خلاف الغصون % % 6(@)(@@) % لهفي على ذالك الهلال الذي % % شقت له السحب ثياب الدجون % % 7(@)(@@) % لهفي على دينار خد له % % عاجله الدهر بصرف المنون % % 8(@)(@@) % وغيضت العلياء في حالتي % % غيظ وغيض وطمت من شجون % % 9(@)(@@) % إنا إلى الله فقد كان ما % % خاف أبو تمامها أن يكون % % 10(@)(@@) % هذا على أن اللقا بيننا % % مقترب الآماد فالأمر دون % % PageV01P2034 11(@)(@@) % ان منع الغياب أن يقدموا % % لنا فانا لهم مقدمون % % 12(@)(@@) % عزاء مولانا وتسليمه % % فكل خطب قد عداه يهون % % 13(@)(@@) % ما سخنت فيها عيون الورى % % حتى تجليت فقرت عيون % % 14(@)(@@) % فلا خبا شخصك عن معشر % إلى العلى بالنجم هم يهتدون % % PageV01P2035 البحر : - 1(@)(@@) % فتور على أجفانها وفتون % % تريك معاني الحسن كيف تكون % % 2(@)(@@) % محجبة ما خلت قبل جفونها % % على كبدي ان السيوف جفون % % 3(@)(@@) % أخاف فأبكي بعدها قبل وقته % % فيا لعيون دمعهن عيون % % 4(@)(@@) % ويا عاذلا يقسو علي وما أرى % % ثرى لمحة من وجهها وتلين % % 5(@)(@@) % لقد كنت ذا قلب كقلبك عاقل % % فجن لليلى والجنون فنون % % 6(@)(@@) % وطال حديث الناس عن شجني بها % % فقد صح لي أن الحديث شجون % % 7(@)(@@) % الا من لصب من جفاه وشجوه % % شكى السقم حتى ما يكاد يبين % % PageV01P2036 البحر : - 1(@)(@@) % أأحبابنا داركم والعيش نعمان % % والسفح دمعي ودار القلب حران % % 2(@)(@@) % أشكو اشتياقا وما بالوصل من قدم % % كأن وصلي لفرط الحب هجران % % 3(@)(@@) % وربما رمت أن أشكو السهاد إلى % % عدل المنام وقلت النوم سلطان % % 4(@)(@@) % يا أيها الناصر السلطان لا غمضت % % عين لها عن سنا مرآك سلوان % % 5(@)(@@) % كم في ملوك الورى فضل ومعرفة % % كانوا ومثلك في ذا النحو ما كانوا % % 6(@)(@@) % ان يمض كسرى فكم إيوان معدلة % % لديك قد زانه يمن وايمان % % 7(@)(@@) % أمرت شعري شعري يا خير الملوك على % % أشعار قوم فلي أمر وديوان % % PageV01P2037 البحر : - 1(@)(@@) % بي من الترك ساجي الطرف وسنان % % ماالصب منه معاذ وهو فتان % % 2(@)(@@) % بي ضيق العين صانوه فقلت لهم % % سم الخياط مع المحبوب ميدان % % 3(@)(@@) % له من الحسن فن لا نظير له % % وللعلا في العلى فن وأفنان % % 4(@)(@@) % البحر علما وجودا جانسا صفة % % فحبذا منه سيحان وجيحان % % 5(@)(@@) % والوارث الفضل ممنوع لسؤدده % % مع 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms404# العلو على السادات رجحان % % 6(@)(@@) % يا سيدا جمعت لي من عوارفه % % نعمى وعليا فأسرار وإعلان % % 7(@)(@@) % غلام بيتك في بيتي ثنا وولا % كما يسرك حسان وسلمان % % PageV01P2038 البحر : - 1(@)(@@) % تناسبت المحاسن يا لبينا % % فخلنا من بياض يد جبينا % % 2(@)(@@) % رنيدة وقتها سمنت بلطف % % فما أشهى رنيدا أو سمينا % % 3(@)(@@) % يطالب صدغها والخال قلبي % % كأن علي للحبشان دينا % % 4(@)(@@) % كما طالبت جودك يا ابن يحيى % % وكم أجدى وكم أسدى إلينا % % 5(@)(@@) % علاء الدين دمت لنا ملاذا % وغوثا إن أقمنا أو نأينا % % 6(@)(@@) % لبست من السيادة ثوب فضل % تعود ذيله عطفا علينا % % 7(@)(@@) % ردين يقول ثنا وأجر % ألا حييت عنا يا ردينا % % PageV01P2039 البحر : - 1(@)(@@) % ظبي تبسم عن در ومرجان % % وكان يكفي على الخدين مرجاني % % 2(@)(@@) % آس وورد دعا خدام دمعي إذ % % لباهما لؤلؤي قدما ومرجاني % % 3(@)(@@) % كما دعا جود مولانا الوزير رجا % % لباه ذو فضة تجري وعقيان % % 4(@)(@@) % وزير مصر التي قالت وما كذبت % % أنت العزيز رفيعا فوق أجفاني % % 5(@)(@@) % وذو الخصيبين من ذكر ومن قلم % وذو الفخارين من اسم ومن شان % % 6(@)(@@) % فليهنه العيد اذ هنى بطلعته % % عيدا وهنى جميع الناس عيدان % % 7(@)(@@) % في رفعة تحسن الأعراب أن تره % % يرفع له الشأن أو يكسر له الشاني % % PageV01P2040 البحر : - 1(@)(@@) % من لي بها في الترك ينسب خدها % % فيقال في الأوصاف خد قاني % % 2(@)(@@) % يا نار مالك قلبي العاني لقد % % أحرقت قلب شقائق النعمان % % 3(@)(@@) % في وصفها أوفي ثنا ابن جماعة % % أضحى فريدا في القريض مباني % % 4(@)(@@) % برهان دين الله قد أثبته % % وجدان معنى الجود في الأزمان % % 5(@)(@@) % قل للذي أفتى الورى أن لا فتى % % ظهر الدليل عليه بالبرهان % % 6(@)(@@) % كثرت معاني الفضل منك وجددت % % نعماك للأجواد معنى ثاني % % 7(@)(@@) % فخذ الثنا باق بقاء فتى يرى % % إن الثراء وكل شيء فان % % PageV01P2041 البحر : - 1(@)(@@) % بنوا سكنا ثم نبني ويسكنوا % % و حركنا هذا الزمان ويسكن % % 2(@)(@@) % و ما البيت إلا قبر حي فحقه % % يحسن في أوضاعه ويزين % % 3(@)(@@) % يذكره الجنات طيب مقامه % % فيدأب في تحصيلها كيف يمكن % % 4(@)(@@) % فيالك من دنيا لآخرة دعت % % و يالك بيتا يمنه متبين % % 5(@)(@@) % معجل نعمى حيث شكو مؤمل % % و انى يوفي دينه المتدين % % 6(@)(@@) % إلهي كما حسنت للحي منزلا % فمنزلة 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms405# الثاني بعفوك أحسن % % 7(@)(@@) % و ما أنا من عفو الكريم بآيس % % و حسبي إني واثق الظن مؤمن % % PageV01P2042 البحر : - 1(@)(@@) % لها من جبين البدر أو قامة الغصن % % محاسن قد تجني علينا ولا نجني % % 2(@)(@@) % فان يك غصنا ثاني العطف مثنيا % فتلك كما نثني وفوق الذي نثني % % 3(@)(@@) % و إن يجر شعر الأقدمين بمدحة % لغيرك سلطانا فأنت الذي تغني % % 4(@)(@@) % و ان نحسن الأمداح نظما فانها % % على حسن السلطان مقبلة الحسن % % 5(@)(@@) % له دولة فاقت على كل دولة % % و خدام ملك من بشير ومن يمن % % 6(@)(@@) % فبشرى لهم والعالمين عواقبا % لأهل الهنا تبقي وأهل الشقا تفني % % 7(@)(@@) % و حسب الهنا منا طبيب ومادح % % و من مثل هذين الحليمين في الفن % % PageV01P2043 البحر : - 1(@)(@@) % العبد يهدي على مقداره وعلى % % مقدار ساداتها تسدي يد المنن % % 2(@)(@@) % قالت صحون حلاوات لجاحقه % % هذي المكارم لا قعبان من لبن % % 3(@)(@@) % يا صاحب السيف من ذهن تزان به % % لقد فخرت على سيف ابن ذي يزن % % PageV01P2044 البحر : - 1(@)(@@) % أي شيء يا سيدي يبلغ النا % % س ويصطادهم بكل مكان % % 2(@)(@@) % و هو ذو حافر يسير ويسري % % كل وقت وليس كالحيوان % % 3(@)(@@) % ملحد لا يزال في شرعة الدي % % ن وإن كان ليس بالانسان % % PageV01P2045 البحر : - 1(@)(@@) % كمال الدين عشت لنا ملاذا % تصوغ عن شمائله المعاني % % 2(@)(@@) % وقعت على الجواد وأنت غيث % % و قمت مسلما خصب الجنان % % 3(@)(@@) % فأيقنت الورى خصبا وقالوا % % وقوع الغيث من خصب الزمان % % PageV01P2046 البحر : - 1(@)(@@) % يا سيدي قل لي ما اسم وإن % % تشأفقل كيف وما شئت كان % % 2(@)(@@) % يشيب في أول أوقاته % % و هو على هذا شهي العيان % % 3(@)(@@) % يخفي عن العاقل لكنه % % إن رامه الذاكر في الحال بان % % PageV01P2047 البحر : - 1(@)(@@) % قل للرفيقين اللذان كلاهما % % في طيب وقت قد علاني يمنه % % 2(@)(@@) % شتان ما بيني وبين صفاتكم % % يعقوب عندكم وعندي حزنه % % PageV01P2048 البحر : - 1(@)(@@) % و فارقني من طاربي قبل فرخه % % و كنت لديه في أعز مكان % % 2(@)(@@) % تغطيت عن دهري بظل جناحه % % فعيني ترى دهري وليس يراني % % PageV01P2049 البحر : - 1(@)(@@) % قسما بفضلك انه الفضل الذي % % أبدا أعيش بيمنه وبمنه % % 2(@)(@@) % إني لأذكر من براق مودة % % ارضى الجلوس لدى نعل ابنه % % PageV01P2050 البحر : - 1(@)(@@) % و قالوا أتجهر قطب العلى % % حلا يوم 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms406# جمعتك المستبينه % % 2(@)(@@) % فقلت هو البحر في فضله % % فما ألتقيه سوى في السفينة % % PageV01P2051 البحر : - 1(@)(@@) % تغيب مملوكي الذي قد هويته % % و خلف إيري للهموم يعاني % % 2(@)(@@) % و مانافعي تحت الدجى نصب فيئتي % % و قد حيل بين العيرو النزوان % % PageV01P2052 البحر : - 1(@)(@@) % لأنف حاسدك الرغم الطويل فقد % % ظهرت يا ابن علي مظهرا حسنا % 2(@)(@@) % ما ذلك اللغز إلا روضة أنف % % تفاءل القلب فيه الحسود فنا % % PageV01P2053 البحر : - 1(@)(@@) % هنئت مقدمك الذي هنئته % % مع أن عيشي بالجفا غير الهني % % 2(@)(@@) % بالرغم مني أن أعد هدية % % أتقاصها شيء وليس تعدني % % PageV01P2054 البحر : - 1(@)(@@) % هنأ الله حسبة بك حلت % % و سرى برها لقاص وداني % % 2(@)(@@) % و تعالت إلى السماء لى أن % % نظرت في الذراع والميزان % % PageV01P2055 البحر : - 1(@)(@@) % يامن تحكم في المصالح رأيه % % فتوضحت فيها مطالع يمنه % % 2(@)(@@) % قسما لقد تبعت بيمن ضجيعها % % أحكام عبد الحق أحكام ابنه % % PageV01P2056 البحر : - 1(@)(@@) % أوتيت يا أرجح الأنام نهى % % فضائلا في الورى لها شان % % 2(@)(@@) % فصل خطاب وحكمة بهرت % % فأنت داود أم سليمان % % PageV01P2057 البحر : - 1(@)(@@) % تعجب الناس من صمتي وقد ذكرت % % سلمى وقالوا تسلى قلب هيمان % % 2(@)(@@) % و حار دمعي في عيني فقال فتى % % أقسمت بالقدس ماذي عين سلوان % % 3(@)(@@) % ان افقرتني من صبري المهى فندى % % قاضي القضاة عن السادات أغناني % % 4(@)(@@) % للعلم والجود تاج ان اصغ مدحا % فكم هداني إلى در وهاداني % % 5(@)(@@) % وفي الولا والثنا كم قلت في مدحي % % ما بيت سلمان هذا بيت حسان % % 6(@)(@@) % فليهنه العيد في عز وفي نعم % % وللأنام به والعيد عيدان % % 7(@)(@@) % لا فرق بين ضحايا أو عدى نحرت % % فإن أعداه والأنعام سيان % % PageV01P2058 البحر : - 1(@)(@@) % أشعت حبي ففي القاصي وفي الداني % % وفي عذولي قد أطلقت فداني % % 2(@)(@@) % ثار إلي بلوم لا يلائمني % % فصح لي أنما في العقل ثوران % % 3(@)(@@) % أزداد في حسن حبي إن عذلت جوى % % كانني من علي يوم إحسان % % 4(@)(@@) % قاض له شاهد إرث ومكتسب % في الفضل يا حبذا قاض وعدلان % % 5(@)(@@) % يا قادما وبليغ الجود يقدمه % % فضل وشهرهما أيضا ريعان % % 6(@)(@@) % في الكسوة اليوم إن لم ألق مقدمه % % فكم بامثالها في الجود لاقاني % % 7(@)(@@) % إن كان يحكيك انسان به ملئت % % عيني فلا ملئت عيني بانساني % % PageV01P2059 البحر : - 1(@)(@@) % بأبي على عين 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms407# الحبيبة حاجب % % لكنه بصبابتي مقرون % % 2(@)(@@) % لو شامه حتى الجنيد لراقه % % من حرفها بجبينها ذا النون % % 3(@)(@@) % حبي لها حب العلى لتقيها % % لم يدر لاح أينا المفتون % % 4(@)(@@) % ذو النسك والخلع التي قرت بها % % وتقر جنات لنا وعيون % % 5(@)(@@) % يا سيد الوزراء لا مشستثنيا % من كان من أمثالهم ويكون % % 6(@)(@@) % شرفت صفاتك عن مثيل منهم % وإذا بجعفر عند بحرك دون % % 7(@)(@@) % ان كان للخلفاء مثل يلتقي % % في الشام أين مراحل المأمون % % PageV01P2060 البحر : - 1(@)(@@) % يا طائرا بالحمى يشدو على فنن % % طوقت أنت الناس بالمنن % % 2(@)(@@) % كأنها عن وزير الملك مخبرة % % عن جابر عن عطا عن سعد عن حسن % % 3(@)(@@) % يهوى المعالي ولا يهوى ثراء يد % % كأنه النبع طلاعا على القنن % % 4(@)(@@) % يا خاتم الوزراء الأكرمين ويا % % عزيز مصر ويا سار على السنن % % 5(@)(@@) % حجبت عزا وتوقيرا وما حجبت % % عنا أياديك في سر ولا علن % % 6(@)(@@) % ان خانني الزمن المعروف في سبب % % وخانني زمن في الحل والقطن % % 7(@)(@@) % فقد شفاني دوا نعماك منشدة % % هذا بذاك ولا عتب على الزمن % % PageV01P2061 البحر : - 1(@)(@@) % ألحب مفروض علي لغادة % من لحظها أصبو إلى المسنون % % 2(@)(@@) % فيها التغزل والمديح أصوغه % % لأخي الوزير بلؤلؤ مكنون % % 3(@)(@@) % أهلا بأيام الوزير وصنوه % % فكلاهما للملك خير أمين % % 4(@)(@@) % بيت الوزارة لا يزال معرفا % بأمين ملك في العلى ومكين % % 5(@)(@@) % قال الثناء لبشره ولعرضه % % عج بالنقا يا سعد سعد الدين % % 6(@)(@@) % ما كل من هني بعيد بابه % % بالسعد من بلقائه حيوني % % 7(@)(@@) % بمعجل المكيول من إنعامه % % ومن المدائح فيه بالموزون % % PageV01P2062 البحر : - 1(@)(@@) % جن الدجى واشتقت حسنك % % وقرعت ياذا العذل سنك % % 2(@)(@@) % يا عاذلي في الحب أو % % يا ليل سهدي ما أجنك % % 3(@)(@@) % عشقي كجود ابن العد % % يم فخل في السلوان ظنك % % 4(@)(@@) % قاضي القضاة أخا التقى % % لا يعدم الطلاب منك % % 5(@)(@@) % أكدت فني في الثنا % % ء وفي الندى والعلم فنك % % 6(@)(@@) % فالناس تعلم أنني % % في النظم أو في الفضل أنك % % 7(@)(@@) % فلآشكرنك ما حييت % % وإن أمت فلتشكرنك % % PageV01P2063 البحر : - 1(@)(@@) % وآنسة قد فرق الدهر بيننا % % فلله قلبي ما أحن وما أحنى % % 2(@)(@@) % إلى حاجبيها صار قلبي صبابة % % وقلبي منها قاب قوسين أو أدنى % % 3(@)(@@) % وأكد طول النأي والعسر حيرتي % % بتسع شهور قد خلت 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms408# كلها حزنا % % 4(@)(@@) % فهل لي إلى الباب العلائي قصة % % تملك لفظي أو يدي أملي وزنا % % 5(@)(@@) % من النظم لولا الجود معن رأيته % % هنالك ما لاقيت لفظا ولا معنى % % 6(@)(@@) % وآنست ما قد قال غيري شاكيا % ومثلي من غنى ومثلك من أغنى % % 7(@)(@@) % و ذو صنعة فاستخدموني لصنعتي % % برزقي والا فارزقوني مع الزمني % % PageV01P2064 البحر : - 1(@)(@@) % سالتني مثيلة القمرين % % كيف حالي فقلت يا مثل عيني % % 2(@)(@@) % زمن الليل والنهار تلاه % % زمن في اللسان والركبتين % % 3(@)(@@) % غير أن الدعاء والمدح للسلط % % ان نمني على كلا الحالتين % % 4(@)(@@) % و ذاك مرمى وذاك رفعا إلى الل % % ه وللملك نصب عين اليدين % % 5(@)(@@) % ولأقلام صاحب السر والأنظ % % ار والاحتجاب في الخافقين % % 6(@)(@@) % من يكن ذا صناعة عرفت أو % % زمن خيف لم يضع بين ذين % % 7(@)(@@) % دام رأي العلى متى بر رأيا % يؤته الله أجره مرتين % % PageV01P2065 البحر : - 1(@)(@@) % حاشا لوعدك أن يلويه نسيان % % وحسن وجهك أن يعدوه إحسان % % 2(@)(@@) % يا من وقفت عليه العين ساهرة % % أقسمت لا صد عيني عنك إنسان % % 3(@)(@@) % فيك التغزل والمدح المنظم في % % محمد فلشعري في الورى شان % % 4(@)(@@) % كافي المناصب في سر ومشتهر % % فحبذا منه إسرار وإعلان % % 5(@)(@@) % تهن بالعيد يا عيد العفاة ولا % % زالت بسؤددك الأمداح تزدان % % 6(@)(@@) % عمرت بيت ولاء فيك أو مدح % % حتى كأني سلمان وحسان % % 7(@)(@@) % يجل ديوان مدح أنت صاحبه % % كما يجل بمدحي فيكط ديوان % % PageV01P2066 البحر : - 1(@)(@@) % في خده وعذاره الفتان % % عوذ سناه بزخرف ودخان % % 2(@)(@@) % و استجل وجنته ربيعا أولا % جاء العذار لها ربيعا ثاني % % 3(@)(@@) % و معاطفا تحكي يراع محمد % % غصنا عليه جوامع البستان % % 4(@)(@@) % شيخ الشيوخ إمامها وبليغها % % في عنفوان شبيبة الشيبان % % 5(@)(@@) % يا من مباديه نهاية معشر % % في العلم والحسنات كالإحسان % % 6(@)(@@) % هنئت عيد النحر تنحر شانئا % وتعيش ممتدحا رفيع الشان % % 7(@)(@@) % في رفعة وسيادة وسعادة % % وزهاوة وإفادة وتهاني % % PageV01P2067 البحر : - 1(@)(@@) % أفدي التي كلما حليتها صفة % % كادت مراشف ذكراها تحليني % % 2(@)(@@) % تقلي محبا وتشوي قلبه وندى % % تاج الشريعة يدنيني وينشبني % % 3(@)(@@) % لا يعدم المدح من قاضي القضاة لهى % % لا يدعي حصرها نظم الدواوين % % 4(@)(@@) % بقية القوم مع قرب النجوم لهم % % هم بمال اليتامى والمساكين % % 5(@)(@@) % في رتبتي علمهم والجود ثم لهم % % فرط العلو 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms409# ورجحان الموازين % % 6(@)(@@) % ناسبت في كل عام من عوارفه % % مواسم الفضل أرويها وترويني % % 7(@)(@@) % ألعصر أقرأ تعويذا لحاسده % % من شر حاسدها والعصر تقريني % % PageV01P2068 البحر : - 1(@)(@@) % قل للامام الذي لولا عواطفه % % ماكان في الشام لي عن مصر سلوان % % 2(@)(@@) % أيام طمويه لي مستنزه أنق % وللمبشر قلب الصب حلوان % % 3(@)(@@) % فان يكن بدمشق اليوم لي وطن % فكل أرض لمدحي فيك أوطان % % 4(@)(@@) % وان يكن قد بكى جفن السحاب فقد % % بكى من البرد لي أنف وأجفان % % 5(@)(@@) % فهل ببعض الفراجي اليوم تعتق لي % % عتيقة لي بها في العمر أزمان % % 6(@)(@@) % أميل خوف ازدجحام الناس تحطمها % % كأنني مثل بعض الناس سكران % % 7(@)(@@) % لا زلت يا كعبة المعروف تمنحني % % من كسوة لي بها في المدح أركان % % PageV01P2069 البحر : - 1(@)(@@) % دع هلالا لاح أو غصنا تثنى % % وامتدح مفرد وقت ما تثنى % % 2(@)(@@) % في حمى الشام وفي الدنيا له % % نعم زائدة تخجل معنى % % 3(@)(@@) % حبذا تجنيس ألفاظ الثنا % % حسنا عنكم وإحسانا وحسنى % % 4(@)(@@) % يا أميرا ناصر المنتمي % % حبذا بيت العلا والمدح معنى % % 5(@)(@@) % ونعم شكرا لها من نعم % بوركت حتى أكلنا ولبسنا % % 6(@)(@@) % قلت للجوع وللعرى معا % آل فضل الله يمحو السوء عنا % % 7(@)(@@) % رب أيد من علا أيديهم % أصلح الباطن والظاهر منا % % PageV01P2070 البحر : - 1(@)(@@) % يا سيد الوزراء العادلين لقد % % صيرت في منزلي للجوع إحسانا % % 2(@)(@@) % لكن بني وإن كانوا ذوي عذر % ليسوا من الصبر في شيء وان هانا % % 3(@)(@@) % كأن ربك لم يخلق لمسغبه % % سواهم من جميع الناس إنسانا % % 4(@)(@@) % قد طيروني وإن أخرت مطلبهم % % طاروا اليك ذرافات ووحدانا % % 5(@)(@@) % فأمر بما طلبوا لا شان بابكم % بنوا للقيطة من ذهل ابن شيبانا % % PageV01P2071 البحر : - 1(@)(@@) % سقى صوب الغمام زمان وصل % % قضينا فيه للأشواق دينا % % 2(@)(@@) % و قابلنا بدور في غصون % % طوالع فاجتلينا واجتنينا % % 3(@)(@@) % فما أصغت لداعي القرب أذن % % إلى أن مد داعي البين عينا % % 4(@)(@@) % فأمسينا كأنا ما افترقنا % % وأصبحنا كأنا ماالتقينا % % PageV01P2072 البحر : - 1(@)(@@) % شد اشد والحمام وماس غصنا % غني الحسن يطرب إن تغنى % % 2(@)(@@) % فريد وهو فتان التتني % % فيا لله فرد تثنى % % 3(@)(@@) % بعطف مثل منطقه رشيق % % ولفظ يعجب الأسماع لحنا % % 4(@)(@@) % و شكل معرب عن كل حسن % % وخصر مثل جسمي فيه مضنى % % 5(@)(@@) % فما أشهى 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms410# محيا منه زاه % % ولحظا ما رمى قلبا فأسنا % % 6(@)(@@) % و ما أشهى عذارا قد سباني % % بحرف جاء في حسن لمعنى % % PageV01P2073 البحر : - 1(@)(@@) % فريد حسن تجافى % % هلا شفعت بحسني % % 2(@)(@@) % يا ليت شعري لمعنى % % جفوت أم لا لمعنى % % 3(@)(@@) % أسهرتني وعذولي % % على هواك معنى % % 4(@)(@@) % ليلي وعاذل مثلي % % هذا وذا ما أجنى % % PageV01P2074 البحر : - 1(@)(@@) % قاضي القضاة بعثت لي شبه السما % % ونجومها وأردت رفعة شاني % % 2(@)(@@) % بالخلعة الزرقاء تتلو الفضة ال % % بيضاء ذات الحسن والاحسان % % 3(@)(@@) % اما برفع سهيل أمسى طالعي % % قدرا واما طالع الميزان % % 4(@)(@@) % يا نائبا للشرع في أحكامه % % أنا نائب في الشكر عن حسان % % PageV01P2075 البحر : - 1(@)(@@) % لا يعدم العافون يمنك % % في كل مقصدهم ومنك % % 2(@)(@@) % فالشهب تعلم أنك ال % % عالي سنا والسحب أنك % % 3(@)(@@) % صيرت فني في المدا % % ئح إذ جعلت الجود فنك % % 4(@)(@@) % فلأشكرنك ما حيي % % ت وإن أمت فلتشكرنك % % PageV01P2076 البحر : - 1(@)(@@) % ماذا على ذي الحسن لو % % أفضى إلى إحسانه % % 2(@)(@@) % ملك الملاح كما ترى % % والكل من غلمانه % % 3(@)(@@) % يرنو ويشرق حسنه % % في ناظري ولهانه % % 4(@)(@@) % فهو الغزالة والغزا % % ل بعينه وعيانه % % PageV01P2077 البحر : - 1(@)(@@) % يا فاضل الدنيا دع المصري قد % % أخملت ذكر الفاضل النيساني % % 2(@)(@@) % قسما لأنت أحق بالقول الذي % % قال السعيد لعظم ذاك الشاني % % 3(@)(@@) % أخذت بمجلسك المهابة حقها % % فترى البريء لديه مثل الجاني % % 4(@)(@@) % فلو استطعت نقلت من ديوانه % % هذا الثنا فرضا على ديواني % % PageV01P2078 البحر : - 1(@)(@@) % على اليمن والنعمى قدومك انه % % قدوم الحيا الساري إلى كل ظمآن % % 2(@)(@@) % و عودك للأوطان من مصر فائزا % بملك ومن أرض الحجاز بغفران % % 3(@)(@@) % حلفت بدهر أنت غوث عفاته % % لقد نفذت فيه العفاة بسلطان % % PageV01P2079 البحر : - 1(@)(@@) % فديتك من كل ما تختشي % % وعشت وصنوك كالفرقدين % % 2(@)(@@) % عن الحال يا سيدي لا تسل % % ولا عن طبيبي المقل اليدين % % 3(@)(@@) % بطب أبقراط وافى إلي % % وراح ولكن بخفي حنين % % PageV01P2080 البحر : - 1(@)(@@) % يا خير من ينبي على جوده % % كف ومن يثنى عليه لسان % % 2(@)(@@) % قد طال إصغائي إلى مخبر % % عن راتبي فامنن جزيت الجنان % % 3(@)(@@) % إن الثمانين وبلغتها % % قد أحوجت سمعي إلى ترجمان % % PageV01P2081 البحر : - 1(@)(@@) % و حقك لولا دلقي الصوف مكرما % % وشاشي لما اخترت التحرك من هنا % % 2(@)(@@) % و لكنني قاسيت بينهما أذى % % تنوع في مكروهه وتفننا 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms411# % % 3(@)(@@) % و قد كنت عصفورا بشاشي مالحا % فأصبحت عصفورا بدلقي مطجنا % % PageV01P2082 البحر : - 1(@)(@@) % عذبوني في هواها عذلي % % ونهوني زائدا والقلب مفتون % % 2(@)(@@) % ثم قالوا أنت مجنون بها % % قلت مجنون ومجنون ومجنون % % 3(@)(@@) % ان يك عشقي مفروضا على % % مهجتي ان لذاك اللحظ مسنون % % PageV01P2083 البحر : - 1(@)(@@) % إن البراغيث قد ماتت تشييني % % فبت أحيي الدجى نسكا وإيمانا % % 2(@)(@@) % فلو رأيتهم يستخرجون دمي % % رأيت أكثر خلق الله عدوانا % % 3(@)(@@) % ضحوا باشمط عنوان السجود به % % يقطع الليل تسبيحا وقرآنا % % PageV01P2084 البحر : - 1(@)(@@) % من منصفي من أناس % فيهم تحير ذهني % % 2(@)(@@) % لا درهما وزنوه % % وحاولوا الشر مني % % 3(@)(@@) % و هل سمعتم بشعر % % يأتي على غير وزن % % PageV01P2085 البحر : - 1(@)(@@) % لو آذنتني عذالي بحربهم % % إذ في التكاريش قد أصبحت هيمانا % % 2(@)(@@) % إذا لقام بمصري معشر حشن % % عند الحفيظة إن ذو لوثة لانا % % 3(@)(@@) % قوم اذا الإير أبدى ناجذيه لهم % % طاروا اليه ذرافات ووحدنا % % PageV01P2086 البحر : - 1(@)(@@) % يثني عليك لسان حالي في الورى % % أضعاف ما يثنى عليك لساني % % 2(@)(@@) % قسما لقد أخجلت معنى في الندى % % بعوارف لك قد أتت بمعان % % 3(@)(@@) % و رفعت في أفق العلى أزماننا % % يا تاجه رأسا على الأزمان % % PageV01P2087 البحر : - 1(@)(@@) % يا من به ارتوت الآمال بعد ظما % % ومر تحت صفيح اللحد ريان % % 2(@)(@@) % لله يمن بلاد أنت ناظرها % % فحبذا ناظر فيها وإنسان % % 3(@)(@@) % أحييت موتى الأماني بعد ما دفنت % % فقل لنا أنت عيسى أم سليمان % % PageV01P2088 البحر : - 1(@)(@@) % عتبت ابن الوكيل وشك ظني % % فاعتبني وعاد إلى اليقين % % 2(@)(@@) % و قال نواله هيهات يشكو % % ذوو الإقتار من عهدي المتين % % 3(@)(@@) % و ماذا يبتغي الشعراء مني % % وقد جاوزت حد الأربعين % % PageV01P2089 البحر : - 1(@)(@@) % بروحي سيدا ماكا % % ن للسادات يحوجني % % 2(@)(@@) % بلطف النظم ابهجه % % وبالاحسان يبهجني % % 3(@)(@@) % ففي بيت أفرجه % % وبستان يفرجني % % PageV01P2090 البحر : - 1(@)(@@) % عش يا رفيع الذكر والشان % % في خلع مجملة التيباني % % 2(@)(@@) % ما فتحت يوما على مثلكم % % في مثلها مقالة ذي شان % % 3(@)(@@) % تكسي فتكسوني تبعا لها % % فكل من هناك هناني % % PageV01P2091 البحر : - 1(@)(@@) % يا ملاذ الأنام هنال الل % % ه بعيد مبارك ميمون % % 2(@)(@@) % لا تسلني عن حال عائلتي في % % ه فاني في أمرهم في جنون % % 3(@)(@@) % ليس غيري في البيت قطعة لحم % فتفضل من قبل أن يأكلون % % PageV01P2092 البحر : - 1(@)(@@) % لا تنس رسم 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms412# العيد في العي % % د الذي يختال حسنا % % 2(@)(@@) % و اهنأ به وعلى الحقي % % قة فهو أولى أن يهنى % % 3(@)(@@) % و انحر عداتك والضحا % % يا وافنهم قرنا فقرنا % % PageV01P2093 البحر : - 1(@)(@@) % حبسوه لا بجريمة لكنهم % % بخلوا على لحظ العيون بحسنه % % 2(@)(@@) % رشأ يجور مع اعتدال قوامه % % فينا ويفتك مع حداثة سنه % % 3(@)(@@) % يا سائلي عن يوسف هو يوسف % كل القلوب بأسرها في سجنه % % PageV01P2094 البحر : - 1(@)(@@) % سر على اليمن والسعاة يا من % % شيد الله في المعالي مكانه % % 2(@)(@@) % أنت سهم لله ما كان يخلى % % منه أوطان مصر وهي كنانه % % PageV01P2095 البحر : - 1(@)(@@) % الله ينصر من وقى الإس % % لام من خوف وأمن % % 2(@)(@@) % و الله يرحم من درى % % هذا الدعا فيمن فأمن % % PageV01P2096 البحر : - 1(@)(@@) % لعمري لقد جردت في القدس عزمة % % معربة الأوصاف عالية المبنى % % 2(@)(@@) % يشيد بعد المسجد الطهر سوقه % % فقد شمل الأقصى نوالك والأدنى % % PageV01P2097 البحر : - 1(@)(@@) % فديت صيادة في البحر لاهية % % بحسنها وعن السلوان تلهيني % % 2(@)(@@) % تصيدني مثل صيد الحوت محرقة % % لي بالقلا فهي تقليني وتشويني % % PageV01P2098 البحر : - 1(@)(@@) % لما تبدي في الحنين % % تحاربت كبدي وعيني % % 2(@)(@@) % فاعجب لها من غزوة % % جلاءت ببدر في حنين % % PageV01P2099 البحر : - 1(@)(@@) % يا هاجرين ترفقوا بمتيم % % ذي مدمع سار ووجد قاطن % % 2(@)(@@) % لسع الجفاء حشاه وهو يرومكم % % حقا لقد أمسى سليم الباطن % % PageV01P2100 البحر : - 1(@)(@@) % رأيت في جلق غزالا % تحار في حسنه العيون % % 2(@)(@@) % فقلت ماالاسم قال موسى % % قلت هنا تحلق الذقون % % PageV01P2101 البحر : - 1(@)(@@) % قالت ووفرة شعري % % شابت وسائر ذقني % % 2(@)(@@) % قدام عينك هذا % % فقلت من خلف أذني % % PageV01P2102 البحر : - 1(@)(@@) % زادت محاسن سلطان الورى حسن % % إسما وفعلا وفاق السر والعلن % % 2(@)(@@) % وقيل أحسنها ماذا فقلت لهم % % وما محاسن شيء كله حسن % PageV01P2103 البحر : - 1(@)(@@) % عذرا لجاجتي المهدي لأنعمكم % % يا خجلتي منه في سر وفي علن % % 2(@)(@@) % لكل قاصر علم عنه لمحته % % رأي يفرق بين الماء واللبن % % PageV01P2104 البحر : - 1(@)(@@) % سعيت لباب سلطان البرايا % % ودمع الشوق ملء المقلتين % % 2(@)(@@) % فإن يك قد حظي مني حضورا % فما دمعي بدون المقلتين % % PageV01P2105 البحر : - 1(@)(@@) % قال لي الصحب ما نباتك يا % % منتسبا قلت لا تغموني % % 2(@)(@@) % بوعد محمود إذ أعيش به % % علمت اني نبات كمون % % PageV01P2106 البحر : - 1(@)(@@) % ومفرد الحسن تعشقته % % فكان حتى مفرد العين % % 2(@)(@@) % تقول للعشاق ألحاظه % % ما 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms413# يضرب الله بسيفين % % PageV01P2107 البحر : - 1(@)(@@) % لذ بشيخ الشيوخ يوم رجاء % % والتجيء واهنأ ميامن منه % % 2(@)(@@) % و الق منه الصفا فما هو ممن % % يتولى عن مادحيه بركنه % % PageV01P2108 البحر : - 1(@)(@@) % إيري يحاربني وعبدي منشد % الرأي قبل شجاعة شجعان % % 2(@)(@@) % قدام تلك وخلف هذا دأبه % % هي أول وهي المحل الثاني % % PageV01P2109 البحر : - 1(@)(@@) % أجاب مدحي مليح % قفاي بالصنع يعني % % 2(@)(@@) % فما تكلمت لكن % % سكت من خلف أذني % % PageV01P2110 البحر : - 1(@)(@@) % لبست من المدائح ثوب مجد % فقد انقطعت عوارفه علينا % % 2(@)(@@) % لها ردنان من نظم ونثر % ألا حييت عنا يا ردينا % % PageV01P2111 البحر : - 1(@)(@@) % مولاي نور الدين لي نسوة % % في مالح الأكل لها محنه % % 2(@)(@@) % يصمن عن قصدي ولكن اذا % % سألت عن قصد لها صحنه % % PageV01P2112 البحر : - 1(@)(@@) % إن السراج رفيقنا مع خيره % % تعساه شر مبينا متبين % % 2(@)(@@) % صدق الذي قد قال في أمثاله % % إن السراج على سناه يدخن % PageV01P2113 البحر : - 1(@)(@@) % لعمري لقد أفحمت بالقضل منطقي % % وقد كنت ذا نظق وفضل بيان % % 2(@)(@@) % و حركت ميزاني فأثنى لسانه % % فما زلت مشكورا بكل لسان % % PageV01P2114 البحر : - 1(@)(@@) % أشكوا إلى الله ما أقاسي % % من شدة الفقر والهوان % % 2(@)(@@) % أصبحت من ذلة وعري % % مافي داف سوى لساني % % PageV01P2115 البحر : - 1(@)(@@) % أهواه لدن القوام منعطفا % يسل من مقلتيه سيفين % % 2(@)(@@) % وهبت قلبي له فقال عسى % % نومك أيضا فقلت من عيني % % PageV01P2116 البحر : - 1(@)(@@) % سيدي شكرا لنعماك التي % % داركنا حين أعي أمرنا % % 2(@)(@@) % كم تدللنا لمن نقصده % % وبنعماك تهذب قدرنا % % PageV01P2117 البحر : - 1(@)(@@) % أمولانا الوزير دعاء عبد % تبدل في بلادكم فصنه % 2(@)(@@) % و لا تعطي العطا إلا هنيئا % اذا كان العطا لا بد منه % PageV01P2118 البحر : - 1(@)(@@) % جفاني الدرهم من بعدكم % % فبينكم يفضي إلى بينه % % 2(@)(@@) % و الذهب المذكور لي مدة % % ما وقعت عيني على عينه % % PageV01P2119 البحر : - 1(@)(@@) % و مصاحب تلقاه عند عيوبه % % خلدا بلا بصر به يتبين % 2(@)(@@) % فاذا بداعيب الصديق وجدته % % فهدا جميع الجسم منه أعين % PageV01P2120 البحر : - 1(@)(@@) % يا رب لص سالب ناهب % وهو من الحسن ملء عيني % % 2(@)(@@) % يرنو إلى سرب الظبا لحظه % % فيسرق الكحل من العين % % PageV01P2121 البحر : - 1(@)(@@) % لقد عدناكم لما صفعتم % % ولا والله ما وافيتمونا % % 2(@)(@@) % أقيموا في ضناكم أو افيقوا % % فان عدنا فإنا ظالمونا % % PageV01P2122 البحر : - 1(@)(@@) % ظلم الزمان فما ألمت بظلمه % % شيئا وصادف 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms414# طائرا متوطنا % % 2(@)(@@) % و غدا يهددني بمحزن خطبه % % هيهات يدري الحزن من عرف الهنا % % PageV01P2123 البحر : - 1(@)(@@) % نأت عن محبيه أعطافه % % وأمسوا إلى الطيف يستطلعون % % 2(@)(@@) % فهاهم قيام لفرط الأسى % % قليلا من الليل ما يهجعون % % PageV01P2124 البحر : - 1(@)(@@) % لهفي على فرسي الذي % % أضحى قريح المقلتين % % 2(@)(@@) % يكبو وأملك رقه % % فمعثر في الحالتين % % PageV01P2125 البحر : - 1(@)(@@) % سيدي أصبحت مقروح الحشا % % وبشي اللحم في ذا اليوم عان % % 2(@)(@@) % زخرف الألفاظ قد أرسلته % % فعسى تملا بيتي بالدخان % % PageV01P2126 البحر : - 1(@)(@@) % حملت قلبي فيك مالم يكن % % يحمله قلب وجثمان % % 2(@)(@@) % وعدت تعبانا لحملي له % % وحامل الحامل تعبان % % PageV01P2127 البحر : - 1(@)(@@) % و قالوا أحاطت ذقنه بخدوده % % ووجدك لا ينفك يذكر حسنه % % 2(@)(@@) % فقلت نعم ضيف يقلبي نازل % % أعظم مثواه وأكرم ذقنه % % PageV01P2128 البحر : - 1(@)(@@) % يا مشتكي الهم دعه وانتظر فرجا % ودار وقتك من حين إلى حين % % 2(@)(@@) % و لا تعاند اذا أصبحت في كدر % فإنما أنت من ماء من طين % % PageV01P2129 البحر : - 1(@)(@@) % و أغيد جارت في القلوب فعاله % % وأسهرت الأجفان أجفانه الوسنى % % 2(@)(@@) % أجل نظرا في حاجبيه ولحظه % % ترى السحر منه قاب قوسين أو أدنى % % PageV01P2130 البحر : - 1(@)(@@) % يقولون لي رفقا بجفنك في البكا % % فقلت لهم هيهات يتركني حزني % % 2(@)(@@) % سأبذل جفني بعد سيف فقدته % % اذا السيف أودى فالعفاء على الجفن % % PageV01P2131 البحر : - 1(@)(@@) % كل فعال العلاء يعجبني % % كانني بالعلاء مفتون % % 2(@)(@@) % يحمض بالمطل حلو موعده % % فوعده سكر وليمون % % PageV01P2132 البحر : - 1(@)(@@) % فدا لابن ريان الكرام فانه % % أخو منن يروي بها كل ظمان % % 2(@)(@@) % اذا جال فكري في تسرع جوده % % تقول القوافي إنه من سليمان % % PageV01P2133 البحر : - 1(@)(@@) % تبسم الشيب بذقن الفتى % % يوجب سح الدمع من جفنه % % 2(@)(@@) % حسب الفتى بعد الصبا ذلة % % أن يضحك الشييب على ذقنه % % PageV01P2134 البحر : - 1(@)(@@) % قال لي خلي تزوج تسترج % % من أذى الفقر وتستغني يقينا % % 2(@)(@@) % قلت دع نصحك واعلم انني % % لم أضع بين ظهور المسلمينا % % PageV01P2135 البحر : - 1(@)(@@) % رب نحوي بدا في خده % % عارض كاللام ما أعلى وأسنى % % 2(@)(@@) % قلت ما هذا السواد المنتحى % % قال حرف جاء في الحسن لمعنى % % PageV01P2136 البحر : - 1(@)(@@) % سادتي ما كان أجمع شملي % % فأصاب ذلك الشمل عين % % 2(@)(@@) % يا لها عين رقيب أصابت % % فمتى أبصرها وهي غين % % PageV01P2137 البحر : - 1(@)(@@) % يقولون من وطيء النساء خف العمى % % فقلت 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms415# دعوا قصدي فما فيه من شين % % 2(@)(@@) % اذا كان شفر العين دون محلها % % فعندي أنا الأشفار خير من العين % % PageV01P2138 البحر : - 1(@)(@@) % بشرى شمائلكم بطلعة كوكب % % يومي اليه بالسعود بنانها % % 2(@)(@@) % إن المنابر أورقت بأكفكم % % فتكاثرت من نسلكم أغصانها % % PageV01P2139 البحر : - 1(@)(@@) % كل شهر لنا هلال جديد % مبرز للفناء كل مصون % % 2(@)(@@) % بقرأ الناظر المفكر فيه % % فوق طرس السماء نون المنون % % PageV01P2140 البحر : - 1(@)(@@) % وصاحبي ساءني تعشقه % % لشاحب الوجنتين حوران % % 2(@)(@@) % لو كنت في الليل ناظرا لهما % % قلت شهاب في ظهر شيطان % % PageV01P2141 البحر : - 1(@)(@@) % كذا أبدا تزهى العلى بجلالها % % فلله ما أسرى فخارا وما أسنى % % 2(@)(@@) % رأى فضله أن يجعل الحال بيننا % % فوضع النمدى منه ووضع الدعا منا % % PageV01P2142 البحر : - 1(@)(@@) % مولاي دعوة من رمته عداته % % يملآءة وهو الشقي بدينه % % 2(@)(@@) % ان كان يملك من نضار حبة % % فالله يسبك عينها في عينه % % PageV01P2143 البحر : - 1(@)(@@) % ليت شعري كم أشكو الأذى من % % فلان عن فلان عن فلان % % 2(@)(@@) % كنت أرجو سندا لي فإذا % % هو عن فقري صحيح وهواني % % PageV01P2144 البحر : - 1(@)(@@) % شكرت لابن المحسني الندى % % والحمد لله على أنني % % 2(@)(@@) % عوضت في بابك يا سيدي % % بالنعم المربي على المحسن % % PageV01P2145 البحر : - 1(@)(@@) % لهفي على فاتر الأجفان منعطف % % أضحى يكايد غزلانا وأغصانا % % 2(@)(@@) % قاست ذوائبه بالليل حسده % % حتى نضى فاذا بالفرق قد بانا % % PageV01P2146 البحر : - 1(@)(@@) % طحانكم قد زهى جمالا % فلا يطاق السلو عنه % % 2(@)(@@) % و دق خصرا فليت شعري % % بكم يباع الدقيق منه % % PageV01P2147 البحر : - 1(@)(@@) % قل عوني على الزمان فأصبح % % ت صبورا على مراد الزمان % % 2(@)(@@) % حابس اللفظ واليراع عن النا % % س فلا من يدي ولا من لساني % % PageV01P2148 البحر : - 1(@)(@@) % لنا ملك قد قاسمتنا هباته % % فنثر العطا منه ونثر الثنا منه % % 2(@)(@@) % يذكرنا أخبار معن بجوده % % فننشي له لفظا وينشي لنا معنى % % PageV01P2149 البحر : - 1(@)(@@) % سقى الجدث العزي صوب غمامة % % وياليت تأويه الغداة يعاين % % 2(@)(@@) % فيبصر وجها في الورى زان خاله % % وعهدي بأن الخال للوجه زائن % % PageV01P2150 البحر : - 1(@)(@@) % أمستخبرا بالشام عن كنه حالتي % % ألم ترني مستبشرا بعد أحزاني % % 2(@)(@@) % و قد كنت أرعى النجم هما وخيفة % % فها أنا قد أمسيت والنجم يرعاني % % PageV01P2151 البحر : - 1(@)(@@) % شكرا وأجرا لما أوليت من نعم % % في عسرة أظلمت فيها مطالعنا % % 2(@)(@@) % أقسمت لولا نداك 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms416# المستهل لما % % كانت عقيقتنا إلا مدامعنا % % PageV01P2152 البحر : - 1(@)(@@) % يا من يقول البدر أو شمس الضحى % % كمعذبي لأكيد للقمرين % % 2(@)(@@) % أبوجه ذاك ووجه تلك تقيسه % % قسما لقد أخطأت من وجهين % % PageV01P2153 البحر : - 1(@)(@@) % نسبوه حسنا للهلال وعينه % % للظبي تنسب لا رميت بعينه % % 2(@)(@@) % فإذا بدا فإلى هلال أصله % % واذا رنا فهو الغزال بعينه % % PageV01P2154 البحر : - 1(@)(@@) % تبدت وقد أخفاني السقم وانبرت % % على حكمها عيناي منهملان % % 2(@)(@@) % يحجبها دمعي وحجبني الضنا % % فلست أرى ليلى وليس تراني % % PageV01P2155 البحر : - 1(@)(@@) % و صافي الولا والجسم مقتبل الرضا % % عريين قد أمنت بيني وبينه % % 2(@)(@@) % توثق شخصي في العناق بشخصه % % فما تدخل الأثواب بيني وبينه % % PageV01P2156 البحر : - 1(@)(@@) % من ميعني على دقيقة خصر % فاحتيالي مضاعف أشجاني % % 2(@)(@@) % أحسنت كي تزيد في الصد همي % % فهي مذمومة على الاحسان % % PageV01P2157 البحر : - 1(@)(@@) % لا حبذا شيب بشعري ولا % % شيب بقلبي أفد يا عيني % % 2(@)(@@) % ما كنت بالتائب عن صبوتي % % طوعا فقد تبت بشيبين % % PageV01P2158 البحر : - 1(@)(@@) % هام بالركن هائم % % عدلوا فرط حزنه % % 2(@)(@@) % فعصى كل عاذل % % وتولى بركنه % % PageV01P2159 البحر : - 1(@)(@@) % تغيب مملوكي الذي قد هويته % % وخلفت إيري للهموم يعاني % % 2(@)(@@) % و مانافعي تحت الدجى فمنيتي % % وقد حيل بين العير والنزوان % % PageV01P2160 البحر : - 1(@)(@@) % لك يا نديمي في التألف خطوة % % فاعهد لها أن أعوز الامكان % % 2(@)(@@) % و اصطد بها العنقاء فهي حبالة % % واقتد بها الجوزاء فهي عنان % % PageV01P2161 البحر : - 1(@)(@@) % أجران حمام الشآ % % م تسمعي لي لفظين % % 2(@)(@@) % لاتذكري أحواض مص % % ر فأنت دون المقلتين % % PageV01P2162 البحر : - 1(@)(@@) % إمنع وصالك يا فلا % % نة فلست منك ولست مني % % 2(@)(@@) % قد كان وجهك في الورى % % لمعا فقد صار ابن جني % % PageV01P2163 البحر : - 1(@)(@@) % تعودت من نعماك أحسن عادة % % فأقبلت أرجو منك عادة إحسان % % 2(@)(@@) % وجئت وما عندي نصف درهم % % ولكنه يا سيدي نصفه الثاني % % PageV01P2164 البحر : - 1(@)(@@) % أحبب بها ناعورة كم حدثت % % بلسان ماء والحديث شجون % % 2(@)(@@) % حنت فباطنها قلوب كله % % وبكت فظاهرها الجميع عيون % % PageV01P2165 البحر : - 1(@)(@@) % بروحي من أضحى له الحسن عسكرا % حوى كل قاص في الجمال وداني % % 2(@)(@@) % فيالحظه الماضي وأحمر خده % % رفيقك قيسي وأنت يماني % % PageV01P2166 البحر : - 1(@)(@@) % أحنت معاطفي السنون وغيرت % % عند الغواني ما بها المتبينا % % 2(@)(@@) % إيه لعهدك يا زمان البان من % % عطفي وآها يا زمان المنحى % % PageV01P2167 البحر : - 1(@)(@@) % كم 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms417# صار مثل دبيب النمل لي كلم % % من الهوان صغيرا بين أقراني % % 2(@)(@@) % حتى وفي لي صديق قال حاسده وقفت على وردي لفظ مبشر بما سوف من أنواعه الزهر يلقانيفياحبذا في شهرنا في رياضه أوائل ورد في آواخر شعبانفداك من الأسواء كل مومل ملأت يديه بالنوال وعينهوذي فكر أودعتها مبدع الثنا ولو لم يكن فيك الثنا ماوعينهقصدت حماك أرجي % % كبرت يا نمل أو صرت السليمانيسقط بيتين ص % % PageV01P2168 البحر : - 1(@)(@@) % م محراب صدغيه يحث توجهي % % و به على شرف البدور تجوهي % % 2(@)(@@) % قمر يقول سناه يا قمر الدجى % % فضح التكلف وجنة المتشبه % % 3(@)(@@) % عطر اللمى واللفظ واشوقي إلى % % فم شادن في الحالتين مفوه % % 4(@)(@@) % في صدغه الوأوا يجيد نسيبه % % ولعقل عاذلي انتساب الأبله % % 5(@)(@@) % أبدا به أتلو الشجون فليتها % % عن نافع عن أنة المتأوه % % 6(@)(@@) % وقفي على ذكراه إن سمت الكرى % % وبها ابتداء عند وقت تنبهي % % 7(@)(@@) % جل الذي أبدى لعاشق وجهه % % ماء عزيز الوصف من ماء مهي ن % % 8(@)(@@) % كالروض أو كالبدر أو كالشمس قد % % شرح الملاحة من ثلاثة أوجه % % 9(@)(@@) % مالعذل في حبي له متوجه % % فعلى م عذل الناصح المتوجه % % 10(@)(@@) % واذا رأيت الغصن ثم رأيته % % يختال تاه القلب منه بأتيه % % PageV01P2169 11(@)(@@) % هيهات أن يشفى فؤادي فيه من % % شجو ومدنف طرفه لم ينقه % % 12(@)(@@) % وكأن مبسمه نظام قصيدة % % بكرت نظام الملك بالعقد البهي % % 13(@)(@@) % وبدت وباعث شهوتي للقول قد % % ولي فها أنا أشتهي أن أشتهي % % 14(@)(@@) % حسناء من لي لو بدت وشبيبتي % % لسوى الحسان ووصفها لم يبده % % 15(@)(@@) % ما شيبة في فود مستجلى الدمى % % إلا قذاة بين جفني أمره % % 16(@)(@@) % أحسن بريعان الصبا ولبتره % % ماء على الخدين غير مموه % % 17(@)(@@) % أيام في لعس الشفاه تنقلي % % لثما وفي روض الخدود تفكهي % % 18(@)(@@) % والدهر حيث طلبت مثل مجرد % % والعيش حيث طربت مثل موله % % 19(@)(@@) % عيش كريم كم عتبت بمنطق % % فحشى فمي درا فقال له ره % % 20(@)(@@) % كانت لنا الايام ثم تصرمت % % واعتاض فاقدها بآه عن قه % % PageV01P2170 21(@)(@@) % سقيا لها ولمعشر فارقتهم % % إثر الصبا العادي فراق المكره % % 22(@)(@@) % وقصيدة لو لم يعد عهد الصبا % % عادت بأرفع من سناه وأرفه % % 23(@)(@@) % منظومة الأسلاك في عليا فتى % % عان بحب 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms418# المكرمات مدله % % 24(@)(@@) % لا عيب فيه غير أن جميله % % وجماله قاض بعجز المدره % % 25(@)(@@) % عمرية أعراقه علوية % % ومديحه لمكرريه شه شهي % % 26(@)(@@) % و هبت يداه ونبهت آراؤه % % فرووا العلى عن وهب بن منبه % % 27(@)(@@) % و أصخ لمدحة ناظم في حجرها % % آوى يتيم النظم غير مسفه % % 28(@)(@@) % أهلا بها من أهل مصر وحبذا % % من منزل بالشام جاد بمنزه % % 29(@)(@@) % جاءت مذكرة الجمال شريفة % % مثل المليحة في إزار لهله % % 30(@)(@@) % ما بين جاريتين وهي سبوقة % % بمدا العلى سبق الجياد السمه % % PageV01P2171 31(@)(@@) % ظهرت وأسكرت العقول فحبذا % % بين المحافل خمرة المستنكه % % 32(@)(@@) % إيه بعيشك يا بديع مقالها % % قل كيف شئت عن الهوى لا أنتهى % % 33(@)(@@) % عارضت أبيات العماد فعاذر % % ولو أنها ذات العماد بأن تهي % % 34(@)(@@) % و تركتها تبكي لآلة سمعة % % قد عطلت بعد العماد الآله % % 35(@)(@@) % و حططت للكندي تاج تملك % % عن جبهة من قبلها لم تجبه % % 36(@)(@@) % حتى عن الظليل حجبت الهدى % % وسخرت بالمتنبيء المتأله % % 37(@)(@@) % كم أصفهاني غدا بك أغيرا % في الترب لم يفتح عيون منوه % % 38(@)(@@) % و سليل أعراب فضلت فلم تدع % % لمزهزه وصفا ولا لمجهجه % % 39(@)(@@) % ببديعه ان قالها متحجب % % عنا فلا حجبت مقالة مدره % % 40(@)(@@) % درت بمذهيه الكلامي الذي % % قال البيان لفكره أشعر وافقه % % PageV01P2172 41(@)(@@) % من لو أشار إلى الدقائق كمه % % قرأت خواتمها عيون الأكمه % % 42(@)(@@) % سبق الجدال وقبله سبق الوغى % % فلووا نسيق المازق المتعنه % % 43(@)(@@) % و تعطلت آراء طالب شبههم % % وهم الردى لمعطل ومشبه % % 44(@)(@@) % هذاك أصلهم وهذا فرعهم % % أعظم بفضل المبتدي والمنتهي % % 45(@)(@@) % و ممدح يحصى لمادح فضله % % مصغ فنور يا ربيع ونوه % % 46(@)(@@) % ذي البيت وافته بيوت قصيدة % % لاقت فنحنح يا بيان ونهنه % % 47(@)(@@) % من آل فضل الله والقوم الأولى % % زانوا الزمان وكان مثل مشوه % % 48(@)(@@) % أوروا زناد معاجز ما مسها % % قدح وظنوا كل دهر أدره % % 49(@)(@@) % آثارهم عدد النجوم زواهرا % % وعلاهم عد الزمان المزدهي % % 50(@)(@@) % الصاعد الرتب التي خاضت به % % نهر المجرة لا يقال لها مه % % PageV01P2173 51(@)(@@) % و الكاتب الاسرار يحبس خطوها % % مع أنها في صدره في مهمه % % 52(@)(@@) % أي الممالك لم يشد بالرأي أم % % أي العقول بوصفه لم يبده % % 53(@)(@@) % فالعز في العتبات من أبوابه % % ما العز في صهوات خيل الأجبه % % 54(@)(@@) % حجبت يراعته 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms419# الخطوب فيالها % % من نعمة عن فضلها لم نعمه % 55(@)(@@) % سد يا علي على ذوي قلم وقل % % ليراعك اضحك بالصرير وقهقه % % 56(@)(@@) % و أمر بما تروي صداي أقم بها % % مدحا يضيق بها بيان الأفوه % % 57(@)(@@) % إني إذا التبس البيان وجدتني % % أضع العمامة عن جبين أجله % % 58(@)(@@) % حررت مدحك في البديع وقلته % % ورأيت كفك والغمام وقلت هيه % % PageV01P2174 البحر : - 1(@)(@@) % فداك من الأسواء كل مؤمل % % ملأت يديه بالنوال وعينه % % 2(@)(@@) % و ذي فكر أودعتها مبدع الثنا % % ولو لم يكن فيك الثنا ما وعينه % % PageV01P2175 البحر : - 1(@)(@@) % قصدت حماك أرجي الغنى % % وأشكو من العسر داء دفينا % % 2(@)(@@) % فما كان بيني وبين اليسار % % سوى أن مددت اليك اليمينا % % PageV01P2176 البحر : - 1(@)(@@) % إذا البلغاء نحوا غاية % % فهم بضيائك يسترشدون % % 2(@)(@@) % فأحسن بهم في دياجي السطور % % قياما وبالنجم هنم يهتدون % % PageV01P2177 البحر : - 1(@)(@@) % أصم حديث القرن يا روق مسمعي % % بتأخيره يا حابسين الندى عني % % 2(@)(@@) % فلا تجعلوني في العفاة نعامة % % غدت تبتغي قرنا فعادت بلا أذن % % PageV01P2178 البحر : - 1(@)(@@) % نزهت وعدك أن اذكرك الوفا % % يا من نداه لمن رجاه ضمين % % 2(@)(@@) % يا من إذا مزج الدعاء له الولا % % قال الرجا في الحالتين أمين % % PageV01P2179 البحر : - 1(@)(@@) % ألا يا وزير الملوك البليغ % % ويامن له قلم الصنعتين % % 2(@)(@@) % أحاشيك تنسى وصول المحال % % فيغدو محلا على الصيغتين % % PageV01P2180 البحر : - 1(@)(@@) % إن في نائب الشآم اعتبارا % للبرايا ما بين عال ودون % % 2(@)(@@) % كان أرغون شاه فاجأ الذن % % ح فأمسى شاه بلا أرغون % % PageV01P2181 البحر : - 1(@)(@@) % يمنى إمام الوقت قد أنشأت % % من يمنك لي عادات إحسان % % 2(@)(@@) % فان أكن بالعشر هنأته % % فانه بالخمس هناني % % PageV01P2182 البحر : - 1(@)(@@) % جلوسنا ما بين أيديكم % % منصب إعزاز وإمكان % % 2(@)(@@) % و العزل في العام له روعة % % فكيف في اليوم وفي الثاني % % PageV01P2183 البحر : - 1(@)(@@) % يا فاضلا حمدت منه مودته % % وحسن إصغائه للمادح اللسن % % 2(@)(@@) % عندي عقود ثنا لم ترج جائزة % % فهل لجيدك في عقد بلا ثمن % % PageV01P2184 البحر : - 1(@)(@@) % رب مليح حسن صورته % % قالوا وقد أصبح ذا ذقن % % 2(@)(@@) % لحيته قد قطعت ذقنه % % قلت من الأذن إلى الأذن % % PageV01P2185 البحر : - 1(@)(@@) % ينسى الفتى إحسانه فيما مضى % % خوف امتنان لا يليق بمحسن % % 2(@)(@@) % و أراك زدت معي على هذا الثنا % % فنسيت إحسانا مضى ونسيتني % % PageV01P2186 البحر : - 1(@)(@@) % شغل الكبار 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms420# من الرعية فكرهم % % في شأنهم فكبارنا كصغارنا % % 2(@)(@@) % ألرفق يا موسى الزمان بنا فقد % % حلقت ذقون صغارنا وكبارنا % % PageV01P2187 البحر : - 1(@)(@@) % قل للوزير ابن تاج الدين يا سندا % % وفي ببري باديه وباطنه % % 2(@)(@@) % شعري وقلبي بيتا مدحة وولا % % حاشاك تهدم بيتا أنت ساكنه % % PageV01P2188 البحر : - 1(@)(@@) % يا كريم الاب والصه % % ر نسا في حرمين % % 2(@)(@@) % حج في المملوك يا من % % عاش بين العلمين % % PageV01P2189 البحر : - 1(@)(@@) % راموا سلوي حيث لاحت نقطة % % في عين ظبي لا رميت ببينه % % 2(@)(@@) % هيهات أصرف عن هواه بنقطة % % هذا الصغار بعينه وبغينه % % PageV01P2190 البحر : - 1(@)(@@) % ان اللديغ هو السليم كما رووا % % ولكم بقلبي أي لدغ كامن % % 2(@)(@@) % و لعاذلي طمع بصبري عنكم % % أنا والعذول إذاسليما الباطن % % PageV01P2191 البحر : - 1(@)(@@) % شكرا لقاضي القضاة نجم علا % % هدى رجائي له وهاداني % % 2(@)(@@) % عددت أوصافه وأطعمني % % حلوى فحليته وحلاني % % PageV01P2192 البحر : - 1(@)(@@) % أقول وقد جاء الغلام بصحنه % % عقيب طعام الفطر يا غاية المنى % % 2(@)(@@) % بعيشك قل لي جاء صحن قطايف % وبح باسم من تهوى ودعني من الكنى % % PageV01P2193 البحر : - 1(@)(@@) % نفثت بنو الشام الدما وتتابعوا % % للموت من طاغ ومن مسكين % % 2(@)(@@) % حل القضاء بهم ووالى قهرهم % % فالكل مذبوح بلا سكين % % PageV01P2194 البحر : - 1(@)(@@) % و يحي من الحظ كم أحاوله % % في دهرنا وهو حائد عني % % 2(@)(@@) % يظنني عاقلا كما زعموا % % فهو على الظن نافر عني % % PageV01P2195 البحر : - 1(@)(@@) % لعمري لقد أحييت للشعر خاطري % % وقد كان ما بين الأنام كفاني % % 2(@)(@@) % و أصبح لي ذكر بمدحك سائر % % فلو لم تجد لي بالنوال كفاني % % PageV01P2196 البحر : - 1(@)(@@) % أشكو اليك حالة قد أوقعت % % محسوب هذا العمر في طول العنا % % 2(@)(@@) % يتطلب المال ولا يناله % % لا راحة الفقر ولا عيش الغنى % % PageV01P2197 البحر : - 1(@)(@@) % لوى صدغه كالنون من فوق وجنة % % تسعر نارا في حشى كل مفتون % % 2(@)(@@) % و ناديته ما اسم الفتى قال يونس % % فآمنت في عشقي بيونس ذي النون % % PageV01P2198 البحر : - 1(@)(@@) % محراب صدغيه يحث توجهي % % و به على شرف البدور تجوهي % % 2(@)(@@) % قمر يقول سناه يا قمر الدجى % % فضح التكلف وجنة المتشبه % % 3(@)(@@) % عطر اللمى واللفظ واشوقي إلى % % فم شادن في الحالتين مفوه % % 4(@)(@@) % في صدغه الوأوا يجيد نسيبه % % ولعقل عاذلي انتساب الأبله % % 5(@)(@@) % أبدا به أتلو الشجون فليتها % % عن نافع عن 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms421# أنة المتأوه % % 6(@)(@@) % وقفي على ذكراه إن سمت الكرى % % وبها ابتداء عند وقت تنبهي % % 7(@)(@@) % جل الذي أبدى لعاشق وجهه % % ماء عزيز الوصف من ماء مهي ن % % 8(@)(@@) % كالروض أو كالبدر أو كالشمس قد % % شرح الملاحة من ثلاثة أوجه % % 9(@)(@@) % مالعذل في حبي له متوجه % % فعلى م عذل الناصح المتوجه % % 10(@)(@@) % واذا رأيت الغصن ثم رأيته % % يختال تاه القلب منه بأتيه % % PageV01P2199 11(@)(@@) % هيهات أن يشفى فؤادي فيه من % % شجو ومدنف طرفه لم ينقه % % 12(@)(@@) % وكأن مبسمه نظام قصيدة % % بكرت نظام الملك بالعقد البهي % % 13(@)(@@) % وبدت وباعث شهوتي للقول قد % % ولي فها أنا أشتهي أن أشتهي % % 14(@)(@@) % حسناء من لي لو بدت وشبيبتي % % لسوى الحسان ووصفها لم يبده % % 15(@)(@@) % ما شيبة في فود مستجلى الدمى % % إلا قذاة بين جفني أمره % % 16(@)(@@) % أحسن بريعان الصبا ولبتره % % ماء على الخدين غير مموه % % 17(@)(@@) % أيام في لعس الشفاه تنقلي % % لثما وفي روض الخدود تفكهي % % 18(@)(@@) % والدهر حيث طلبت مثل مجرد % % والعيش حيث طربت مثل موله % % 19(@)(@@) % عيش كريم كم عتبت بمنطق % % فحشى فمي درا فقال له ره % % 20(@)(@@) % كانت لنا الايام ثم تصرمت % % واعتاض فاقدها بآه عن قه % % PageV01P2200 21(@)(@@) % سقيا لها ولمعشر فارقتهم % % إثر الصبا العادي فراق المكره % % 22(@)(@@) % وقصيدة لو لم يعد عهد الصبا % % عادت بأرفع من سناه وأرفه % % 23(@)(@@) % منظومة الأسلاك في عليا فتى % % عان بحب المكرمات مدله % % 24(@)(@@) % لا عيب فيه غير أن جميله % % وجماله قاض بعجز المدره % % 25(@)(@@) % عمرية أعراقه علوية % % ومديحه لمكرريه شه شهي % % 26(@)(@@) % و هبت يداه ونبهت آراؤه % % فرووا العلى عن وهب بن منبه % % 27(@)(@@) % و أصخ لمدحة ناظم في حجرها % % آوى يتيم النظم غير مسفه % % 28(@)(@@) % أهلا بها من أهل مصر وحبذا % % من منزل بالشام جاد بمنزه % % 29(@)(@@) % جاءت مذكرة الجمال شريفة % % مثل المليحة في إزار لهله % % 30(@)(@@) % ما بين جاريتين وهي سبوقة % % بمدا العلى سبق الجياد السمه % % PageV01P2201 31(@)(@@) % ظهرت وأسكرت العقول فحبذا % % بين المحافل خمرة المستنكه % % 32(@)(@@) % إيه بعيشك يا بديع مقالها % % قل كيف شئت عن الهوى لا أنتهي % % 33(@)(@@) % عارضت أبيات العماد فعاذر % % ولو أنها ذات العماد بأن تهي % % 34(@)(@@) % و تركتها تبكي لآلة سمعة % % قد عطلت بعد العماد الآله % % 35(@)(@@) % و حططت للكندي 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms422# تاج تملك % % عن جبهة من قبلها لم تجبه % % 36(@)(@@) % حتى عن الظليل حجبت الهدى % % وسخرت بالمتنبيء المتأله % % 37(@)(@@) % كم أصفهاني غدا بك أغيرا % في الترب لم يفتح عيون منوه % % 38(@)(@@) % و سليل أعراب فضلت فلم تدع % % لمزهزه وصفا ولا لمجهجه % % 39(@)(@@) % ببديعه ان قالها متحجب % % عنا فلا حجبت مقالة مدره % % 40(@)(@@) % درت بمذهيه الكلامي الذي % % قال البيان لفكره أشعر وافقه % % PageV01P2202 41(@)(@@) % من لو أشار إلى الدقائق كمه % % قرأت خواتمها عيون الأكمه % % 42(@)(@@) % سبق الجدال وقبله سبق الوغى % % فلووا نسيق المازق المتعنه % % 43(@)(@@) % و تعطلت آراء طالب شبههم % % وهم الردى لمعطل ومشبه % % 44(@)(@@) % هذاك أصلهم وهذا فرعهم % % أعظم بفضل المبتدي والمنتهي % % 45(@)(@@) % و ممدح يحصى لمادح فضله % % مصغ فنور يا ربيع ونوه % % 46(@)(@@) % ذي البيت وافته بيوت قصيدة % % لاقت فنحنح يا بيان ونهنه % % 47(@)(@@) % من آل فضل الله والقوم الأولى % % زانوا الزمان وكان مثل مشوه % % 48(@)(@@) % أوروا زناد معاجز ما مسها % % قدح وظنوا كل دهر أدره % % 49(@)(@@) % آثارهم عدد النجوم زواهرا % % وعلاهم عد الزمان المزدهي % % 50(@)(@@) % الصاعد الرتب التي خاضت به % % نهر المجرة لا يقال لها مه % % PageV01P2203 51(@)(@@) % و الكاتب الاسرار يحبس خطوها % % مع أنها في صدره في مهمه % % 52(@)(@@) % أي الممالك لم يشد بالرأي أم % % أي العقول بوصفه لم يبده % % 53(@)(@@) % فالعز في العتبات من أبوابه % % ما العز في صهوات خيل الأجبه % % 54(@)(@@) % حجبت يراعته الخطوب فيالها % % من نعمة عن فضلها لم نعمه % 55(@)(@@) % سد يا علي على ذوي قلم وقل % % ليراعك اضحك بالصرير وقهقه % % 56(@)(@@) % و أمر بما تروي صداي أقم بها % % مدحا يضيق بها بيان الأفوه % % 57(@)(@@) % إني اذا التبس البيان وجدتني % % أضع العمامة عن جبين أجله % % 58(@)(@@) % حررت مدحك في البديع وقلته % % ورأيت كفك والغمام وقلت هي % % PageV01P2204 البحر : - 1(@)(@@) % له إذا غازلتك عيناه % % سهام لحظ أجارك الله % % 2(@)(@@) % و في صفا خده وسالفه % % للحسن ماء الهوى ومرعاه % % 3(@)(@@) % غزال رمل تحلو جنايته % % و غصن بان يعز مجناه % % 4(@)(@@) % من حور رضوان في محاسنه % % لكن نار الفؤاد مأواه % % 5(@)(@@) % أسكنته مهجتي ويا خجلي % % فما أراني أكرمت مثواه % % 6(@)(@@) % لو لقته العذال ما عذلت % % دعها ولا في المنام تلقاه % % 7(@)(@@) % أورى برغمي نار الجفا عوضا % عن برد كنت لاثما 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms423# فاه % % 8(@)(@@) % لا أبعد الله الطيف منه ولا % % أصغر فوق العيون ممشاه % % PageV01P2205 البحر : - 1(@)(@@) % أقول لنظام المحامد يمموا % % مقام ابن شاد في دمشق ومغناه % % 2(@)(@@) % معالي المقام الأفضلي مقيمة % وأمداحه سيارة وعطاياه % % 3(@)(@@) % لئن نزلت عن بلدة يد ملكه % % فما نزلت من بلدة الأفق علياه % % PageV01P2206 البحر : - 1(@)(@@) % يا مليحا كلما زدت خض % % وعا زاد تيها % % 2(@)(@@) % ضرطة باستك قصدي % % فأدرها واسقنيها % % PageV01P2207 البحر : - 1(@)(@@) % يا مولعا بملامي حسبك الله % % كم ذا تهيج مغرى القلب مضناه % % 2(@)(@@) % هذا الحبيب وذا فكري وذا جلدي % % في راحتيه فقل لي كيف أنساه % % 3(@)(@@) % إني لأعلم أن الرشد اجمعه % % في تركه غير أن النفس تهواه % % 4(@)(@@) % ساجي اللواحظ خمري مقبله % % داجي الذوائب بدري محياه % % 5(@)(@@) % إن كان للحب شخص فهو مهجته % % أو كان للحسن لفظ فهو معناه % % 6(@)(@@) % أفديه بدرا بقلب الصب غزوته % % وفي السماء برغم الصب لقياه % % 7(@)(@@) % لو لم يكن ريقة خمرا ومرشفه % % ما عربدت عينه واهتز عطفاه % % PageV01P2208 البحر : - 1(@)(@@) % عذار خديه راق مرآها % % فحبذا ماؤها ومرعاها % % 2(@)(@@) % أخضر نفس الفتى به ألفت % % والنفس خضراء قد عرفناها % % 3(@)(@@) % أجني بها الحسن من تفضل محيي الد % % ين أنشى العلا وأحياها % % 4(@)(@@) % ذو البيت علياه خير شاهدة % % أفلح من بالمديح زكاها % % 5(@)(@@) % آخر أمجاده كأولهم % % كحلقة بدؤها كعقباها % % 6(@)(@@) % شكرا لجدواه أنها سحب % أحيت نبات الرجا بسقياها % % 7(@)(@@) % إذا وصفنا مذاق أنعمه % % حلته أمداحنا وحلاها % % PageV01P2209 البحر : - 1(@)(@@) % يا طرس قبل امرئ فطن % بالفضل لا غافل ولا لاهي % % 2(@)(@@) % تفديه قوم تشبهوا حسدا % به وليسوا له بأشباه % % 3(@)(@@) % ان نطقوا بالجميل أو فعلوا % % فللريا والكمال لله % % PageV01P2210 البحر : - 1(@)(@@) % بروحي صديق حجب الترب شخصه % % وأذكرني معنى حياتي معناه % % 2(@)(@@) % مضى معه اسم قد لقينا به الورى % % ولا بد من أن يتبع اسما مسماه % % 3(@)(@@) % فمالي الا الدمع يخرج ماؤه % % على الترب حتى يخرج الترب مرعاه % % PageV01P2211 البحر : - 1(@)(@@) % أيا واحدا بالمن منه وبالثنا % % تحلت رقاب للورى وشفاه % % 2(@)(@@) % تهن بشهر واضح الفضل مشرق % % تناسب فيما تشتهي طرفاه % % 3(@)(@@) % فإما أخو كيد تفطر قلبه % % واما أخو قلب تفطر فاه % % PageV01P2212 البحر : - 1(@)(@@) % ولي صاحب قد غيرته سعادة % % فما كدت من بعد التواصل ألقاه % % 2(@)(@@) % أرى الشهب في الدنيا يؤثر سعدها % % وهذا شهاب 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms424# أثرت فيه دنياه % % PageV01P2213 البحر : - 1(@)(@@) % أشكو جفا غادة عراني % % من لوعة الحب ما عراها % % 2(@)(@@) % ضنيت والدمع ملء جفني % % فما تراني ولا أراها % % PageV01P2214 البحر : - 1(@)(@@) % ومليح يقول حسن حلاه % % لي جبين بالشعر حف سناه % % 2(@)(@@) % ان رآني هذا وذاك ممن % % أنعم الله صبحه ومساه % % PageV01P2215 البحر : - 1(@)(@@) % يا سيدي عطفا على حالة % % قد زاد مس العسر في صدغها % % 2(@)(@@) % وقد مضى الصوم ولي مقلة % % ما نظرت قطرا سوى دمعها % % PageV01P2216 البحر : - 1(@)(@@) % بهت العذول وقد رأت ألحاظها % % تركية تدع الحليم سفيها % % 2(@)(@@) % فثنى الملام وقال دونك والأسى % % هذي مضايق لست أدخل فيها % % PageV01P2217 البحر : - 1(@)(@@) % يا مذكري بندى يديه وبابه % % شهوات مصر لنا وطيب حماها % % 2(@)(@@) % ان يحل عندي مشتهى أبوابه % % فلقد حلا من سكر هرماها % % PageV01P2218 البحر : - 1(@)(@@) % شرعت يدا قاضي القضاة محمد % % شرع الندى في كل حال واهي % % 2(@)(@@) % فاذا بغى فقري علي حملته % % بيدي إلى شرع ابن عبد الله % % PageV01P2219 البحر : - 1(@)(@@) % أيا سيدي ما لفظ شعري بروضة % % ولا بحره للواردين بمشتهى % % 2(@)(@@) % ولكنه بحر الندى حيث جاءكم % % يكون أجاجا دونكم فاذا انتهى % % PageV01P2220 البحر : - 1(@)(@@) % يا ماجدا ما ظننت همته % % تفعل ما المكرمات تأباه % % 2(@)(@@) % ان لم تكن مدحتي موافقة % % فهاتها قد أقالك الله % % PageV01P2221 البحر : - 1(@)(@@) % يا راحلا من بعد ما أقبلت % % مخايل للخير مرجوه % % 2(@)(@@) % لم تكتمل حولا وأورثتني % % ضعفا فلا حول ولا قوه % % PageV01P2222 البحر : - 1(@)(@@) % أمط بالدواء ثياب الأذى % % و طب في الرواح به والغدو % % 2(@)(@@) % و كرر أحاديث بيت الخلا % % و لكن على رغم أنف العدو % % PageV01P2223 البحر : - 1(@)(@@) % نقل الضنا عن مهجتي % % خبر الصبابة والجوى % % 2(@)(@@) % و حياتكم ما ضل في % % نقل الحديث ولا غوى % % 3(@)(@@) % آها على العيش الذي % % بيد الفراق قد انطوى % % 4(@)(@@) % ما كان أسرع ما انقضى % % وحصلت منه على الهوى % % 5(@)(@@) % عجبا لمثلي ما على % % نأي الحبيب له قوى % % 6(@)(@@) % يقوى لنبل الراشقي % % ن وليس يقوى للنوى % % PageV01P2224 البحر : - 1(@)(@@) % لو ساعدتني حالة كان لي % % ببعض من لاقكم أسوه % % 2(@)(@@) % حتى ترى عيني مقام العلى % % وكعبة المعروف في الكسوه % % PageV01P2225 البحر : - 1(@)(@@) % قبلته عند النوى فتمررت % % تلك الحلاوة بالتفرق والجوى % % 2(@)(@@) % ولثمته عند القدوم فحبذا % % رطب الشفاه السكري بلا نوى % % PageV01P2226 البحر : - 1(@)(@@) % بعثت اليك الشكر عما بعثته % % ومالي 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms425# بغير الشكر يا سيدي قوى % % 2(@)(@@) % ولما انقضى عهد النوى جئت بالندى % % هنيئا فيالله تمر بلا نوى % % PageV01P2227 البحر : - 1(@)(@@) % أهلا بمقدم صاحب العلم الذي % % فضل السيوف بجوده وبسطوه % % 2(@)(@@) % ود الملاح خدودهم وعيونهم % % عوض الأزاهر والمروج لخطوه % % PageV01P2228 البحر : - 1(@)(@@) % يا عاليا للنجم لا يهوى به % % أفق اذا ما النجم من أفق هوى % % 2(@)(@@) % يفديك كل مؤمل لك قائل % % حاشا لجسمك أو لنفسك من هوى % % PageV01P2229 البحر : - 1(@)(@@) % افتح دواة فضائل وفواضل % نعم العيان لمن رأى ولمن روى % % 2(@)(@@) % تشفي ضعيف الحال منها مدة % % فهي الدواة لمن تأمل والدوا % % PageV01P2230 البحر : - 1(@)(@@) % يا دار بطيخ بمصر عهدتها % % مأوى لمن أهوى ونعم الآوي % % 2(@)(@@) % أنا ان لوى عذارا أخضرا % في جنة المعهود عبد لاوي % % PageV01P2231 البحر : - 1(@)(@@) % كان لابن الوكيل بالشعر علم % مستجاد لكن قليل الطلاوه % % 2(@)(@@) % وأرى صدر وقتنا قائلا ما % % كل صدر يهدى لهذي الحلاوه % % PageV01P2232 البحر : - 1(@)(@@) % تهن يا مجزل العطايا % % قدوم شهر له طلاوه % % 2(@)(@@) % حلا وأثنى عليك صدقا % % فهو اذا صادق الحلاوه % % PageV01P2233 البحر : - 1(@)(@@) % أمنزل ذات الخال حييت منزلا % % وان كان قلبي فيك بالوجد مبتلى % % 2(@)(@@) % لك الله قلبا لا يزال مقيدا % بشجو ودمعا لا يزال مسلسلا % % 3(@)(@@) % يعبر عن سر الهوى وأضيعه % % فيا لك دمعا معربا راح مهملا % % 4(@)(@@) % كفى حزنا أن لا أراقب لمحة % % ولا أنظر اللذات الا تخيلا % % 5(@)(@@) % ولا أستزير الطيف خوف فراقه % % لما ذقت من طعم التفرق أولا % % 6(@)(@@) % وأقسم لو جاد الخيال بزورة % لصادف باب الجفن بالفتح مقفلا % % 7(@)(@@) % وأغيد قد أضنى العواذل أمره % % فقل في أسى أضنى محبا وعذلا % % 8(@)(@@) % غرير رنت أجفانه ووصفنه % % فراح كلانا في الورى متغزلا % % 9(@)(@@) % اذا شئت أن أشدو بأوصاف ثغره % % بدأت ببسم الله في النظم أولا % % 10(@)(@@) % حذار عوادي القتل من سيف طرفه % % فما كسر الاجفان الا ليقتلا % % PageV01P2234 11(@)(@@) % بليت به ساجي اللحاظ كليلها % % وما زال تعذيب الكليلة أطولا % % 12(@)(@@) % اذا ما بدا أو ماس أو صان أورنا % % فما البدر والخطي والليث والطلا % % 13(@)(@@) % وقالوا أتحكيه الغزالة في الضحى % % فقلت ولا لحظ الغزالة في الفلا % % 14(@)(@@) % فلا تنكرا منه حلاوة لحظه % % فذاك أراه بالنعاس معسلا % % 15(@)(@@) % ولا تعجبا من ردفه وثباته % % فلولا وشاحا عطفه لتهيلا % % 16(@)(@@) % غدا 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms426# البدر أن يحكي سناه وانما % % رأى مللا من خلقه فتنقلا % % 17(@)(@@) % وماثل ريق النحل لذة ريقه % % فقال اللمى ما أخجل المتنحلا % % 18(@)(@@) % تبارك من جلى صحائف أوجه % وأوضح آيات الثغور ورتلا % % 19(@)(@@) % و شيد للملك المؤيد رتبة % % من المجد تملي المادح المتوسلا % % 20(@)(@@) % مليك رقى قبل الصبا كاهل العلى % % فكيف وقد أبصرته متكهلا % % PageV01P2235 21(@)(@@) % كريم الثنا نال الكواكب قاعدا % وجاوزغايات العلى متمهلا % % 22(@)(@@) % تخاف الغوادي من نداه كسادها % % وما نفحت كفاه الا لتفعلا % % 23(@)(@@) % يقولون أعدى باليمين يساره % % فجادت فمن أعدى الذي جاد أولا % % 24(@)(@@) % و من في المعالي قد تقدم ورده % % أجل انها عادات آبائه الاولى % % 25(@)(@@) % ملوك اذا قام الزمان لمفخر % % غدا بليالي ملكهم متجملا % % 26(@)(@@) % كرام ثووا ثم استقل حديثهم % % فأحزن في عرض البلاد وأسهلا % % 27(@)(@@) % أناملهم تحت الثرى ربع مائه % % وأقدامهم يكفيه أن يتزلزلا % % 28(@)(@@) % رقوا ما رقوا من سؤدد ثم قوضوا % % فزاد على ما أنهجوه من العلا % % 29(@)(@@) % هنيئا لدست الملك بدرا وغرة % % اذا انهل في يوم الندى وتهللا % % 30(@)(@@) % دع الغيث سار البرق والطود راسيا % ويممه ان راع الزمان وأمحلا % % PageV01P2236 31(@)(@@) % لراحة اسماعيل أصدق موعدا % وساحته الفتحاء أمنع مقفلا % % 32(@)(@@) % هنالك تلقى أنعما تترك الثرى % % يراد وعزما يترك الماء يصطلى % % 33(@)(@@) % و أصيد من نسل الملوك اذا انتدى % % رأيت معما في السيادة مخولا % % 34(@)(@@) % أخا كرم تبغي العواذل عطفه % % فتلقاه أندى ما يكون معذلا % % 35(@)(@@) % دنا رفده قيد الوريد وانما % % ترفع حتى خاطب النجم أسفلا % % 36(@)(@@) % فداه كرام العالمين فإنه % % أبرهم مالا وأشرف موئلا % % 37(@)(@@) % اذا فاخر الانداد جاء فخاره % % بهذا الثنا يستوقف المتأملا % % 38(@)(@@) % و بالعلم وضاح الهدى متألقا % وبالحلم فياح الجنا متهدلا % % 39(@)(@@) % و بالمنطق الأزكى أسد محررا % وبالسؤدد الأجلى أغر محجلا % % 40(@)(@@) % و بالزهد موصول القيام كأنما % % يغازل طرفا من دجى الليل أكحلا % % PageV01P2237 41(@)(@@) % وبالبأس سل عنه الصوارم في الوغى % % وكانت مواضي البيض أفصح مقولا % % 42(@)(@@) % و ما هي الاهمة ملكية % % قضى عزمها فرض العلى وتنفلآ % 43(@)(@@) % يخص سجاياها الوفا وهو مسلم % % وكان يهوديا يخص السموألا % % 44(@)(@@) % و يغني عن الأمداح مشهور فضلها % % وماالصبح محتاج إلى الوصف والحلى % % 45(@)(@@) % و ما الشمس في أفق السماء منيرة % % تخال بها من ضحوة 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms427# الغيظ أفكلا % % 46(@)(@@) % بأوضح للأبصار من مجده الذي % % توقد حتى لم تجد متوقلا % % 47(@)(@@) % ثنى رجله فوق النجوم ولو علت % % وطالت ثنى باعيه أعلى وأطولا % % 48(@)(@@) % و ما روضة خاطت بها إبرة الحيا % % من الودق ثوبا علق الوشي مسبلا % % 49(@)(@@) % بأعبق من أوصافه الغر نفحة % % وابرع من ألفاظه الزهر مجتلى % % 50(@)(@@) % أوابد قد أعيي امرء القيس قبلنا % % سنا نجمها الهادي فمات مضللا % % PageV01P2238 51(@)(@@) % له راحة ضمت يراعا ومرهفا % كأنهما زاداه بالمكث أنملا % % 52(@)(@@) % يراعا اذا مدته يمناه بالندى % % رأيت عباب البحر قد مد جدولا % % 53(@)(@@) % وسيفا كأن القين سواه جذوة % % فلو لم يعاهد بالطلا لتأكلا % % 54(@)(@@) % مبيد لو أن المرء ضاعف درعه % % ومثله في نفسه لتجدلا % % 55(@)(@@) % يؤيد خديه يد ضربت به % % دراكا فما تحتاج كالبيض صيقلا % % 56(@)(@@) % ألا رب شأو رامه فتسهلت % % رباه وصعب راضه فتذللا % % 57(@)(@@) % وجيش كأن الجو قد مد أنجما % % عليه ووجه الأرض أنبت دبلا % % 58(@)(@@) % كأن عتاق الطير بين رماحه % % بنود تهاوى للطعان وتعتلى % % 59(@)(@@) % اذا نبضت يوما بواد قسيه % % تلبس ثوب النقع بالنبل مجملا % % 60(@)(@@) % رماه بعزم فانجلى ليل خطبه % % ولو رامه الصبح المنير لما انجلى % % PageV01P2239 61(@)(@@) % وذي ظمأة بادي الخمول توعرت % % عليه مساري الرزق حتى تحيلا % % 62(@)(@@) % علا وارتوى لما دعاه كأنما % % يشافه من حوض الغمامة منهلا % % 63(@)(@@) % وبيداء مقفار اليه قطعتها % % فلاقيت معلوما وفارقت مجهلا % % 64(@)(@@) % وقلت لخلي انزلاني فهذه % % منازله ثم أعقلا وتوكلا % % 65(@)(@@) % هنالك عاهدت الرياض أنيقة % % ترف وجاورت الغمائم هملا % % 66(@)(@@) % وقضيت في ظل النعيم لياليا % لو انتقضت كانت كواكب تجتلى % % 67(@)(@@) % ولا عيب في نعمائها غير أنها % % تجود فتوهي الكاهل المتجملا % % 68(@)(@@) % وإني اذا أجهدت مدحي فإنما % % قصاراي منها أن أقول فأخجلا % % 69(@)(@@) % لبابك يا ابن الأكرمين بعثتها % % أؤانس من مدح عن الغير جفلا % % 70(@)(@@) % وأرسلتها غراء كالغصن يانعا % % وزهر الربى ريان والريح سلسلا % % PageV01P2240 71(@)(@@) % ممنعة المغزى تجر برأسه % % جريرا وتلقي من جرى الكلب جرولا % % 72(@)(@@) % شببت لها فكري وفاحت حروفها % % كأني قد دخنت في الطرس مندلا % % 73(@)(@@) % واعتقت رقي من خمول عهدته % % فخرت ولا قلبي وللمعتق الولا % % 74(@)(@@) % وأنت الذي أسعفتني فصنعتها % % ولولا الحيا لم يصبح الترب مبتلا % % 75(@)(@@) % فلو رامها الطائي من قبل لم يقل % % لهان 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms428# علينا أن نقول ونفعلا % % 76(@)(@@) % وكم مثلها أهديتها طي مدرج % % تكاد لفرط الشوق أن تتسللا % % 77(@)(@@) % يفوه بها الراوي فيملأ لفظها % % فم الخل درا أو فم الضد جندلا % % 78(@)(@@) % جمعت بنعمى راحتيك فنونها % % كما جمع السلك الجمان المفصلا % % 79(@)(@@) % ومثلك من حلت أياديه حسنها % % فزاد وثنى حظها فتكملا % % 80(@)(@@) % بقيت لهذا الدهر تبسط إن أسا % % يديك فما ينفك أن يتنصلا % % PageV01P2241 81(@)(@@) % ودمت لشأو المجد بالطول راقيا % % وممن طلب المجد العلي تطولا % % 82(@)(@@) % حلفت يمينا ليس مثلك في الورى % % فما شرع الاسلام أن أتحللا % % PageV01P2242 البحر : - 1(@)(@@) % بعثت طيفها الينا رسولا % % فبلغنا من الزيارة سولا % % 2(@)(@@) % ثم ولى فليت أنا قدرنا % % فاتخذنا مع الرسول سبيلا % % 3(@)(@@) % يا له واصلا الي وما كا % % د بدمعي أن يستطيع وصولا % % 4(@)(@@) % خل يا دمع مقلتي في الدجى إن % لها في النهار سبحا طويلا % % 5(@)(@@) % و أعد يا نسيم أخبار مصر % ربما طارح العليل عليلا % % 6(@)(@@) % أنت لا شك من صبا أرض مصر % فلهذا أرى عليك قبولا % % 7(@)(@@) % و ملول هويته غير أني % % لا أراه من الملال ملولا % % 8(@)(@@) % ذو جمال على بثينة يزهى % % يا شكاة الهوى فصبرا جميلا % % 9(@)(@@) % و رضاب حماه رمح التثني % % فهوينا العسال والمعسولا % % 10(@)(@@) % جل رب أعطاه تحسين مرآ % % ه وأعطى الأفضل التفضيلا % % PageV01P2243 11(@)(@@) % ملك قد زهى به مربع المل % % ك فحيي فروعه والأصولا % % 12(@)(@@) % شادوي ما فيه لو يوم وصف % لا ولا للسؤال في لفظه لا % % 13(@)(@@) % عذلوا جوده وشيمته الغرا % % ء ترضي الورى وتعطي العذولا % % 14(@)(@@) % فيه بشر وفيه للروع حد % مثل ما ينتضي الحسام الصقيلا % % 15(@)(@@) % نعم تترك الذليل عزيزا % وسطا تترك العزيز ذليلا % % 16(@)(@@) % و مقيم على محاريب نسل % % حسبه نور وجهه قنديلا % % 17(@)(@@) % فإذا رامه العداة بكيد % % أخذتها الأيام أخذا وبيلا % % 18(@)(@@) % حاش لله أن نرى لك ضدا % يا ابن أيوب في العلى أو مثيلا % % 19(@)(@@) % لك بيت في الملك قد جمع الأوزا % % ن جمعا يوافق التفعيلا % % 20(@)(@@) % كرما وافرا ومجدا مديدا % وثنا كاملا وذكرا طويلا % % PageV01P2244 21(@)(@@) % و على شخصك الكريم من السؤ % % دد نور يكفي العقول دليلا % % 22(@)(@@) % كم سمعنا عن فضله وشهدنا % % فحمدنا المنقول والمعقولا % % 23(@)(@@) % دمتم للفخار يا آل أيو % % ب وبوركتم أبا 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms429# وسليلا % % 24(@)(@@) % كيف أنسى نوالكم وهو حولي % % أتلقاه بكرة وأصيلا % % 25(@)(@@) % لم أذق صد جودكم فأغني % % قمت ليل الصدود إلا قليلا % % PageV01P2245 البحر : - 1(@)(@@) % إن طيفا عن حال شجواي أملي % % لست أدري أدى الأمانة أم لا % % 2(@)(@@) % جاء ضيفا ورده سهد عيني % فولى بي الهموم وولى % % 3(@)(@@) % ليت طيف الحبيب ينقل جسمي % % لا حديثي فكان يحسن نقلا % % 4(@)(@@) % بأبي من إذا تثنى دلالا % أطرقت في رياضها القضب خجلا % % 5(@)(@@) % فاتك اللحظ وهو حلو مع الف % % تك فيا حبذا الحسام المحلى % % 6(@)(@@) % عرف الناس سحر عينيه لما % % هز جفنا فصير الجفن نصلا % % 7(@)(@@) % و عليه تأصل الحب لما % % مد فرعا فصير الفرع أصلا % % 8(@)(@@) % مد صدغا على عذار وخد % فرأينا مرعى وماء وظلا % 9(@)(@@) % ورنا بعده الغزال فقلنا % % حط يا ظبي عن جفونك ثقلا % % 10(@)(@@) % ليس يسلى هواه من قلب صب % % ونعم فوق نار خديه يسلى % % PageV01P2246 11(@)(@@) % يا سلوي عليه بعدا وسحقا % واشتياقي اليه أهلا وسهلا % % 12(@)(@@) % أشتكي جوره التذاذا بذكرى % % شخصه كالأريحي منه عدلا % % 13(@)(@@) % عجبي منه ظالما مستطيلا % وهو إن ماس أعدل الناس شكلا % 14(@)(@@) % باخل بالكلام لكن له سيا % ف لحظ تكلم الناس طفلا % % 15(@)(@@) % يا بخيلا بلفظه ولقاه % % شذ ما قد بخلت قولا وفعلا % % 16(@)(@@) % خنت عهدي ولست أول خل % % خان بعد الولاء والود خلا % 17(@)(@@) % رب يوم قد كان ريقك فيه % % لي راحا وكان خدك نقلا % % 18(@)(@@) % سائلي عن قديم دهري إيها % % ذاك وقت مضى ودهر تولى % % 19(@)(@@) % و ليال جادت وأعقبت اله % % م فياليت جودها كان بخلا % % 20(@)(@@) % و حبيب جفا ولست بسالي % % ه وحاشا ذاك الجمال وكلا % PageV01P2247 21(@)(@@) % تتقلى به العواذل غبنا % فهو يهوى وعذلي فيه ثقلى % % 22(@)(@@) % عذلوني وفي الحشا عقد ود % لم يدع لاستماع عذل محلا % % 23(@)(@@) % أنافي الحب مثل قاضي قضاة الد % % ين في الجود ليس يسمع عذلا % % 24(@)(@@) % معرف في العلى لماضيه يتلو % % وثناه على البسيطة يتلى % % 25(@)(@@) % دلفي يوم الفخار يجلى % % وبه منهم الخطوب تجلى % % 26(@)(@@) % حاز غايات أهله بمساع % % قدمته إلى السيادة أهلا % % 27(@)(@@) % فأفاض الجودين عدلا ومالا % وحمى الجانبين حزنا وسهلا % % 28(@)(@@) % وحرام أن يطرق العسر والجو % % ر فتى كان في مغانيه حلا % 29(@)(@@) % همة تحسب النجوم على الأف 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms430# % % ق شعاعا من جرمها يتجلى % % 30(@)(@@) % وعلوم فاضت على الأرض بحرا % هاديا لم يعف كالبحر سبلا % % PageV01P2248 31(@)(@@) % كم قضى فرض قاصد لحماه % % ثم والى فأتبع الفرض نفلا % % 32(@)(@@) % كم جنينا منه المواهب شهدا % إذ بنينا له الركائب نملا % % 33(@)(@@) % كم إلى بيت ماله في العطايا % % قد ضربنا بطالع العيس رملا % % 34(@)(@@) % لائميه على المكارم كفوا % % إن للصب بالصبابة شغلا % % 35(@)(@@) % يا له سالكا بغير مثيل % % في طريق من السيادة مثلى % % 36(@)(@@) % وإماما أقلامه كل يوم % % تتلقى الاقلام قدح معلى % % 37(@)(@@) % صان للفضل ذمة وحوى العل % % م جميعا فلم تقل فيه إلا % % 38(@)(@@) % لو أرادت شهب النجوم علاه % % ما عزا الفيلسوف للشهب عقلا % % 39(@)(@@) % ما ألذ النعمى لديه وما أش % % قى حسودا بناره بات يصلى % % 40(@)(@@) % وعدوا ان لم ينازله بالقت % % ل كفاه سيف التحسد قتلا % % PageV01P2249 41(@)(@@) % أضعف الهم جسمه فاذا قا % % ل لرجليه بادري كتبت لا % % 42(@)(@@) % قد بلونا السادات شرقا وغربا % % فوجدنا جلال علياه أجلى % % 43(@)(@@) % قيل يعني عطاردا قلت لا بل % % مشتري الحمد بالنفائس بذلا % % 44(@)(@@) % يا إماما اذا المفاخر نادت % % ه مشى ساحب الذيول مدلا % % 45(@)(@@) % أتشكى لك الزمان الذي تمل % % ك إصلاحه لدي فهل لا % % 46(@)(@@) % ومقام للعلم لولا نظام % % من مساعيك ما تنظم شملا % % 47(@)(@@) % ومحاريب شدتها بدروس % % وصلاة تحبى اليها وتجلى % % 48(@)(@@) % حبذا أنوار شخصك في سجا % % د محرابه النقى والمصلى % % 49(@)(@@) % رب مدح لولاك أمسى محالا % % ورجاء لولاك أصبح محلا % % 50(@)(@@) % حبذا لي مدائح فيك تبدى % % من حياء كالروض يحمل طلا % PageV01P2250 51(@)(@@) % طال إملاؤها عليك ولكن % % لك كف من العطا لم يملا % % 52(@)(@@) % عادة لامها النصيح على البذ % % ل فقالت سجية الأصل مهلا % % 53(@)(@@) % إن أكن أحسن الثنا فيك قولا % % فلقد أحسنت أياديك فعلا % % 54(@)(@@) % زادك الله بسطة واقتدارا % % ومقاما على السهى ومحلا % % 55(@)(@@) % جمع الله فيك ما عز في الخل % % ق فسبحانه وعز وجلا % % PageV01P2251 البحر : - 1(@)(@@) % بدت ورنت لواحظه دلالا % % فما أبهى الغزالة والغزالا % % 2(@)(@@) % وأسفر عن سنا قمر منير % ولكني وجدت به الضلالا % % 3(@)(@@) % صقيل الخد أبصر من رآه % % سواد العين فيه فخال خالا % % 4(@)(@@) % وممنوع الوصال اذا تبدى % % وجدت له من الألفاظ لالا % % 5(@)(@@) % وأعجب اذ وضعت سلاح صبري % % لمنظره وما 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms431# رفع القتالا % % 6(@)(@@) % عجبت لثغره البسام أهدى % % لنا درا وقد سكن الزلالا % % 7(@)(@@) % شهدت بشهد ريقته لاني % % رأيت على سوالفه نمالا % % 8(@)(@@) % و أشهد أن في خديه جمرا % % لان بمهجتي منه اشتعالا % % 9(@)(@@) % فيا لنعيم جسم قد حواه % % وقد أهدى إلى قلبي الوبالا % % 10(@)(@@) % سأشكو الحزن ما بقيت حياتي % % وأشكر في صنائعه الجمالا % % PageV01P2252 11(@)(@@) % على حمد ابن محمود استقرت % % عقول العالمين ولا جدالا % % 12(@)(@@) % رئيس للعلى طالت يداه % % ولم يفخر بذاك ولا استطالا % % 13(@)(@@) % بديهي المواهب يوم جود % % اذا روى الورى وهب ارتجالا % % 14(@)(@@) % ونحوي العوارف يوم جاه % فكم نصبت على التمييز حالا % % 15(@)(@@) % و كم عطفت لذا من بعد هذا % % وكان العطف والبذل اشتمالا % % 16(@)(@@) % لقد زهت العواصم يوم وافى % % وأمست عصمة وغدت ثمالا % % 17(@)(@@) % و صح حمى الشمال بيمن رأي % % أنال من السعادة ما أنالا % % 18(@)(@@) % فما يشكو سوى لحظ الغواني % % ونشر الروض سقما واعتدالا % % 19(@)(@@) % و كيف وقد تولى في حماه % % علي القدر ذو كرم توالى % % 20(@)(@@) % حكى السبع الشداد علا وحاكت % % عليه مدائحي السبع الطوالا % % PageV01P2253 21(@)(@@) % أعاذله على المعروف دعه % % فإن له به عنك اشتغالا % % 22(@)(@@) % و طالب شأوه في المجد أقصر % % ودع ليث العرينة يا ثعالى % % 23(@)(@@) % له قلم يكف الخطب كفا % % وينهمل الندى منه انهمالا % % 24(@)(@@) % إذا جلى الحروف فلست أرضى % % سنا ابن هلال ثم ولا الهلالا % % 25(@)(@@) % تجانس صنعه فترى سجلا % يروق وفي النوال ترى سجالا % % 26(@)(@@) % براحة منعم تعبت فسادت % % وحاول طوله العليا فطالا % % 27(@)(@@) % وثقت بجوده فرأيت مالا % أرى من غيره وكنزت مالا % % 28(@)(@@) % ألم تر أنني في كل عام % إلى طلب العلى أبغي الشمالا % % 29(@)(@@) % بإسماعيل ابتدئ الأيادي % % وابراهيم اختتم النوالا % % 30(@)(@@) % لقد رفعا قواعد بيت جود % % دعا حج المقاصد واستمالا % % PageV01P2254 31(@)(@@) % و لا والله لا أزجي ركابا % لغيرهما ولا أنهي سؤالا % % 32(@)(@@) % إليك جمال دين الله قصدا % تعود منك عزما واحتفالا % 33(@)(@@) % و كنت بلوت برك من قديم % % فلم أصرف لغير حماك بالا % % 34(@)(@@) % رعاك الله ما دعي ابن غيث % % وزاد ندى يديك ولا أزالا % % 35(@)(@@) % لقد حسنت فعالك في البرايا % % فحسن فيك مادحك المقالا % % PageV01P2255 البحر : - 1(@)(@@) % دعوني لذكرى حسنه أقتضي العذلا % % ليملأ سمعي عنه أحسن ما يملى % % 2(@)(@@) % بروحي أمر 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms432# الناس نأيا وجفوة % وأحلاهم ثغرا وأملحهم شكلا % % 3(@)(@@) % يقولون في الأحلام يوجد شخصه % % فقلت ومن ذا بعده يجد الأحلا م % % 4(@)(@@) % ومن لي بطرف يستزير خياله % % وقد حلف التسهيد من بعده أن لا % % 5(@)(@@) % روى وجهه من تحت صدغيه معرضا % فأعدم طرفي ذلك الروض والظلا % 6(@)(@@) % و كلفتني في رحلتي وإقامتي % % على حسنه المطلوب أن أضرب الرملا % % 7(@)(@@) % كأني لم أختم على تبر خده % % بلثم ولم أجعل عناقي له قفلا % % 8(@)(@@) % ولم يسع نحوي شخصه أو خياله % % فان لم أصب من وصله الوبل فالطلا % % 9(@)(@@) % على أن لي فيه أماني فكرة % % أعيد على رغم الحسود بها الوصلا % % 10(@)(@@) % و لي في الذي أهوى هوى فلوانه % % تكلف لي عطفا لناديته مهلا % % PageV01P2256 11(@)(@@) % و كان بودي لو أطقت تسليا % فحققت عنه صبوتي كلما ملا % 12(@)(@@) % و حملت عنه ما عناه فلم أدع % % على خصره سقما ولا جفنه ثقلا % % 13(@)(@@) % تحكم في ودي لديه وسلوتي % % فأحسن في أحكامه العقد والحلا % 14(@)(@@) % و إني على ظني به وصبابتي % % لأقنع من يدري على الطرف أن يجلى % % 15(@)(@@) % أبى الله أن يجزي بذكرى أسرة % % تطفلت في العليا على مجدهم طفلا % % 16(@)(@@) % فيالك بيتا لا يقال لأهله % % عزيز علينا أن نرى ربعكم يبلى % % 17(@)(@@) % و لو حل بي طيفا وللراح سورة % بعقلي لم أسلك به غير ما حلا % % 18(@)(@@) % سجية آباء كرام ورثتها % % وفقه عفاف يجمع الفرع والأصلا % % 19(@)(@@) % و يدعو حماه طالبا بعد طالب % % إلى المال يستجدى أو العلم يستجلى % % 20(@)(@@) % فياليت شعري هل أراني واقفا % على بابه لا أقتضي أقتضي الكتب والرسلا % % PageV01P2257 21(@)(@@) % فآوي بشط النيل طرفي وناقتي % % وأطرح في تياره السرح والرحلا % % 22(@)(@@) % و أسكن حيث الشهب حصباء واطئ % % وحيث يمد العز من فوقها ظلا % % 23(@)(@@) % و حيث أصوغ اللفظ أهلا لمدحه % % وأما سوى لفظي هناك فلا أهلا % % 24(@)(@@) % و حيث زماني فهو ضد معاكس % % يعود إذا طارحته صاحبا خلا % % 25(@)(@@) % أقول أبو جهل فلما أحفني % % ظلال الحمى العالي أقول أبي جهلا % % 26(@)(@@) % هنيئا لوفد سائرين لبابه % % لقد حمدوا المسرى وقد عرفوا السبلا % % 27(@)(@@) % و ان امرأ أسرت اليه جياده % % ليعظم أن يرضى الهلال لها نعلا % % 28(@)(@@) % وإن لقاضي المسلمين 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms433# عوارفا % بها كم أقمنا للثنا شاهدا عدلا % % 29(@)(@@) % ونحوا من العلياء نزه وضعه % % فما الاسم منقوص ولا الفعل معتلا % % 30(@)(@@) % ردوا بحره واستصغروا ورد جعفر % % وقيسوا به الآمال واطرحوا الفضلا % % PageV01P2258 31(@)(@@) % بني دلف طبتم وطاب قديمكم % % فأكرم بكم فرعا وأكرم بكم أصلا % % 32(@)(@@) % و جزتم مدا العلياء لم يتل سبقكم % % ولكن على الأسماع ذكركم يتلى % % 33(@)(@@) % فلا طرقت أيدي الخطوب لكم حمى % % ولا فرقت عين الزمان لكم شملا % % PageV01P2259 البحر : - 1(@)(@@) % حاشاك من وحشة تحت الثرى وجلا % % ياسائرا صرت في حزني له مثلا % % 2(@)(@@) % سقيا لقربك والأيام عاطفة % % والقلب يسحب أذيال الهنا جذلا % % 3(@)(@@) % والسمع قد صم عن نجوى عواذله % % وسيف جفنك عندي يسبق العذلا % % 4(@)(@@) % حيث التبسم طلاع الثنية من % % فرط السرور وبشر الطلعة بن جلا % % 5(@)(@@) % فبينما أنا معطوف على سكن % حتى تحركت الأيام فانتقلا % % 6(@)(@@) % أشكو إلى الله بينا لا انقضاء له % % ورحلة للنوى لا تشبه الرحلا % % 7(@)(@@) % بينا أرى فيه للنعش انبعاث سرى % % لا ناقة للسرى فيه ولا جملا % % 8(@)(@@) % فليت أن بنات النعش تسعدني % % بأدمع النوء للبدر الذي أفلا % % 9(@)(@@) % لهفي عليك وهل لهف بنافعة % % إذا تحدر دمع العين وانهملا % % 10(@)(@@) % لم يترك الدهر من أوقات منتظري % % إلا وآخر عمر تندب الأولا % % PageV01P2260 11(@)(@@) % و تربة يتلقى الحزن زائرها % % كأنها تنبت التبريح والوجلا % % 12(@)(@@) % حديثه الظهر إلا أن باطنها % % قد استجن جنان الروضة الخضلا % % 13(@)(@@) % أستوقف الجسد المضنى لأندبها % % يا من رأى نادبا يستوقف الطللا % % 14(@)(@@) % متيما نصلت فودا شبيبته % % وقلبه من حداد الحزن ما نصلا % % 15(@)(@@) % يا غائبا ذهبت أيدي الحمام به % % بعدا ليومك ماذا بالحشا فعلا % % 16(@)(@@) % إن ينأ شخصك اني بعد فرقته % % أدنى وأيسر ما قاسيت ما قتلا % % 17(@)(@@) % أو ينقضي للمنايا بعدنا شغل % % فقد تركن بقلبي للأسى شغلا % % 18(@)(@@) % آها لعطف معان فيك ذي نسق % % جعلت من بعده نار الأسى بدلا % % 19(@)(@@) % هلا بغيرك ألقى الموت جانبة % % لقد تأنق فيك الموت واحتفلا % % 20(@)(@@) % هلا قضى غصنك الزاهي شبيبته % % فما ترعرع حتى قيل قد ذبلا % % PageV01P2261 21(@)(@@) % أفدي الذي كان لي عيشا ألذ به % % فما أبالي أجاد العيش أم بخلا % % 22(@)(@@) % دعا التجلد قلبي يوم رحلته % % فقلت لا ودعا سقمي فقال هلا 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms434# % % 23(@)(@@) % سقم ملكت به معنى النحول فإن % % جاء الخلال بسقم جاء منتحلا % % 24(@)(@@) % و مقلة قد طغى إنسان ناظرها % % فكان أكثر شيء بالبكا جدلا % % 25(@)(@@) % لا نلت قربك من دار النعيم غدا % ان كان قلبي المعنى عن هواك سلا % % 26(@)(@@) % يا منية الصب أما ثكل مهجته % % فقد أقام وأما صبرها فخلا % % 27(@)(@@) % ما أحسن العيش في عيني وأنت به % % أما وأنت بأكناف التراب فلا % % 28(@)(@@) % سقي ضريحك رضوان ولا برحت % % ركائب السحب في أقطاره ذللا % % PageV01P2262 البحر : - 1(@)(@@) % يا صاحبي أرانا الدهر شوالا % % فبادرا وانصبا للذة الحالا % % 2(@)(@@) % لا تحذرا مع عفو الله موبقة % % تحصى ولا مع ندى السلطان اقلالا % % 3(@)(@@) % جاد المؤيد حتى كدت أحسبه % % مع فضل فطنته لا يعرف المالا % % 4(@)(@@) % ولا كحلت بمرأى مثله بصري % % هذا وقد جبت ظهر الأرض أميالا % % 5(@)(@@) % فليهنه من هلال العيد مقترف % % يدنو فيركع إعظاما وإجلالا % % 6(@)(@@) % حتى ترى نونه من فرط خدمتها % % تود لو صيرت في أفقها دالا % % PageV01P2263 البحر : - 1(@)(@@) % ملوز الطرف أهلا % كل الحلا إن تحلى % % 2(@)(@@) % وحاكم العقل يقضي % % إن الملوز أحلى % % 3(@)(@@) % وخشكنانا أتاني % % في مثل عيدي فتلا % % 4(@)(@@) % من أفضل الناس نفسا % وأنفس الناس فضلا % 5(@)(@@) % وفي انتساب وعلم % % أجل فرعا وأصلا % % 6(@)(@@) % علي هنئت عيدا % في الصيغتين محلى % % 7(@)(@@) % أنهيت عالي قصدي % % فيه ورأيك أعلى % % PageV01P2264 البحر : - 1(@)(@@) % طلبت ري الغليل منها % % وعاذلي يطلب المحالا % % 2(@)(@@) % عنفني ثم قال تسلى % % عن حب ماما فقلت لالا % % PageV01P2265 البحر : - 1(@)(@@) % ياحبيب القلب أهلا % بالهوى فيك وسهلا % % 2(@)(@@) % ما ألذ الوجد عندي % % في معانيك وأحلى % % 3(@)(@@) % غزلت عيناك لي % % ثوب سقم ليس يبلى % % 4(@)(@@) % فاقض لي ما أنت قاض % % لست ممن يتسلى % % 5(@)(@@) % لا وشعر لك داج % وجبين يتجلى % % 6(@)(@@) % لا تسليت ولا قل % % ت لألحاظك مهلا % % 7(@)(@@) % لا ولا استدفعت صدا % منك واستدعيت وصلا % % 8(@)(@@) % غير أن العبد ينهي % % حاله والرأي أعلى % % PageV01P2266 البحر : - 1(@)(@@) % متع لواحظنا التي أضنيتها % % لما اتخذت إلى البعاد سبيلا % % 2(@)(@@) % وأعد بعودك للعيون منامها % % فلقد ترحل يوم رمت رحيلا % % 3(@)(@@) % أولا فنظرتها اليك ألذ من % % عود المنام لو جفته طويلا % % 4(@)(@@) % يا قادما أقسمت لو قسم الورى % % حر الخدود له لكان قليلا % % 5(@)(@@) % أهلا بقربك فهو كحل نواظر % % كم راقبت من 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms435# نحو أرضك ميلا % % 6(@)(@@) % صحت بك الأيام حتى ما يرى % % متأمل إلا النسيم عليلا % % PageV01P2267 البحر : - 1(@)(@@) % دم يا علاء الدين في رفعة % % رأيك فيما يقتضي أعلى % % 2(@)(@@) % كتاب مولانا بأشفاقه % % لا يختشي من سفر ثقلا % % 3(@)(@@) % يصطاد في المشتى مهماتكم % % ونحن نصطاد من المقلى % % 4(@)(@@) % لكن لي في الشام يا سيدي % % قرائن من همها حبلى % % PageV01P2268 البحر : - 1(@)(@@) % بأبي غصن كبدر % % قد تثنى وتجلى % % 2(@)(@@) % قلت اذ اضمر قصدي % % قبلة يا بدر هلا % 3(@)(@@) % قال من خدي خذها % % قلت بل من فيك أحلى % % PageV01P2269 البحر : - 1(@)(@@) % يا مهديا من خطه قاعدا % على سواد العين محمولا % % 2(@)(@@) % لفظك فينا مطرب كله % % لم يبق للسامع معقولا % % 3(@)(@@) % يرتد عن إدراكه مسلم % % ويصبح الفاضل مفضولا % % PageV01P2270 البحر : - 1(@)(@@) % كم أقاسي من الغرام وأخفي % % عن وشاتي صبابة وغليلا % % 2(@)(@@) % آه يا ويلتي وياليت أني % % كنت لم أتخذ فلا نا خليلا % % PageV01P2271 البحر : - 1(@)(@@) % لي سيد رقى إلى % % أفق المعالي فعلا % % 2(@)(@@) % أقسم لا ينسى الندى % % إلا إذا ما فعلا % % PageV01P2272 البحر : - 1(@)(@@) % شهدنا بأن إله السماء % % يحبك يا أكرم الناس حالا % % 2(@)(@@) % يقول نبي الهدى إنه % % تعالى جميل يحب الجمالا % % PageV01P2273 البحر : - 1(@)(@@) % لم أزل منذ غاب شخصك عني % % أرتجي وصل كتبه والوصالا % % 2(@)(@@) % أرقب الغرب حين أذكر مولا % % ي كأن الشهاب صار هلالا % % PageV01P2274 البحر : - 1(@)(@@) % سقى وواعدني وصلا ألذ به % % عند الرقاد ولا والله ما فعلا % % 2(@)(@@) % فياله الله من ساق مواعده % % كانت مواعيد عرقوب لها مثلا % % PageV01P2275 البحر : - 1(@)(@@) % لا وخمر بابليه % % في ثنايا لؤلؤيه % % 2(@)(@@) % لا رقى سفح دموعي % % في هوى تلك الثنيه % % 3(@)(@@) % ربع سلواني خراب % % وشجوني عامريه % % 4(@)(@@) % حربي من ذات حسن % % باسم تبكي البريه % % 5(@)(@@) % غادة يروي لماها % % عن صحاح جوهريه % % 6(@)(@@) % من بيوت الترك ترمي % % عن قسي عربيه % % 7(@)(@@) % رحلتني عن سلوي % % بلغات فارسيه % % 8(@)(@@) % لست أرضى يا عذولي % % في هواها بالتقيه % % 9(@)(@@) % و لقد أبذل روحي % % في معانيها السنيه % % 10(@)(@@) % لم أخف في عبلة السا % % ق وفاها العنبريه % % PageV01P2276 11(@)(@@) % لا ولا أخشى من الدن % % يا عواديها الجريه % % 12(@)(@@) % حجبتني يد إسما % % عيل عن كل بليه % % 13(@)(@@) % ملك أغنى عن السح % % ب بجدواه المليه % % 14(@)(@@) % حاتمي الكف يثني % % من أذى الدهر عديه % % 15(@)(@@) % معرق الآباء باهي الش % % خص وضاح السجيه 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms436# % % 16(@)(@@) % قد رعى الله ببقيا % % ملكه هذي الرعيه % % 17(@)(@@) % حبذا بحر بكفي % % ه الأماني والمنيه % % 18(@)(@@) % ذو حسام يكشف الخط % % ب برؤياه المضيه % % 19(@)(@@) % عادل يقسم في نا % % زلة قسم السويه % % 20(@)(@@) % شرف الاسياف حتى % % سميت بالمشرفيه % % PageV01P2277 21(@)(@@) % و يراع ناحل الجس % % م له نفس قويه % % 22(@)(@@) % ساهر في ظلم الخ % % ير لتأمين البريه % % 23(@)(@@) % جامع في الجود والع % % لم صفات كوكبيه % % 24(@)(@@) % هكذا تبنى المعالي % % بمزايا هندسيه % % 25(@)(@@) % يا مليكا خصه الل % % ه بأوصاف سنيه % % 26(@)(@@) % لك عندي صدقات % % وافادات خفيه % % 27(@)(@@) % تقتضي المدح وان كا % % نت عن المدح غنيه % % 28(@)(@@) % فابق مخدوم السجايا % % بتحايا عنبريه % % 29(@)(@@) % واصل الملك بأسبا % % ب السعود الأبديه % % PageV01P2278 البحر : - 1(@)(@@) % أوجهك أم جنة عاليه % % قطوف لرائيها دانيه % % 2(@)(@@) % ومبسمك العذب أم بارق % تحث سحائب أجفانيه % % 3(@)(@@) % بروحي مالكة للحشا % % دموعي من حلقها جاريه % % 4(@)(@@) % و والية كدرت بالجفا % % حياتي فيا ليتها القاضيه % % 5(@)(@@) % تعذبني وهي لي جنة % % و تجرحني واسمها آسيه % % 6(@)(@@) % معذبة القلب في حبها % % لتهنك عيشتك الراضيه % % 7(@)(@@) % لأرخص دمعي غداة السرا % % تأرج أنفاسك الغاليه % % 8(@)(@@) % فلله رائحة من شذاك % % حياتي من أجلها غاديه % % 9(@)(@@) % غنيت بحسنك عن واصف % % وما كل غانية غانيه % % 10(@)(@@) % و وافقني في طريق الردى % % حسام لواحظك العاديه % % PageV01P2279 11(@)(@@) % و شق السهاد سما مقلتي % % فيومئذ أضلعي واهيه % % 12(@)(@@) % وزادت جنوني ذات الدلال % % وليس المدامع بالراقيه % % 13(@)(@@) % و رب عذول على حبها % % عصيت ملامته الناهيه % % 14(@)(@@) % فقال وأحنق في غيظه % % أقوم فقلت إلى الهاويه % % 15(@)(@@) % أطيع وقد قال لي باطلا % وأين سلوي والواشيه % % 16(@)(@@) % فقدتك ناصية للوشاة % % فإنك كاذبة خاطيه % % 17(@)(@@) % أرى الحب يا صاحبي خلة % % تدل على رقة الحاليه % % 18(@)(@@) % فدع قلبي الصب يغشى الردى % % وتقتله الفئة الباغيه % % 19(@)(@@) % ذكرت الشباب وأقماره % % جوانح للمة الداجيه % % 20(@)(@@) % و روضا كأن سقاه المدام % % تباري سواقيه الجاريه % % PageV01P2280 21(@)(@@) % تولى الزمان بهذا وذا % % فلم يبق ساق ولا ساقيه % % 22(@)(@@) % و طوح بي الدهر في غربة % % صليت بنيرانها الحاميه % % 23(@)(@@) % كأني خارج خط استواء % % فما لي في ظلها زاويه % % 24(@)(@@) % طروسي ناشرة فضلها % % وبالجوع لي مهجة طاويه % % 25(@)(@@) % أضيع وقد ضاع من منطقي % % شذا ما بدا قبل في الباديه % % 26(@)(@@) % عسى كرم الأفضل المرتجى % % يوقع في قصتي الشاكيه % % 27(@)(@@) % مليك له سور في الثنا 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms437# % % تظل السراة لها جاثيه % % 28(@)(@@) % و بأس تبيت عيون الجرا % % ح لهيبته في الوغى داميه % % 29(@)(@@) % و إيضاح رأي بنحو العلى % % قضاياه شافية كافيه % % 30(@)(@@) % و عفو يقول لساري الذنو % % ب إلى جبل الحلم يا ساريه % % PageV01P2281 31(@)(@@) % و لفظ يقرط أسماعنا % % بما لا رأت مثله ماريه % % 32(@)(@@) % و جود ينقص جود الحيا % % موازين أنعمه الوافيه % % 33(@)(@@) % فخذ من قواعد أكياسه % % ودع لندى حاتم الماشيه % % 34(@)(@@) % له الله من سائر المكرما % % ت وأطواد سؤدده راسيه % % 35(@)(@@) % متيمة بالعلى نفسه % % وعين السهى تحتها ساهيه % % 36(@)(@@) % و حاكمة بين حساده % % وقصاده يده الساميه % % 37(@)(@@) % فهاتيك خائفة بأسها % % وهذي لأنعمها راجيه % % 38(@)(@@) % تظل على العسر أقلامه % % فتأخذه أخذة رابيه % % 39(@)(@@) % سمعنا محاسن قوم ولا % % كمثل محاسنه الباديه % % 40(@)(@@) % من القوم تمحى نجوم السما % % وآثار سؤددهم باقيه % % PageV01P2282 41(@)(@@) % رياض محامدهم غضة % % وسحب عوارفهم هاميه % % 42(@)(@@) % أأزكى الورى أسرة برة % % وأسعدهم همة عاليه % % 43(@)(@@) % إليك بعثت وفود الرجا % % ووجهت همتي القاصيه % % 44(@)(@@) % و أملت برك دون الورى % % زمان يدي عنهم نائيه % % 45(@)(@@) % دعاني سواك لعين النوال % % فقلت على عينك الراقيه % % 46(@)(@@) % و كان المؤيد ثم انقضى % % فأيد مطالبي العانيه % % 47(@)(@@) % و خذها عقيلة مدح على % % بني الشعر رتبتها عاليه % % 48(@)(@@) % بحق الركوب لمن قالها % % على عنق الضد بالغاشيه % % 49(@)(@@) % يتيمة فكر امرئ يرتجي % % كفالة أيامه الماضيه % % PageV01P2283 البحر : - 1(@)(@@) % بدا وقامته تختال بالتيه % % فأي شمس على رمح تحاكيه % % 2(@)(@@) % وقمت أذكره بالظبي ملتفتا % فقال لي طرفه من غير تشبيه % % 3(@)(@@) % أغن يبعد مشتاقا ويرشقه % % باللحظ فهو على الحالين يرميه % % 4(@)(@@) % ماللذي فتنت قلبي محاسنه % % أضحى يعذب روحي وهي تفديه % % 5(@)(@@) % و ما لعاذل قلبي في محبته % % تعبان يدخل فيما ليس يعنيه % % 6(@)(@@) % ألفاظه الريح لكن في الحشا لهب % % وربما كان مر الريح يذكيه % % 7(@)(@@) % و القلب قد أشكر الله الحبيب به % % فما الملام على حال بمخليه % % 8(@)(@@) % لا يختشي بيت قلبي غزو لا ئمه % % فإن للبيت ربا سوف يحميه % % 9(@)(@@) % يا ثاني العطف من تيه ومن غضب % % حتى كأني قلت الغصن ثانيه % % 10(@)(@@) % خفض قلاك وعللني بوعد لقا % % وخل عمري يقضى في تقاضيه % % PageV01P2284 11(@)(@@) % و ابعث خيالا تراني منه في جدل % % فالروح تثبته والجسم ينفيه % % 12(@)(@@) % هيهات طال سهادي في هواك 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms438# فلا % % طيف أراه ولا سقم أواريه % % 13(@)(@@) % أحيي الليالي تسهادا فيالفتى % % يميته الليل حزنا وهو يحييه % % 14(@)(@@) % لو كان لليل سلطان كما زعموا % % لكان ينصف جفني من تشكيه % % 15(@)(@@) % سقيا لوصلك والأيام عاطفة % % ترد دمع المعنى من مآقيه % % 16(@)(@@) % و صل تكنف روحي بعد ما جهدت % % كما تكنف دين الله محييه % % 17(@)(@@) % حامي حمى الملك بالأقلام مشرعة % % على المنى والمنايا حول واديه % % 18(@)(@@) % لو ألقيت كعصا موسى على حجر % % تفجر الماء من أقصى نواحيه % % 19(@)(@@) % جاءت بيحيى معاليه مبشرة % % فصدقت يده بشرى معاليه % % 20(@)(@@) % يد بأصل نداها فرع كل ندى % % كالبحر ناقلة عنه سواقيه % % PageV01P2285 21(@)(@@) % سارت وراء خباها السحب وادعة % % لا تأخذ الماء الا من مجاريه % % 22(@)(@@) % يا محسن الظن هذا نحو أنعمه % % بمفرد الفضل قد نادى مناديه % % 23(@)(@@) % يمم مغانيه بالقصد محتكما % إن الغنى اشتق فينا من مغانيه % % 24(@)(@@) % ذاك الذي يستمد النيل أنعمه % % فما الأصابع الا من أياديه % % 25(@)(@@) % حوت كنانة سهما من براعته % % لا تعرف اليمن الا حين تحويه % % 26(@)(@@) % بكف زاكي السجايا ان برى قلما % يكاد ينطق تمجيدا لباريه % % 27(@)(@@) % ذو السؤدد المحض لا طود يجاذبه % % ثوب الوقار ولا نجم يساميه % % 28(@)(@@) % ماضي شبا العزم كم حال به علقت % % تعلق الحال من فعل بماضيه % % 29(@)(@@) % في بيت فضل على الجوزاء مرتفع % % تعنو القصائد عن أدنى مبانيه % % 30(@)(@@) % لم ندر ما فيه من وصف فنحصره % % وصاحب البيت أدرى بالذي فيه % % PageV01P2286 31(@)(@@) % بيت ليحيى من الفاروق متصل % % بخ لماضيه من بيت وباقيه % % 32(@)(@@) % قل للذي نهضت للمجد همته % % ضاهي السماك ويحيى لا يضاهيه % % 33(@)(@@) % ان السيادة قد نضت سوالفها % % لواحد العصر يصبيها وتصبيه % % 34(@)(@@) % مقسم الدين والدنيا على شيم % % قد أتعبت في المعالي من يجاريه % % 35(@)(@@) % أيامه للعلى والمجد قائمة % % وللعفاف وللتقوى لياليه % % 36(@)(@@) % ما زال يعمل آراء وأدعية % % حتى استوى الملك في أعلى صياصيه % % 37(@)(@@) % و استوثق العدل في الدنيا فليس بها % % جان سوى راتع في الروض يجنيه % % 38(@)(@@) % يا من له الفضل باديه وحاضره % % ومن له القصد دانيه وقاصيه % % 39(@)(@@) % دين الرجا قد تناهت لي مطالبه % % على الزمان ولكن أنت قاضيه % % 40(@)(@@) % أدعوك دعوة شاكي الحال معتقد % % أن ليس غيرك بعد الله يشكيه % % PageV01P2287 41(@)(@@) % ان 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms439# لم تراع برأي منك مقصده % % يا ابن السراة فقل لي من تراعيه % % 42(@)(@@) % في نظرة منك تأميلي ومفترجي % % ولفظة منك تنويلي وتنو يهي % % 43(@)(@@) % أقول والدمع قد سارت ركائبه % % إلى حماك وقد طافت أمانيه % % 44(@)(@@) % هذا نباتي لفظ يشتكي عطشا % لعل أفقك بالأنواء يسقيه % % 45(@)(@@) % نعم وهذا مقال داثر فعسى % % يا من له قلم الانشاء تنشيه % % PageV01P2288 البحر : - 1(@)(@@) % تبسم عن حلو الرضاب شهيه % % روينا صحيح الحسن عن جوهريه % % 2(@)(@@) % وأقبل وضاح السنا متبسما % فأفصح عن قمريه قمريه % % 3(@)(@@) % وغنى وقد مالت به نشوة الصبا % % نديمي ماس الغصن في سندسيه % % 4(@)(@@) % فلم أر أحلى منه غصنا ترنمت % % على ورق الديباج ورق حليه % % 5(@)(@@) % و بدرا له في العرب والترك نسبة % % دعتني إلى داني الهوى وقصيه % % 6(@)(@@) % يهز علي الرمح من علوية % % قواما ويرمي السهم من فحقيه % % 7(@)(@@) % و يسكر عقلي خده بمدامة % % سقاها لغيثى من إنا عسجديه % % 8(@)(@@) % فيالك من دينار خد قد انتمى % % يحاكيه من حسنى إلى يوسفيه % % 9(@)(@@) % تطلبت بالإخلاص في الحب عدنه % % وتبت يد العذال في لهبيه % % 10(@)(@@) % و اني لتصفو لي المدامة باسمه % % ولائم سمعي فيه مثل صفيه % % PageV01P2289 11(@)(@@) % و صبرني الواشي فيا لمصبر % % قتيل بمسنون اللحاظ مشيه % % 12(@)(@@) % و كيف يلذ الصبر عن ثغر باسم % % جرى الريق بالذكرى على سكريه % % 13(@)(@@) % نأى ولمن لم يألف العشق غادر % % فما عذر عذري الغرام وفيه % % 14(@)(@@) % و إن فاتني ماء الحياة بثغره % % فكم نصب لاقيت من دون ريه % % 15(@)(@@) % و رب مدام بيننا قد أدارها % % بنان مدامي اللماء عليه % % 16(@)(@@) % غزاني بخديه بياض وحمرة % % فويلاه من قيسيه يمنيه % % 17(@)(@@) % و آها على سر الصبا بظلامه % % فلا كان شيب فاضح بنقيه % % 18(@)(@@) % و لا قيدت عن مصر قافية الحيا % % ولا عطلت أبياتها من رويه % % 19(@)(@@) % هويت من الآثار آثار عمرها % % ومن بيت فضل الله فضل عليه % % 20(@)(@@) % وزير ملوك شد بالرأي إزرهم % % وحاتم دهر كف بأس عديه % % PageV01P2290 21(@)(@@) % و صاحب تدبيرين عن فاضليه % % تحدثت العليا وعن أفضليه % % 22(@)(@@) % بكف روت أقلامه عن تميرها % % وزند روت آراؤه عن وريه % % 23(@)(@@) % و ذو النسب المرفوع عن محبوبه % % إلى عمري المنتمى عدويه % % 24(@)(@@) % و ذو القلم الخطي إما بدرجه % % وإما بما 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms440# يختال من سمهريه % % 25(@)(@@) % يراع بتأثير الحروف حمى الحمى % % فكان ابتداء النصر من إلفيه % % 26(@)(@@) % سطافي الوغى حدا وأينع في الندى % % فلله جاني فرعه وجنيه % % 27(@)(@@) % و صاغ بديعا حفه بمكارم % % فلم تخل في الحالين من ذهبيه % % 28(@)(@@) % براحة من أولى الورى كل راحة % % بما سار من سر العطا وجليه % % 29(@)(@@) % و يمنى لها في الحظ والجود والتقى % % ملأت صفات لم تحد عن وليه % % 30(@)(@@) % اذ استخدمت مداحها استخدموا لها % % بديع الثنا من محضه عربيه % % PageV01P2291 31(@)(@@) % ترقى ابن فضل الله في الفضل غاية % % قضت ذل شانيه وعز صفيه % % 32(@)(@@) % فيا فوز قوم آمنوا تحت رقه % % ويا ويح من لا آمنوا برقيه % % 33(@)(@@) % هو البحر في تياره وحيائه % % أو السيل في إروائه وأتيه % % 34(@)(@@) % اذا قيل من أسمى جلالا ونسبة % % حلفنا لوصفيه على عمريه % % 35(@)(@@) % اذا سار سار النصر تلو يراعه % % وان حل حل الفضل صدر دنيه % % 36(@)(@@) % اذا حف في نادي السعود بقومه % % فما البدر في بيت السما بكفيه % % 37(@)(@@) % علوتم به يا آل يحيى بشامخ % % إلى أن نظرتم للسها من عليه % % 38(@)(@@) % فإن شئتم ورد الغمام بأفقكم % % أطلتم خيال المستقي لركيه % % 39(@)(@@) % أخا العلم والعلياء علمت منطقي % % غرائب من ساري الكلام سريه % % 40(@)(@@) % بإنشائك المهدي إلى العقل نشوة % % وإن كان من طهر المقال زكيه % % PageV01P2292 41(@)(@@) % و شعر بكرنا قبله متنبئا % وكدنا نقول الآن شعر نبيه % % 42(@)(@@) % بمعجز نظم الدر غير منقب % % وإخراج ما أعيى الورى من جنيه % % 43(@)(@@) % نشرت قريضي بعد ما قد طويته % % وأغديته بعد امتناع طويه % % 44(@)(@@) % و قد كان عافي البيت أنشد رسمه % % هو الربع جارته دموع وليه % % 45(@)(@@) % إلى أن أعاد العطف لي منك عاتيا % بشعري طلاعا على معنويه % % 46(@)(@@) % يفوح على رغم العدى عنبريه % % ويخبر آراء الرضا عن بريه % % 47(@)(@@) % أتى لك ما محض العلى وسميها % % فخذ من حداقي الثنا عيشميه % % 48(@)(@@) % و عش يا ابن يحيى ذا حياة سعيدة % % وعيش هني المستطاب مريه % % 49(@)(@@) % تقابلك الأعوام ذا في قدومه % % بسعد وذا بالحمد عنه مضيه % % 50(@)(@@) % كأن هلال العام زورق قادم % % عليك بمملوك الثناء مليه % % PageV01P2293 51(@)(@@) % فهنئته ألفا وألفا ومثلها % % إلى أن يتيه العقل في عدديه % % 52(@)(@@) % لكل امرئ والاك حظ سعيده 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms441# % % وكل امريء عاداك حظ شقيه % % PageV01P2294 البحر : - 1(@)(@@) % جاءت العاذلات شيئا فريا % % وظمئنا إلى لقاك فريا % % 2(@)(@@) % يا قريبا من المحب بعيدا % وعذابا إلى المحب شهيا % % 3(@)(@@) % وغزالا لناظريه فتور % تركا القلب كالزناد وريا % % 4(@)(@@) % غلب الصبر في هوى ناظريه % % وضعيفان يغلبان قويا % 5(@)(@@) % و على وجنتيه نار أراني % % إن تسليت عن هواها شقيا % % 6(@)(@@) % يا خليلي عندها خلياني % % أنا أولى بوجنتيه صليا % % 7(@)(@@) % أنا أدري بأن لي من سناها % % في الجبين طالعا قمريا % % 8(@)(@@) % لا أرى حين حل عقرب صدغ % % سفر القلب في هواها رديا % % 9(@)(@@) % بأبي غصن معطفيه على القر % % ب وفي البعد جانيا وجنيا % % 10(@)(@@) % و يتيم من لؤلؤ الثغر حلو % راح في مثله الرشيد غويا % % PageV01P2295 11(@)(@@) % ذو ابتسام بالسهد أرمد عيني % % مع أني اكتحلته لؤلؤيا % % 12(@)(@@) % تارة في بضائع الحسن يأتي % % جوهريا وتارة سكريا % % 13(@)(@@) % فتنة الحسن فوق خديه لا تب % % رح قيسي رأيه يمنيا % % 14(@)(@@) % أنظم الشعر وهو يبسم عجبا % ولهذا أتى به جوهريا % % 15(@)(@@) % عامريا من التغزل فيه % % ومن المدح بعده قرشيا % % 16(@)(@@) % حبذا من قريش في الشام فرع % % أبطحي أكرم به بهنسيا % % 17(@)(@@) % شمس عليا عمت منافعها الخ % % لق قريبا من الورى وقصيا % % 18(@)(@@) % و كريم زاكي الأصول هززنا % % منه للمكرمات فرعا زكيا % % 19(@)(@@) % فإذا ما دعى رسول رجاء % % فضل أبوابه دعى خزرجيا % % 20(@)(@@) % و اذا ما سقى نداه نباتي % % طاب مدحي في الحالتين رويا % % PageV01P2296 21(@)(@@) % كم سبرنا له تقى ونوالا % فوجدنا في الحالتين وليا % % 22(@)(@@) % كم ثناء وإلى لعلياه مدحا % حسنا في الورى وقدرا عليا % % 23(@)(@@) % و معان يحيى لها فلقد أو % % تي حكم الفخار فيها صبيا % % 24(@)(@@) % تاليا في العلى وزيرا شهدنا % % ه لآمالنا وفيا حفيا % % 25(@)(@@) % قال إحسانه تهنوا نوالا % وزكاة منه وكان تقيا % % 26(@)(@@) % حبذا تلو ذاك شمسا تلونا % % مدح أيامه جليلا جليا % % 27(@)(@@) % خطبته مناصب الدين والدن % % يا كما قد نرى فكان الكفيا % % 28(@)(@@) % عن تفاريق يمنه فاسأل الجا % % نع تسئل لسان صدق عليا % % 29(@)(@@) % ياله في الورى فتى قريشا % عم بالخير جامعا أمويا % 30(@)(@@) % و رئيسا نجا ذوو القصد لما % % قربت منها الملوك نجيا % % PageV01P2297 31(@)(@@) % و رأوا عزمه لدين ودنيا % % شافيا كافيا غنيا مليا % % 32(@)(@@) % سائرات أقلامه يوم حفظ % % وعطاء 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms442# على الصراط سويا % % 33(@)(@@) % فترى الحق كالصباح رواء % وترى الخير كالغمام رويا % % 34(@)(@@) % و ترى اليراع يجري بجود % % وبيان جواده العربيا % % 35(@)(@@) % صان وجهي عن الورى بأياد % % وأياد غيرن حالي الرزيا % % 36(@)(@@) % فأنا اليوم والزمان بخير % % ها كأن السعيد كان شقيا % % 37(@)(@@) % جنة من دمشق نرتع فيها % % ولنا الرزق بكرة وعشيا % % 38(@)(@@) % يا كريما يخفي أياديه لو كا % % ن شذا المسك والصباح خفيا % % 39(@)(@@) % أصلح الباطن افتقادك والظا % % هر اذ كنت جائعا وعريا % % 40(@)(@@) % فابق ما شئت كيف شئت مرجى % % مستفاض النعمى سنيا سريا % % PageV01P2298 41(@)(@@) % يلتقيك الثنا ويزداد طيبا % مثلما يلتقي الرياض الوليا % % PageV01P2299 البحر : - 1(@)(@@) % نبه الملك عزمك العمر يا % % لمهماته ونام هنيا % % 2(@)(@@) % ودعا وجهك السعيد فما كا % % ن حمى مصر بالدعاء شقيا % % 3(@)(@@) % أنت بين السادات كالذهب الخا % % لص لا غرو أن يرى مصر يا % % 4(@)(@@) % أنت أولى مدبر ومشير % % قربته الملوك منها نجيا % % 5(@)(@@) % أنت ترعى الأمور والله يرعا % % ك فلا زلت راعيا مرعيا % % 6(@)(@@) % حبذا منك للسيادة كفؤ % وافر الفضل والثناء وفيا % % 7(@)(@@) % عرف الملك منه أصلاعريقا % بين أوطانه وفرعا عليا % % 8(@)(@@) % وحوى من علاه كوكب رأي % % طالع السعد بكرة وعشيا % % 9(@)(@@) % ناظرا ساهرا على الملك يدري % % كيف يهدي له المرام الخفيا % % 10(@)(@@) % ان أردنا التقى لديه أو الجو % % د وجدنا في الحالتين وليا % % PageV01P2300 11(@)(@@) % باهر المطلعين رأيا ومرأى % % حبذا الفضل لامعا ألمعيا % % 12(@)(@@) % حاملا في مواطن السلم والحر % % ب يراعا يردي الزمان الرديا % % 13(@)(@@) % قلما جائلا اذا خط حرفا % حمد الناس رمحه الخطيا % % 14(@)(@@) % يانع الغصن كلما هزه أس % % قط مال البلاد منه جنيا % % 15(@)(@@) % يا رئيسا دعا الزمان له الوف % % د وقال الرجاء حثوا المطيا % % 16(@)(@@) % دام للقاصدين شخصك غوثا % وغماما للواردين رويا % % 17(@)(@@) % قال إحسانه تهنوا نوالا % وزكاة منه وكان تقيا % % PageV01P2301 البحر : - 1(@)(@@) % ما للندى لا يلبي صوت داعيه % % أظن أن ابن شاد قام ناعيه % % 2(@)(@@) % ما للرجاء قد اشتدت مذاهبه % % ما للزمان قد اسودت نواحيه % % 3(@)(@@) % مالي أرى الملك قد فضت مواقفه % % مالي أرى الوفد قد فاضت مآقيه % % 4(@)(@@) % نعى المؤيد ناعيه فيا أسفي % % للغيث كيف غدت عنا غواديه % % 5(@)(@@) % واروعتا لصباح عند رؤيته % % أظن أن صباح الحشر ثانيه % % 6(@)(@@) % واحسرتاه لنظمي في مدائحه 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms443# % % كيف استحال لنظمي في مراثيه % % 7(@)(@@) % أبكيه بالدر من جفني ومن كلمي % % والبحر أحسن ما بالدر أبكيه % % 8(@)(@@) % أروي بدمعي ثرى ملك له شيم % % قد كان يذكرها الصادي فترويه % % 9(@)(@@) % أذيل ماء جفوني بعده أسفا % لماء وجهي الذي قد كان يحميه % % 10(@)(@@) % جاد من الدمع لا ينفك يطلقه % % من كان يطلق بالإنعام جاديه % % PageV01P2302 11(@)(@@) % و مهجة كلما فاهت بلوعتها % % قالت رزية مولاها لها إيه % % 12(@)(@@) % ليت المؤيد لا زالت عوارفه % % فزاد قلب المعنى في تلظيه % % 13(@)(@@) % ليت الحمام حبا الأيام موهبة % % فكان يفني بني الدنيا ويبقيه % % 14(@)(@@) % ليت الاصاغر تفدى الأكبرون بها % % فكانت الشهب في الآفاق تفديه % % 15(@)(@@) % أعزز علي بأن ألقى عوارفه % % ملىء الزمان واني لا ألاقيه % % 16(@)(@@) % أعزز علي بأن تبلى شمائله % % تحت التراب وماتبلى أياديه % % 17(@)(@@) % أعزز علي بأن ترعى النجوم على % % سرح من الملك قد خلاه راعيه % % 18(@)(@@) % هلا بغير عماد الدين حادثة % % ألقت رداه وأوهت من مبانيه % % 19(@)(@@) % هلا ثنى الدهر غربا عن محاسنه % % فكان كوكب سعد في لياليه % % 20(@)(@@) % ترى درى الدهر مقدار الذي فقدت % % من فيض أدمعه أحوال أهليه % % PageV01P2303 21(@)(@@) % ترى درى الدهر ما معزى سماحته % % فجاء مهجته في زي عافيه % % 22(@)(@@) % لا أعتب الزمن المودي بسيده % % يكفيه ما قد تولى عنه يكفيه % % 23(@)(@@) % لهفي وهل نافعي لهفي على ملك % % بات الغمام على الآفاق يبكيه % % 24(@)(@@) % لهفي وهل نافعي لهفي على ملك % % كسى الزمان حدادا من دياجيه % % 25(@)(@@) % لهفي على الملك قد أهوت سناجقه % % إلى التراب وقد حطت غواشيه % % 26(@)(@@) % لهفي على الخيل قد وفت صواهلها % % حق العزا فهو يشجيها وتشجيه % % 27(@)(@@) % لهفي على ذلك السلطان حين قضى % % من الحمام عليه حكم قاضيه % % 28(@)(@@) % لهفي عليه لممتار ومطلب % % بالمال يقريه أو بالعلم يقريه % % 29(@)(@@) % لهفي عليه لجود كان يعجبه % % فيه الملام كأن اللوم يغريه % % 30(@)(@@) % ما خلف ابن علي من ذخائره % % إلا ثنا أضحت الدنيا تواليه % % PageV01P2304 31(@)(@@) % لهفي عليه لحلم كان يبسطه % % على العفاة ومدح كان يجنيه % % 32(@)(@@) % كان المديح له عرسا بدولته % % فأحسن الله للشعر العزا فيه % % 33(@)(@@) % كان الفقير اذا أمر الزمان بغى % % عليه قام إلى السلطان ينهيه % % 34(@)(@@) % كان المؤيد في يومي ندى وردى % % غيثا لراجيه أو غوثا 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms444# للاجيه % % 35(@)(@@) % تروى صحاح القضايا عن براعته % % والنصر في الحرب يروي عن عواليه % % 36(@)(@@) % من للعلوم وللأعلام ينشرها % % و للوغى ورداء الخوف يطويه % % 37(@)(@@) % من للكسير من الأهوال يجبره % % وللطريد من الأيام يؤويه % % 38(@)(@@) % من للتصانيف أمثال الكواكب في % % ليل المداد لساري الفكر يهديه % % 39(@)(@@) % مضى وقد كان عضبا للزمان فيا % % لهفي على مغمد في الترب ماضيه % % 40(@)(@@) % لو أمكن الصبر عنه ما أنست به % % فكيف والحزن من أحشاي ينعيه % % PageV01P2305 41(@)(@@) % آها لأحمر دمع بعد أشهبه % % أجراه حتى لقد أفناه مجريه % % 42(@)(@@) % أفنى المؤيد تبر الدمع من بصري % % وتلك عادته في التبر يفنيه % % 43(@)(@@) % كيف السلو وحولي من صنائعه % % ما يمنع الصخر من أدنى تسليه % % 44(@)(@@) % هذي حماة أغص الهم واديها % % وطاوع الخزن فيه دمع عاصيه % % 45(@)(@@) % كأنه استشعر الأحزان من قدم % % فللنواعير نوح في نواحيه % % 46(@)(@@) % هذي المنازل والدنيا معطلة % % كأنها اللفظ خال من معانيه % % 47(@)(@@) % جاد الحيا قبره الزاكي فلا برحت % % سحائب العفو والرضوان تسقيه % % 48(@)(@@) % نعم السحائب تسقي صوب وابلها % % نعم الضريح ونعم المرء ثاويه % % 49(@)(@@) % مهنأ بجنان الخلد دانية % % ونحن نصلى بنار من تنائيه % % 50(@)(@@) % من كان يتعب في المعروف راحته % % فهو المهنى بترحيب وترفيه % % PageV01P2306 51(@)(@@) % يا آل أيوب صبرا إن إرثكمو % % من اسم أيوب صبر كان ينجيه % % 52(@)(@@) % هي المنايا على الأقوام دائرة % % كل سيأتيه منها دور ساقيه % % 53(@)(@@) % هي المقادير هذا الأصل تنزعه % % بعد النمو وهذا الفرع تنميه % % 54(@)(@@) % كأنني بسليل المكرمات وقد % % سعى بحق تراث الملك ساعيه % % 55(@)(@@) % محمد وهو اسم عنه مشتهر % % ولي بعه بيت اسماعيل ينشيه % % 56(@)(@@) % يا ناصر الدين أنت الملك قد قرأت % % علائم الملك فيه عين رائيه % % 57(@)(@@) % و من أبيك تعلمت الثبات فما % % تحتاج تذكر أمرا أنت تدريه % % 58(@)(@@) % لا تخش بيتك أن يلوي الزمان به % % فإن للبيت ربا سوف يحميه % % PageV01P2307 البحر : - 1(@)(@@) % لو أن شكوى الاسى يا عز يغنيه % % لكان بث لسان الدمع يكفيه % % 2(@)(@@) % فيا له دمع عين كل غادية % % لا تأخذ الماء الا من مجاريه % % 3(@)(@@) % كأن جود علاء الدين صارله % % رواية فهو يرويها وترويه % % 4(@)(@@) % ذو اللفظ والفضل لوقال البحار طمت % % قالت فواضله من غير تشبيه % % 5(@)(@@) % يامن أطارح منك البيت أسكنه % % آوى 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms445# اليه كما أرضى وأنشيه % % 6(@)(@@) % ما أحسن البيت من نعماك اسكنه % % بكل بيت من الأمداح يعنيه % % 7(@)(@@) % مثلي ومثلك يدري فضل ذاك وذا % % وصاحب البيت أدرى بالذي فيه % % PageV01P2308 البحر : - 1(@)(@@) % إمام المسلمين تعز عمن % % فقدت وعش تفدى بالبرايا % % 2(@)(@@) % ودم لمدائح وصفا أجور % % لك المرباع منها والصفايا % % 3(@)(@@) % فقيدتك التي صغرت كبير % % قضاء عزائها بين القضايا % % 4(@)(@@) % فيا لك طفلة من بيت علم % % عليها قد تطفلت المنايا % % 5(@)(@@) % ويا لك زهرة من دوح قوم % % سرت بجدودها مسرى البجايا % % 6(@)(@@) % لقد وضع الاسى دمعا عليها % % وقد طلعت شجون من ثنايا % % 7(@)(@@) % ولم أعرف لها إسما ولكن % % أقول الآن فاطمة الرزايا % % PageV01P2309 البحر : - 1(@)(@@) % أعربت يا مقلتي الغافيه % % عن زورة كافية شافيه % % 2(@)(@@) % طيف كرى ما زال الا غدت % % ما الشرط للأحزان ما النافيه % % 3(@)(@@) % كما نفت وافر خوف الورى % % عافية من سيدي وافيه % % 4(@)(@@) % قاضي قضاة الدين ماشهبه % % خائفة القطع ولاخافيه % % 5(@)(@@) % ذو العلم والجدوى التي شردت % % محلا وجهلا عينها الصافيه % % 6(@)(@@) % ماذا جرى للخلق خوفا على % % مهجته من أدمع طافيه % % 7(@)(@@) % فالآن أحزان الورى قد عفت % % فالحمد لله على العافيه % % PageV01P2310 البحر : - 1(@)(@@) % لست أنسى ابتسامها اللؤلؤيا % % ملبسا خدي الدموع حليا % % 2(@)(@@) % وخدودا حمرية اللون أشكو % % من جفاها وناظرا مستحيا % % 3(@)(@@) % لشذور الأغزال مع مدح تاج الد % % ين اصبحت صائغا جوهريا % % 4(@)(@@) % ان عبد الوهاب قاضي قضاة الد % % ين أوفى الورى ندى أو نديا % % 5(@)(@@) % هبة للعلى من الله ما زا % % ل لدى النسك والعطاء وليا % % 6(@)(@@) % يا إماما تهوي الغمائم من خل % % ف عطاياه سجدا وبكيا % % 7(@)(@@) % ما فقدنا أما خلفت جنايا % % كان للمعتفي وكان تقيا % % PageV01P2311 البحر : - 1(@)(@@) % يا إماما له مقام سني % % وثناء في الخافقين وفي % 2(@)(@@) % مااسم شيء فيه لقوم طعام % % ولكل الورى بخمسيه ري % 3(@)(@@) % وهو مستضعف العيان ولكن % % فيه للسامعين بأس قوي % 4(@)(@@) % لا تقل لي في اللغز بالفتح ريب % % فهو لغز اذا نظرت جلي % 5(@)(@@) % سائل الذكر إن عكست وإن أس % % قطت حرفا كذاك منه السري % PageV01P2312 البحر : - 1(@)(@@) % يا محسنا إن أساء الزمان وإن % % مزق حال الفتى فمر فيه % % 2(@)(@@) % ينشد من ودك الجميل ومن % % مدحك في صدره وفي فيه % % 3(@)(@@) % أرضى لمن غاب عنك غيبته % % فذاك 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms446# ذنب عقابه فيه % % PageV01P2313 البحر : - 1(@)(@@) % تعشقته غصنا ناضرا % يميل به السكر من ناظريه % % 2(@)(@@) % تحجب دون القنا شخصه % % فصفرة لوني شوقا اليه % % 3(@)(@@) % وكم ذا أدور على خصره % % وما وقعت لي عين عليه % % PageV01P2314 البحر : - 1(@)(@@) % سألت قلبي عن ذوي العشق وعن % % ما أوتيته من فنون الحسن مي % % 2(@)(@@) % فقال لي إني وجدت امرأة % % تملكهم وأوتيت من كل شيء % % PageV01P2315 البحر : - 1(@)(@@) % لقد أدى تلاوته ووفى % % قرآءته لمولانا بني % 2(@)(@@) % صغير وهو إذ يتلو كبير % فما أدري بني أو أبي % % PageV01P2316 البحر : - 1(@)(@@) % عجبت لها مدحة ضاع لي % % شذاها وإن لم يكن في وفي % % 2(@)(@@) % فضاعت ولكن على أوجه % ثلاث لدي ومني وفي % PageV01P2317 البحر : - 1(@)(@@) % ومخدومة أتبعت مدح نبيها % % بخادمة أتبعتها بوليها % % 2(@)(@@) % لعلك يا جاه الشفيع محمد % % توصي بها عطف الوصي عليها % % PageV01P2318 البحر : - 1(@)(@@) % يا وزيرا شمل الآفا % % ق بالنعمى الحفيه % % 2(@)(@@) % قال تنويرك في الجا % % مع للشهب حكيه % % 3(@)(@@) % أنجمي عندك سعد % % وقناديل مضيه % % 4(@)(@@) % كيف لا وهي بنور الل % % ه أعمال زكيه % % PageV01P2319 البحر : - 1(@)(@@) % ليس يخشى من نجم سعد سقوطا % % من رأى قاضي القضاة عليا % % 2(@)(@@) % سار قاضي القضاة للشام غيثا % فله الله ساريا وسريا % % 3(@)(@@) % ان وجدنا وسمي جدواه في الدن % % يا وجدنا في الدين منه وليا % % 4(@)(@@) % قال إحسانه تهنوا نوالا % % وزكاة منه وكان تقيا % % PageV01P2320 البحر : - 1(@)(@@) % يا مليكا يجبر قصاده % % جبرا له الله مكاف عليه % % 2(@)(@@) % شكرا لها في الجود مخفية % % يبسط ضيف الباب فيها يديه % % 3(@)(@@) % اذا أتته وهو في صحبه % % صار مضافا ومضافا اليه % % PageV01P2321 البحر : - 1(@)(@@) % الحمد لله كل وقت % % بقرب مخدومنا هني % 2(@)(@@) % هنى دمشق وساكنيها ال % % غيث والقادم الوفي % 3(@)(@@) % ان جاء وسميها سريعا % فالآن قد جاءها الولي % PageV01P2322 البحر : - 1(@)(@@) % بأيمن طالع مسرى وزير % % نوال يداه للآمال محيا % % 2(@)(@@) % فيا ليت البرامك عاينوه % % وأنعمه تعم الخلق سقيا % % 3(@)(@@) % فينضب جعفر ويعوز فضل % % ويبلى خالد ويموت يحيى % % PageV01P2323 البحر : - 1(@)(@@) % يا سراة الأنصار تاج بينكم % % لرؤوس الأنساب أدنى حلي % 2(@)(@@) % ما أراه إلا أبا نصر وقت % % مالك ناصر لقصدي الجلي % 3(@)(@@) % فهو قاض ومالك وأبو نص % % ر وعبد الوهاب وابن علي % PageV01P2324 البحر : - 1(@)(@@) % تقول المعالي لابن يحيى عليها % % ومن كعلي في معاليه أو يحيى % % 2(@)(@@) % اذا كنت يوما 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms447# ذا نبات غرسته % % فعاهد ولا تهمل نباتك بالسقيا % % 3(@)(@@) % يفح لك ريحان الثنا من نسيمه % % ويدعو فيرضي زهرة الدين والدنيا % % PageV01P2325 البحر : - 1(@)(@@) % ومليح اذا نظرت اليه % % قلت أملك له الملاح رعايا % % 2(@)(@@) % فيه للناظر حسن وملح % % فهو يشوي به كبود البرايا % % PageV01P2326 البحر : - 1(@)(@@) % لامني الفتح اذ عزمت على النأ % % ي فقالت ضروراتي إيه % % 2(@)(@@) % أنت دفنت النوى كما زعموا % % فها أنا عن دمشق أنويه % % PageV01P2327 البحر : - 1(@)(@@) % يا علوي الذكر كم نعمة % % الي من بابك مهديه % % 2(@)(@@) % ان لم يكن في الدست حظي فلي % % من جودك الراتب زبديه % % PageV01P2328 البحر : - 1(@)(@@) % فديت فتى يده بالحيا % % وجبهته من حياء نديه % % 2(@)(@@) % يسافر قصدي إلى بابه % % فمنه المكان ومنه الهديه % % PageV01P2329 البحر : - 1(@)(@@) % شكوت صديقا ونافقته % % بشكوى فيا خسر عمري لديه % % 2(@)(@@) % نهاري الجميع دعاء له % % وليلي الجميع دعاء عليه % % PageV01P2330 البحر : - 1(@)(@@) % بأبي فاتر اللواحظ ألمى % % جاء فيه العذول شيبئا فريا % % 2(@)(@@) % غلب الصبر في هوى ناظريه % % وضعيفان يغلبان قويا % % PageV01P2331 البحر : - 1(@)(@@) % يا وزير العليا دعاء محب % % راح للعشر إذ أشرت إليه % % 2(@)(@@) % ما يبالي إذا بكى من هوان % % وافتقار اذا ضحكت عليه % % PageV01P2332 البحر : - 1(@)(@@) % و مبادلين بدمعتين حلاهما % % هذا لهذا قائم بولائه % % 2(@)(@@) % كالبحر تمطره السحاب ومالها % % من عليه لأنها من مائه % % PageV01P2333 البحر : - 1(@)(@@) % شكرا لها يا سيدي من نعمة % % بلغت من التأميل فوق المنتهى % % 2(@)(@@) % لا زال مدحك كل شهر روضة % % يعزى لمصر وكل شيء منتهى % % PageV01P2334 البحر : - 1(@)(@@) % و صلت المدام وذات اللمى % % زمان الصبا والليالي الشهيه % % 2(@)(@@) % فيالك من طيب عيش قطع % % ت بشرب العجوز ورشف الصبيه % % PageV01P2335 البحر : - 1(@)(@@) % أتيت لمصر في كتاب شفاعة % % إلى ولد من والد مورث العليا % % 2(@)(@@) % فيالكتاب جاء من عند ميت % % لحي فقال السعد لبيك يايحيى % % PageV01P2336 البحر : - 1(@)(@@) % و بمهجتي رشأ يميس قوامه % % فكأنه نشوان من شفتيه % % 2(@)(@@) % شغف العذار بخده ورآه قد % % نعست نواظره فدب عليه % PageV01P2337 البحر : - 1(@)(@@) % رأيت فتى من باب دارك طالعا % فأذكرني بيتا قديما شجانيا % % 2(@)(@@) % خليلي لا والله لا نترك البكا % % اذا علم من أرض نجد بدا ليا % % PageV01P2338 البحر : - 1(@)(@@) % بهت العذول وقد رأى ألحاظها % % تركية تدع الحليم سفيها % % 2(@)(@@) % فثنى الملام وقال دونك والاسى % % هذي مضايق لست أدخل فيها % % PageV01P2339 البحر 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms448# : - 1(@)(@@) % كم قائل اذ رآني % % أسعى لأندى البريه % % 2(@)(@@) % عطية منه تبغي % % فقلت ألف عطيه % % PageV01P2340 البحر : - 1(@)(@@) % فوض الجامع السعيد لمن يد % % عى له فيه بالضحى والعشي % 2(@)(@@) % لا عجيب ان خصه دون قوم % % أموي يعزى إلى قرشي % % PageV01P2341 البحر : - 1(@)(@@) % للعبد عندكم رسوم مكارم % % إن أقصيت فنداكم يدنيها % % 2(@)(@@) % و كفاكم أن الغيوث اذا همت % % تمحو الرسوم وغيثكم ينسيها % % PageV01P2342 البحر : - 1(@)(@@) % مرآتك العقل كل وقت % % تريك من نفسك الخفايا % % 2(@)(@@) % فلا تحكم هواك فيها % % إن الهوى يصدئ المرايا % % PageV01P2343 البحر : - 1(@)(@@) % يا سيدي عطفا على عصبة % % أفكارهم للقمح محميه % % 2(@)(@@) % قد طبخت بالشوق أكبادهم % % فيا لها طبخة قمحيه % % PageV01P2344 البحر : - 1(@)(@@) % يا رسولي لصبي % % مائس مثل صبيه % % 2(@)(@@) % خذ متى شئت ثيابي % % وارم في حلقي عشيه % % PageV01P2345 البحر : - 1(@)(@@) % علوت اسما ومقدارا ومعنى % % فيا لله من حسن حلي % 2(@)(@@) % كانكم الثلاثة ضرب خيط % % علي في علي في علي % PageV01P2346 البحر : - 1(@)(@@) % يا سيدي دعوة من نفسه % % محصورة في بيدق الحاشيه % % 2(@)(@@) % ينهي إلى همتك المشتكى % % وانما يشكو إلى العاليه % % PageV01P2347 البحر : - 1(@)(@@) % أصبحت من بعد خمولي الذي % % قد كان مسموما ومرئيا % % 2(@)(@@) % أعمل في الأيام ما أشتهي % % لأنني أصبحت بدريا % % PageV01P2348 البحر : - 1(@)(@@) % رأينا تواقيع تاج الزمان % % و فيها من الفضل معنى جلي % 2(@)(@@) % بنسك وجود وحفظ أجاد % % فقلت الثلاثة حظ الولي % PageV01P2349 البحر : - 1(@)(@@) % تهن بعوده عيدا سعيدا % و عش ما شئت يا كهف البرايا % % 2(@)(@@) % نحرت به جميع عداك فانحر % % قرونا آخرين من الضحايا % % PageV01P2350 البحر : - 1(@)(@@) % رب مولى مال عني بعد ما % % كان بالإحسان ميالا الي % 2(@)(@@) % فاضل سلمت في الدهر له % % ليته سلم في الحين علي % PageV01P2351 البحر : - 1(@)(@@) % برغمي أن أهاديكم بمعنى % % دقيق في مقابلة العطايا % % 2(@)(@@) % فيا خجلي ويا عتبي لدهر % % اذا وصل الدقيق إلى الهدايا % % PageV01P2352 البحر : - 1(@)(@@) % بأبي أنت حلوة الريق لكن % % أنا من لسعة الجفا في بليه % % 2(@)(@@) % فيك شهد وفيك لسع فرفقا % بشجي أمسى وأنت خليه % % PageV01P2353 البحر : - 1(@)(@@) % فديتك أيها الرامي بقوس % % ولحظ يا ضنا جسدي عليه % % 2(@)(@@) % لقوسك نحو حاجبك انجذاب % % وشبه الشيء منجذب اليه % % PageV01P2354 البحر : - 1(@)(@@) % أخا الخصر الدقيق فدتك روحي % % نعم وفدت ملاحتك البرايا % % 2(@)(@@) % عسى تهديه لي ضما ومن لي % % بأن يصل الدقيق إلى الهدايا % % PageV01P2355 البحر : - 1(@)(@@) % قالوا وقد 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms449# زدتني برا وتكرمة % % يا خير من لندى كف أناديه % % 2(@)(@@) % ماذا قبضت نهار العشر قلت لهم % % قبضت ميقات موسى من أياديه % % PageV01P2356 البحر : - 1(@)(@@) % بمقدمك السعيد قد استنارت % % دمشق وبشرت بسنا علي % % 2(@)(@@) % وقد كانت إلى الوسمي تهفو % % فأغناها الولي عن الولي % % PageV01P2357 البحر : - 1(@)(@@) % حيينا فإنا في رضى حبهم متنا % % وصح لقانا بالغيوب فما غبنا % % 2(@)(@@) % وقلنا وقد جاء البشير فبشرنا % % أحبتنا صدوا وقد علموا أنا % % 3(@)(@@) % متى ما بعدنا عن جنابهم عدنا % % 4(@)(@@) % بعدنا عيانا والقلوب على المنى % % منى القلب لا تخلوا لديها من الجنا % % 5(@)(@@) % فيا حبذا الأحباب والبين بيننا % % منعنا جناهم فاغتذينا بأننا % % 6(@)(@@) % مدى الدهر ما لذنا بغير ولا عذنا % % 7(@)(@@) % لهم نعم ملء الأيادي مباحة % لها راحتا جود وللبحرراحة % 8(@)(@@) % و مهما عرتنا من صدود إجاحة % لنا برحاء القرب في البعد راحة % 9(@)(@@) % و قد مسنا ضر فكيف ولو أنا % % 10(@)(@@) % سقى جفني البسام سفح المقطم % % وحام عليها نوء دمع ومهزم % % PageV01P2358 11(@)(@@) % فكم في حماهم من شجي القلب مغرم % % وكم في ذراهم من مشوق متيم % % 12(@)(@@) % يود دنو الحين منه اذا حنا % % 13(@)(@@) % و كم مستهام صادح بحنينه % % دفين الاسى يبكي لأجل دفينه % % 14(@)(@@) % و كم ذي بكى يروي عن ابن معينه % % وكم ذي سقام شعر بأنينه % % 15(@)(@@) % و ما شعروا من ضعفه أنه أنا % % 16(@)(@@) % و كم ثم من أغصان غيد ثنيننا % % إلى العهد لا تلوي من الوعد بيننا % % 17(@)(@@) % و رب ظبا عارضننا ورميننا % % وأعين عين رعننا ورعيننا % % 18(@)(@@) % بما أخذت منا وما صرفت عنا % % 19(@)(@@) % علون وأظهرن الجمال مثابة % % تخال لها عند الشموس قرابة % % 20(@)(@@) % و لم تبق من أرواح قوم صبابة % % تجافيننا حتى فتنا صبابة % % PageV01P2359 21(@)(@@) % و لا طفننا حتى سلمنا وما كدنا % % 22(@)(@@) % يجن سواد الليل لي بعد قربكم % % ويضحى نهاري باسما عند عتبكم % % 23(@)(@@) % فلله ليل ما أجن لصبكم % % سلوا إن شككتم في جنوني بحبكم % % 24(@)(@@) % نهاري إذا أضحى وليلي إذا جنى % % 25(@)(@@) % نهاري بأخبار الرضا يتبسم % % وليلي إلى روح الرجا يتنسم % % 26(@)(@@) % و جوهر روحي منكم يتقسم % % تبشرني الألطاف بالقرب منكم % % 27(@)(@@) % فصدري ما أفضى وعيشي ما أهنا % % 28(@)(@@) % و ما أحسن الدنيا نعيما ومنسكا % بدولة سلطان محا شكو من 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms450# شكا % % 29(@)(@@) % بمطلب جود لم يخف منه مهلكا % فسهل للدنيا وللدين مسلكا % % 30(@)(@@) % و أسبل أذيال النجاح فأسبلنا % % PageV01P2360 31(@)(@@) % فيارب أيد دولة الملك الذي % % روى حسن الأوصاف عن عرفها الشذي % % 32(@)(@@) % لقد أخذت في ملكها خير مأخذ % % بسهمي ثناء أو دعاء منفذ % % 33(@)(@@) % ترى الفوز منه قاب قوسين أو أدنى % % 34(@)(@@) % مليك وجدنا بابه الرحب معدنا % % لكسب الثنا والأجر والملك موطنا % % 35(@)(@@) % فجاء الرجا من كل ناحية بنا % % وفاضت بحور الشعر بالمدح والهنا % % 36(@)(@@) % على بابه حتى سبحنا وسبحنا % % 37(@)(@@) % وزدنا به من رائق العيش صفوه % % وجوز من بعد التحرج زهوه % % 38(@)(@@) % و لما رأينا الجد بالجود لهوه % % ركبنا المطايا والسوانح نحوه % % 39(@)(@@) % فيا بحر قد صارت سوابحنا سفنا % % 40(@)(@@) % جرين بنا كالسفن جري السوابح % % إلى باب قصر سافر النجح سافح % % PageV01P2361 41(@)(@@) % سوائر من غاد اليه ورائح % % عمرنا وعمرنا بيوت المدائح % % 42(@)(@@) % فلله حسنى ما عمرنا وعمرنا % % 43(@)(@@) % مليك له في اسم وفعل بنصره % % عوائد من سر الجميل وجهره % % 44(@)(@@) % و لما نصرنا في الحروب بذكره % % قصرنا على كسب الغنى باب قصره % % 45(@)(@@) % فيا حبذا القصر المشيد والمغنى % % 46(@)(@@) % لنا ملك قد كمل الله فضله % % فخوله ملك البسيطة كله % % 47(@)(@@) % بجد وجمع جمع الفضل شمله % % هو البحر إلا أننا سمك له % % 48(@)(@@) % بلقياه نحيى أو بفرقته نفنى % % 49(@)(@@) % مباديه في العلياء غايات من مضى % % من الحائزين الملك يعنو له القضا % % 50(@)(@@) % له صارم عزم وحزم قد انتضى % % فكم حاكم بالعدل في وصفه رضا % % PageV01P2362 51(@)(@@) % و كم معرب يبني وكم شرف يبنى % % 52(@)(@@) % يحق لشعري أن يطيش نباته % % سرورا بسلطان وفت لي صلاته % % 53(@)(@@) % و مدح تسامت كل يوم رواته % % إلى روض قول باكرت زهراته % % 54(@)(@@) % و أعذره لو طاش والانس والجنا % % 55(@)(@@) % لذكرك يا أوفى الملوك الأكارم % % عفا طلل من ذكر معن وحاتم % % 56(@)(@@) % كأنك عنهم قد ختمت بخاتم % % فحاتم طي ما له بشر باسم % % 57(@)(@@) % و معن فلا لفظ يحس ولا معنى % % 58(@)(@@) % لعمري لو كانوا نجوما ترفعت % % وأحملها ضوء الصباح فأقلعت % % 59(@)(@@) % ممدحة يوم النوال تورعت % % وكانوا بحارا في زمان توزعت % % 60(@)(@@) % ندامى كأنا في أحاديثهم خضنا % % PageV01P2363 61(@)(@@) % إلى أن تجلت طلعة ناصرية % % جلت دولة من ملكها قاهرية % % 62(@)(@@) % ملية أبيات العطا 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms451# قادرية % % وكان عطا معن القرى نادرية % % 63(@)(@@) % و أنت القرى أعطيت والكنز والمدنا % % 64(@)(@@) % فلا زال للإسلام ملكا وناصرا % وللمال والأعدا مبيدا وقاهرا % 65(@)(@@) % ولازال كل الناس أصبح شاعرا % يقيم لوزني شعره البر وافرا % 66(@)(@@) % و ما كان ذو وفر يقيم له وزنا % % 67(@)(@@) % و حقك لا أنسى ببابك ثروتي % % مرتبة في حال ضعفي وقوتي % % 68(@)(@@) % و لا قلت ما قال ابن جرح لعسرتي % % أذو صنعة فاستخدموني لصنعتي % % 69(@)(@@) % برزقي وإلا فارزقوني مع الزمنى % % PageV01P2364 البحر : - 1(@)(@@) % صرفت فعلي في الأسى وقولي % % بحمد ذي الطول الشديد الحول % % 2(@)(@@) % يالائما ملامه يطول % % إسمع هديت الرشد ما أقول % % 3(@)(@@) % كلامك الفاسد لست أتبع % % حد الكلام ما أفاد المستمع % % 4(@)(@@) % أفدي غزالا مثلوا جماله % % في مثل قد أقبلت الغزاله % % 5(@)(@@) % ما قال مذ ملك قلبي واسترق % كقولهم رب غلام لي أبق % % 6(@)(@@) % للقمرين وجهه مطالع % % فهي ثلاث ما لهن رابع % % 7(@)(@@) % لأحرف الحسن على خديه خط % % وقال قوم انها اللام فقط % % 8(@)(@@) % داني المزار يحذر الضنين % % عليه مثل بان أو يبين % % 9(@)(@@) % كتمته والحسن ليس يجتلى % % والاسم لا يدخله من وإلى % % 10(@)(@@) % منفرد بالحب في دار الهنا % % مثاله الدار وزيد وأنا % % PageV01P2365 11(@)(@@) % لا يختشي ملاعب الظنون % % والأمر مبني على السكون % % 12(@)(@@) % في خده التبري هان نشبي % % وقيمة الفضة دون الذهب % % 13(@)(@@) % فاصرف عليها ثروة تستام % % فما على صارفها ملام % % 14(@)(@@) % و انفق له دينار من ضن وشح % % ولا تبل أخف وزنا أم رجح % % 15(@)(@@) % و إن رأيت قده العالي فصف % % وقف على المنصوب منه بالألف % % 16(@)(@@) % و العارض النوني ما أنصفته % % وان تكن باللام قد عرفته % % 17(@)(@@) % في مثله انظم ان نظمت محسنا % % وإن ذكرت فاعلا منونا % % 18(@)(@@) % واها لها بحرف نون قد عرف % % كمثل ما تكتبه لا يختلف % % 19(@)(@@) % يأتي بنقط الخال في إعجام % % وتارة يأتي بمعنى اللام % % 20(@)(@@) % دونك ان عشقته بين الورى % % معظما لقدره مكبرا % % PageV01P2366 21(@)(@@) % و ان ترد وجنته المنيره % % فصغر النار على نويره % % 22(@)(@@) % كم ومتى جادلت فيه من عذل % % ولا وحتى ثم أو وأم وبل % % 23(@)(@@) % حتى تولت أوجه العذال % % وأقبل الغلام كالغزال % % 24(@)(@@) % للحظه المسكر فعل يطرب % % مفعوله مثل سقي ويشرب % % 25(@)(@@) % فلا تلم عويشقا فيه تلف % % ولا سكيران 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms452# الذي لا ينصرف % % 26(@)(@@) % لا تلح قلبي في الهوى فتتعبا % % وما عليك عتبه فتعتبا % % 27(@)(@@) % جسمي وذاك الخصر والجفن الدنف % % هن حروف الاعتدال المكتنف % % 28(@)(@@) % بجفنه نادى الهوى يا للشجي % % وكل ياء بعد مكسور تجي % % 29(@)(@@) % يا جفنه الناصب فيه فكري % % ونصبه وجره بالكسر % % 30(@)(@@) % إن قيل للظبي هنا إلمام % % فاكسر وقل ليقم الغلام % % PageV01P2367 31(@)(@@) % و يا مليحا عنه أخرت القمر % % إما لتهوان وإما لصغر % % 32(@)(@@) % كرر فما أحلى لسمعي السامي % % قولك يا غلام يا غلامي % % 33(@)(@@) % و ارفق بمضناك فما سوى اسمه % % و لا لغير ما بقى من رسمه % % 34(@)(@@) % فقد حكى العداة بالوقوف % % فاعطف على سائلك الضعيف % % 35(@)(@@) % أفقرت في الحسن الغواني مثلما % % قالوا حذامي وقطامي في الدما % % 36(@)(@@) % فافخر بمعنى لحظك المعشوق % % في كل ما تأنيثه حقيقي % % 37(@)(@@) % يالك لحظا بسعاد أزرى % % وجاء في الوزن مثال سكرى % % 38(@)(@@) % حتى اسمه منتقص لمن وعى % % كما يقال في سعاد يا سعا % % 39(@)(@@) % يا واصفا أوصاف ذياك الصبا % % تم الكلام عنده فلينصبا % % 40(@)(@@) % هيهات بل دع عنك ما أضنى وما % % وعاص سباب الهوى لتسلما % % PageV01P2368 41(@)(@@) % و حبر الأمداح في علي % قاضي القضاة الطاهر التقي % 42(@)(@@) % بكل معنى قد تناها واستوى % % في كلم شتى رواها من روى % % 43(@)(@@) % باكر إلى ذاك الحمى العالي وصف % % اذا درجت قائلا ولم تقف % % 44(@)(@@) % دونك والمدح ذكيا معجبا % نحو لقيت القاضي المهذبا % % 45(@)(@@) % ذو الجود والعلم عليه أرسى % % وهكذا أصبح ثم أمسى % % 46(@)(@@) % فاضرع إلى قار لقاء نافع % % واقرع إلى حامي حماه المانع % % 47(@)(@@) % يقول للضيف نداه حب وهل % % ومثله ادخل وانبسط واشرب وكل % % 48(@)(@@) % إذا ظفرت عنده بموعد % % يقول كم مال أفادته يدي % % 49(@)(@@) % له يراع كم له في خطره % % حماية منظومة مع دره % % 50(@)(@@) % في الجود واليأس وفي العلم وفي % % ذلك منسوب اليه فاعرف % % PageV01P2369 51(@)(@@) % فقولهم أبيض في الهبات % % كقولهم أحمر في الصفات % % 52(@)(@@) % شم حده يوم الندى والبأس % % فإنه ماض بغير لبس % % 53(@)(@@) % لله ما ألينه عند العطا % % وما أجد سيفه حين السطا % % 54(@)(@@) % يهزه ذو الرفع في العلاء % % والجزم في الفعل بلا امتراء % % 55(@)(@@) % حبر له يثني الثناء قصده % % وخلفه وإثره وعنده % % 56(@)(@@) % إن قال قولا بين الغرائبا % % وقام قس في عكاظ 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms453# خاطبا % % 57(@)(@@) % وان سخا أتى على ذي العدد % والكيل والوزن ومذروع اليد % 58(@)(@@) % معطل السمع من العذال % % فحاله مغير بحال % % 59(@)(@@) % الفضل جنس بيته المهنى % % ونوعه الذي عليه يبنى % % 60(@)(@@) % سام به أهل العلى جميعا % وادفع ولا ردا ولا تفريعا % % PageV01P2370 61(@)(@@) % وإن ذكرت أفق بيت قد نما % % فانصب وقل كم كوكبا يحوي السما % % 62(@)(@@) % بيت نظيم المجد والعلاء % % عند جميع العرب العرباء % % 63(@)(@@) % يقر من يأتي له أو اقترب % % وكل منسوب إلى اسم في العرب % % 64(@)(@@) % تقول مصر في علاه الواجبه % % كقول سكان الحجاز قاطبه % % 65(@)(@@) % أبنية الأنصار طلاع الفنن % % و زاد مبنى حسنه أبو الحسن % % 66(@)(@@) % جار إذا ما امتدت الأيادي % % تقول هذا طلحة الجواد % % 67(@)(@@) % اذا اجتليت في العطا جبينه % % أو استشرت للرجا يمينه % % 68(@)(@@) % تقول قد خلت الهلال لائحا % وقد وجدت المستشار ناصحا % % 69(@)(@@) % كم بالغنى عنه تولى راحل % % وواقف بالباب أضحى السائل % % 70(@)(@@) % فياض سيب في الورى فلم يقل % % في هبة يا هب من هذا الرجل % % PageV01P2371 71(@)(@@) % قال له الشرع امض ما تحاوله % % واقض قضاء لا يرد قائله % % 72(@)(@@) % وأنت يا قاصده سر في جدد % % واسع إلى الخيرات لقيت الرشد % % 73(@)(@@) % إن تكتحل سناه تلقى الرشدا % % وأين ما تذهب تلاق سعدا % % 74(@)(@@) % فافخر به سحب الحيا إن صابا % % واستوت المياه والأخشابا % % 75(@)(@@) % ولا تقل كان غماما ورحل % % كان وما انفك الفتى ولم يزل % % 76(@)(@@) % باب سواه اهجر عداك عيب % وصغر الباب فقل بويب % 77(@)(@@) % هذا الذي يفعل فينا الطولا % % فقدم الفاعل فهو أولى % % 78(@)(@@) % جود به أنسى أحاديث المطر % % فليس يحتاج لها إلى خبر % % 79(@)(@@) % مثل الهبا فيه كلام العذل % % والريح تلقاء الحيا المنهل % % 80(@)(@@) % وبحر شعر خضته لذكره % % وغصت في البحر ابتغاء دره % % PageV01P2372 81(@)(@@) % حنى ملا عيني نداه عينا % % وطبت نفسا إذ قضيت دينا % % 82(@)(@@) % دونكها معسولة الآداب % % ممزوجة بلمحة الاعراب % % 83(@)(@@) % مضى بها الليل مضي الأنجم % % وبات زيد ساهرا لم ينم % % 84(@)(@@) % فافتح لها باب قبول يجتلى % % وان تجد عيبا فسد الخللا % % 85(@)(@@) % لازلت مسموع الثنا ذا منن % جائلة دائرة في الألسن % % 86(@)(@@) % ما لعداك راية تقام % % وليس غير الكسر والسلام % PageV01P2373 البحر : - 1(@)(@@) % أثنى شذا الروض على فضل السحب % % واشتملت بالوشي أرداف الكثب % % 2(@)(@@) % ما بين 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms454# نور مسفراللثام % % وزهر يضحك في الأكمام % % 3(@)(@@) % إن كانت الأرض لها ذخائر % % فهي لعمري هذه الأزاهر % % 4(@)(@@) % قد بسطتها راحة الغمائم % % بسط الدنانير على الدراهم % % 5(@)(@@) % أحسن بوجه الزمن الوسيم % % تعرف فيه نضرة النعيم % % 6(@)(@@) % وحبذا وادي حماة الرحب % % حيث زهى العيش به والعشب % % 7(@)(@@) % أرض السناء والهناء والمرح % % والأمن واليمن ورايات الفرح % % 8(@)(@@) % ذات النواعير سقاة الترب % وأمهات عصفه والأب % % 9(@)(@@) % تعلمت نوح الحمام الهتف % % أيام كانت ذات فرع أهيف % 10(@)(@@) % فكلها من الحنين قلب % لاسيما والماء فيها صب % % PageV01P2374 11(@)(@@) % لله ذاك السفح والوادي الغرد % % والماء معسول الرضاب مطرد % % 12(@)(@@) % يصبو لها الرآئي ويهفو السامع % % ويحمد العاصي فكيف الطائع % % 13(@)(@@) % إذا نظرت للربا والنهر % % فارو عن الربيع أو عن جعفر % % 14(@)(@@) % محاسن تلهي العيون والفكر % % ربيع روضات وشحرور صفر % % 15(@)(@@) % أمام كل منزل بستان % % وبين كل قرية ميدان % % 16(@)(@@) % أما رأيت الورق في الأوراق % % جاذبة القلوب بالأطواق % % 17(@)(@@) % فبادر اللذة يا فلان % % واغنم متى أمكنك الزمان % 18(@)(@@) % و لا تقل مشتى ولا مصيف % فكل وقت للهنا شريف % 19(@)(@@) % كل زمان يتقضى بالجذل % % زمان عيش كيفما دار اعتدل % % 20(@)(@@) % أحسن ما أذكر من أوقاته % % وخير ماأبعث من لذاته % % PageV01P2375 21(@)(@@) % بروزنا للصيد فيه والقنص % % وحورنا من مره أحلى الفرص % % 22(@)(@@) % و أخذنا الوحش من المسارب % % وفعلنا في الطير فوق الواجب % % 23(@)(@@) % لما دنا زمان رمي البندق % % سرنا على وجه السرور المشرق % % 24(@)(@@) % في عصبة عادلة في الحكم % % وغلمة مثل بدور التم % 25(@)(@@) % من كل مبعوث إلى الأطيار % % تظله غمامة الغبار % 26(@)(@@) % و كل معسول الشباب أغيد % % منعطف عطف القضيب الأملد % % 27(@)(@@) % قد حمد القوم به عقبى السفر % % عند اقتران القوس منه بالقمر % % 28(@)(@@) % لولا حذار القوس في يديه % % لغنت الورق على عطفيه % % 29(@)(@@) % في كفه محنية الاوصال % % قاطعة الاعمار كالهلال % % 30(@)(@@) % زهراء خضراء الاهاب معجبه % % مما ثوت بين الرياض المعشبه % % PageV01P2376 31(@)(@@) % فاغرة الافواه للاطيار % % طالبة لهن بالأوتار % % 32(@)(@@) % كأنها حول المياه نون % % أو حاجب بما تشا مقرون % % 33(@)(@@) % لها نبات بالمنى مغدوقة % % من طيبة واحدة مخلوقة % % 34(@)(@@) % سامعة لما تشير الأم % % مع أنها مثل الحجار صم % 35(@)(@@) % واها لها من شهب تخطف % % شاهرة بالعزم وهي تقذف % % 36(@)(@@) % كأنها والطير منها هارب % % خلف الشياطين شهاب ثاقب 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms455# % % 37(@)(@@) % حتى نزلنا بمكان مونق % % اخوان صدق أحدقوا بالملق % % 38(@)(@@) % فياله في الحسن من محل % % مراد جد ومراد هزل % % 39(@)(@@) % للطير في مياهه مواقع % % كأنها من فوقه فواقع % % 40(@)(@@) % فلم نزل في منزل كريم % % نروي حديث الرمي عن قديم % % PageV01P2377 41(@)(@@) % حتى طوى الافق رداء الورس % % والتقم المغرب قرص الشمس % % 42(@)(@@) % و ذر مسك الليل في فرق الافق % % واتشحت خود السماء بالنطق % % 43(@)(@@) % وابتدر القوم إلى المراصد % % من ساهر الليل التمام ساهد % % 44(@)(@@) % بينا الطيور في مداها سائره % % اذا هم من عينه بالساهره % % 45(@)(@@) % كالليث يسطو كفه بأرقم % % والبدر يرمي في الدجى بأنجم % % 46(@)(@@) % و أقبلت مواكب الطيور % % على طروس الجو كالسطور % % 47(@)(@@) % فحبذا السطورفي المهارق % % منقوطة الاحرف بالبنادق % % 48(@)(@@) % من كل تم حق أن يسمي % % ضياءه المشرق بدر التم % % 49(@)(@@) % تخاله من تحت عنق قد سجا % % طرة صبح تحت أذيال الدجى % % 50(@)(@@) % و كل حي حسن الوسامه % % كأنه في أفقه غمامه % % PageV01P2378 51(@)(@@) % تتبعه أوزة دكناء % % من دونها لفلفة غراء % % 52(@)(@@) % تقدمها أنيسة ملونه % % تابعة من كل وصف أحسنه % % 53(@)(@@) % يجني بها الآكل خير ما جنى % % وأحسن المأكول ما تلونا % % 54(@)(@@) % و ربما مر لديها حبرج % % كأنه على نضار يدرج % % 55(@)(@@) % و انقض من بعض الجبال النسر % % له بأبراج النجوم وكر % 56(@)(@@) % مغبر الخلق شديد الأيدي % % يبني على الكسر حروف الصيد % % 57(@)(@@) % و كل كركي عجيب السير % % كأنه طيف خيال الطير % % 58(@)(@@) % ما بين أحشاء الظلام يسري % % من أرض بغداد لأرض مصر % % 59(@)(@@) % يحث مسراه عقاب كاسبه % % خافضة لحظ الطيور ناصبه % % 60(@)(@@) % إذا مضت جملتها المعترضه % % تواصلت خيوطها المنقرضه % % PageV01P2379 61(@)(@@) % و أبيض الغيم يسمى مرزما % % كم بات مثل نوئه منسجما % % 62(@)(@@) % يحث غرنوقا شهي المجتلى % % مقدما على الغرانيق العلى % % 63(@)(@@) % و كل صوع مبهت المفاجي % % كالبرق يخطو فوق ليل داجي % % 64(@)(@@) % و أبيض مثل الغمام يسجم % % وكيف لا يسجم وهو مرزم % 65(@)(@@) % يحفه شبيطر قوي % في ملة الأطيار موسوي % 66(@)(@@) % هذا وكم ذي نظر ممتاز % % ينعت في الواجب بالعناز % % 67(@)(@@) % اسوده ذو غرة في الصدر % % كأنه نور الهدى في الكفر % 68(@)(@@) % فلم تزل قسينا الضواري % % تصيبها بأعين النظار % % 69(@)(@@) % حتى غدت دامية النحور % % ساقطة منها على الخبير % % 70(@)(@@) % كأنها وهي لدينا وقع % % لدى محاريب القسي 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms456# ركع % PageV01P2380 71(@)(@@) % و أصبحت أطيارنا قد حصلت % % فلا تسل بأي ذنب قتلت % % 72(@)(@@) % مستتبعا وجه العشا وجه السحر % % وكل وجه منهما وجه أغر % % 73(@)(@@) % يالك من صيد مقر العين % % يرضي الصحاب وهو ذو وجهين % % 74(@)(@@) % لم نرض ما وفى من الأماني % % حتى شفعناه بصيد ثاني % % 75(@)(@@) % صيد الملوك الصيد بالكواسر % % والخيل في وجه الصباح السافر % % 76(@)(@@) % ذاك الذي تصبو له الجوارح % % فهي إلى طلابه طوامح % % 77(@)(@@) % واثقة بالرزق حيث كانا % % تغدو خماصا وتجي بطانا % % 78(@)(@@) % سرنا على اسم الله والمناجح % % نعوم في الأقطار بالسوابح % % 79(@)(@@) % خيل تحاذي الصيد حيث مالا % % كأنها أضحت له ظلالا % % 80(@)(@@) % تسعى لها قوائم لا تتبع % % وكيف لا وهي الرياح الأربع % % PageV01P2381 81(@)(@@) % رائقة المنظر زهراء الغرر % % كأنها الروضات حيت بالزهر % % 82(@)(@@) % من أحمر للبرق عنه خبر % % يشهد أن الحسن حقا أحمر % 83(@)(@@) % و أصفر الجلدة كالدينار % % يسر كف الصائد الممتار % % 84(@)(@@) % و أشهب كالسهم في انقضاضه % % وصفحة الطرس في ابيضاضه % % 85(@)(@@) % ماضي السباق أظهر اللباس % % ناهيك من سهم ومن قرطاس % % 86(@)(@@) % و أخضر مثل سنا العيش النضر % % يطوى الفلا وكيف لا وهو الخضر % % 87(@)(@@) % و أدهم ساد على الجياد % وهكذا السواد في السواد % % 88(@)(@@) % تحفنا من فوقها غلمان % % كأنهم لدوحها أغصان % % 89(@)(@@) % ترك تريك في سناء الملبس % % كواكبا طالعة في الأطلس % 90(@)(@@) % منظومة الأوساط بالسلاح % % من كل سهم رجل الجناح % % PageV01P2382 91(@)(@@) % و كل عضب ذرب المقاطع % % يحرف الهام عن المواضع % % 92(@)(@@) % على يد الزائر منهم زاده % % من كل باز قرم فؤاده % % 93(@)(@@) % قد كتبت في شكله حروف % % تقري بما يقرى به الضيوف % % 94(@)(@@) % فالمنسر الأشفى بحال جيما % % والعين تجلى بالنضار ميما % % 95(@)(@@) % دان لمن يتلوه خير جم % سهم إذا حبرته أو شهم % % 96(@)(@@) % و كل شاهين شهي المرتمى % % كبارق طار وصوب قد همى % % 97(@)(@@) % بينا تراه ذاهبا لصيده % % معتصما بأيده وكيده % % 98(@)(@@) % حتى تراه عائدا من أفقه % % ملتزما طائره في عنقه % % 99(@)(@@) % أفلح من كان على يسراه % % حتى غدت حاسدة يمناه % % 100(@)(@@) % تلك يد لا تعرف الاعسارا % % لأجل ذا قد سميت يسارا % % PageV01P2383 101(@)(@@) % و كل صقر مسبل الجناح % % مواصل الغدو والرواح % % 102(@)(@@) % ذو مقلة لها ضرام واقد % % تكاد تشوي ما يصيد الصائد % % 103(@)(@@) % كأنما المخلب منه منجل % % لحصد أعمار 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms457# الطيور مرسل % % 104(@)(@@) % عيش ذوي الصيد به عيش رخي % % يصلح أن يدعى وكيل المطبخ % % 105(@)(@@) % يا حبذا طيور جد ولعب % % تهوي إلى الأرض وللأفق تثب % % 106(@)(@@) % من سنقر عالي المدا والشان % % معظم الأخبار والعيان % % 107(@)(@@) % كأنه خليفة قد أقدما % % يفسد الأرض ويسفك الدما % % 108(@)(@@) % يصعد خلف الرزق ليس يمهله % % كأنه من السما يستعجله % % 109(@)(@@) % و من عقاب بأسها مروع % % كأنها للطير جن تفزع % % 110(@)(@@) % كم جلبت لطائر من مهن % % وكم وكم قد أهلكت من قرن % PageV01P2384 111(@)(@@) % و حبذا كواسر الكواهي % % عديمة الأنظار والأشباه % % 112(@)(@@) % مخصومة بالطرد القويم % % حدبا كظهر الذنب الركيم % % 113(@)(@@) % ذاك لعمري حدب للرائي % % يعدل ملك القلعة الحدباء % % 114(@)(@@) % هذا وقد تجهزت أعداد % تجمعها الكلاب والفهاد % % 115(@)(@@) % من كل فهد عنتري الحمله % % اذا رأى شخص مهاة عبله % % 116(@)(@@) % مبارك الإقبال والإعراض % % مستقبل الحال بناب ماض % % 117(@)(@@) % كأنه من حده كنابه % % قد أحرق الأنجم في إهابه % % 118(@)(@@) % له على مسائل الجفون % % خط لبعض الألفات الجون % % 119(@)(@@) % ما أبصر المبصر خطا مثله % % وكيف لا والخط لابن مقله % % 120(@)(@@) % و كل منسوب إلى سلوق % % أهرت وثاب الخطا مشوق % % PageV01P2385 121(@)(@@) % طاوي الفؤاد ناشر الأظافر % % يا عجبا منه لطاو ناشر % % 122(@)(@@) % يعض بالبيض ويخطو بالقنا % % ويسبق الوهم لإدراك المنى % % 123(@)(@@) % كالقوس إلا أنه كالسهم % % والغيم يجلو عن شهاب رجم % % 124(@)(@@) % اذا ترآى بقر الوحش اندفع % % كأنه المريخ في الثور طلع % % 125(@)(@@) % قاصرة عن طرفه يداه % % مشروطة برجله أذناه % % 126(@)(@@) % لو أمكن الشمس التي تجلى له % % ما سميت من خوفها غزاله % % 127(@)(@@) % يشفعه بكل غور غار % % مغالب الصيد على الأوكار % % 128(@)(@@) % يكاد يبغي سلما إلى السما % % أو نفقا في الأرض حيث يمما % % 129(@)(@@) % واها لها من أكلب طوارد % % معربة عن مضمر المصائد % % 130(@)(@@) % قد بالغت من طمع في كسبها % % ففتشت عن أنفس لم تخبها % % PageV01P2386 131(@)(@@) % حتى اذا تمت بها الأمور % % حفت بنا لصيدها الطيور % % 132(@)(@@) % ما بين روضات صمدنا نحوها % % ودور آفاق ملكنا جوها % % 133(@)(@@) % و استقبلت أطيارها البزاة % % معلمة كأنها عزاة % % 134(@)(@@) % فلم تزل تسطو سطا الحجاج % % على الكراكي أو الدراج % % 135(@)(@@) % اذا نحت سائرة محلقة % % عادت بها كمضغة مخلقة % % 136(@)(@@) % حتى غدت تلك الضواري صرعى % % مجموعة لدى التراب جمعا % % 137(@)(@@) % كأن أقطار الفلاة مجزره % % أو روضة 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms458# من الدماء مزهره % % 138(@)(@@) % كأن صرعى وحشها كفار % % الموت عقبى أمرها والنار % % 139(@)(@@) % للمرء فيها منظر أحبه % % يملأ من لحم وشحم قلبه % % 140(@)(@@) % لله ذاك المنظر المهنى % % إن معان عن ذراه عدنا % % PageV01P2387 141(@)(@@) % قد ملئت من ظفر أيدينا % % وقد شكرنا الفضل ما حيينا % % 142(@)(@@) % نشير حول الملك المنصور % % كالشهب حول القمر المنير % % 143(@)(@@) % محمد ناصر دين أحمد % % الملك ابن الملك المؤيد % % 144(@)(@@) % قال الأنام حظه جلي % % قلت نعم وجده علي % 145(@)(@@) % ذاك الذي سامى العلى صبيا % % وجاءه من مهده مهديا % % 146(@)(@@) % ناش على الحر وتقليب المنن % % كأنما مزجته من اللبن % % 147(@)(@@) % بين حجور العلم والاعلام % % تكنفه لواحظ الأقلام % % 148(@)(@@) % محكم السطوة سحاح الديم % % يأخذ بالسيف ويعطي بالقلم % % 149(@)(@@) % لو لمس الصخر لفاض نهرا % أو صحب النجم لعاد بدرا % % 150(@)(@@) % تختمت بيمنه المكارم % فهو على كل الوجوه حاتم % PageV01P2388 151(@)(@@) % لا ظلم تلقى في حماه العالي % % إلا على الأعداء والأموال % % 152(@)(@@) % أما ترى بالصيد فرط حبه % % تمرنا على اعتياد حربه % % 153(@)(@@) % اما ترى الدينار منه خائفا % أصفر في كف العفاة ناشفا % % 154(@)(@@) % يا قاطعا عرض الفلا وواصلا % وقادما يبغي العلى وراحلا % % 155(@)(@@) % إذا تأملت المقام الناصري % % فاعقد عليه أكرم الخناصر % % 156(@)(@@) % ملك إذا حققته قلت ملك % % قاضية بسعده أيدي الفلك % % 157(@)(@@) % كالبدر في سنائه وتمه % % والطود في وقاره وحلمه % % 158(@)(@@) % تسجد ان لاح رؤوس العالم % % وراثة قد حازها من آدم % % 159(@)(@@) % ماضر من خيم في جنابه % % أن لا يكون الشهد من أطنابه % % 160(@)(@@) % مرأى يشف عن فخار الأهل % % ونسخة قد قوبلت بالأصل % % PageV01P2389 161(@)(@@) % جنابه عن جاره لا ينكب % % وباب نجح للمنى مجرب % % 162(@)(@@) % غنيت في ظلاله عن الورى % % غنى نزيل المزن عن قصد القرى % % 163(@)(@@) % و رحت عن نعماه بالتواتر % % أروي أحاديث عطا وجابر % % 164(@)(@@) % معتصما بالكرم المؤيد % % مصلي الحمد على محمد % % 165(@)(@@) % قديم قصد وثناء أو هوى % % ما ضل سعي فيهما ولا غوى % % 166(@)(@@) % يزيد لفظي بهجة ورونقا % % كأنه الخمرة إذ تعتقا % % 167(@)(@@) % حسبك مني في الثناء شاعرا % % وحسب شعري قوة وناصرا % % PageV01P2390 البحر : - 1(@)(@@) % لهفي على غادة إذا أسفرت % % غارت وجوه الشموس واستترت % % 2(@)(@@) % لها من السمر قامة خطرت % % كم قتلت عاشقا وكم أسرت % % 3(@)(@@) % إذا دعت للنهوض ميلها عطفا % كان سحر الجفون حملها 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms459# ضعفا % % 4(@)(@@) % في خدها شامة معنبرة % يا نعمة بالشقيق مزهرة % 5(@)(@@) % وكم لها في الشفاه جوهرة % تحفها ريقة معطرة % 6(@)(@@) % من رام بالشهد أن يمثلها رشفا % % فإنما رام أن يعسلها وصفا % % 7(@)(@@) % تحكم في الناس عنسه وردا % % حكم ابن أيوب في سطا وندا % % 8(@)(@@) % بين عفاة له وبين عدا % % ما يد سميت لديه يدا % % 9(@)(@@) % وهي غمام لمن تأملها وطفا % % سبحان من للعباد أرسلها لطفا % % 10(@)(@@) % مؤيد في ملا مراتبه % يتضح الملك في مناقبه % PageV01P2391 11(@)(@@) % اذا طوى الأرض في كتائبه % ثم سقاها حيا مواهبه % 12(@)(@@) % أنبت أزهارها ودللها قطفا % % من بعد ما كاد أن يزلزلها خسفا % % 13(@)(@@) % وغادة حاد سحر مقلتها % % وراق للناس روض طلعتها % % 14(@)(@@) % جنيت نار الأسى بجنتها % % وصحت من صبوتي بوجنتها % % 15(@)(@@) % وجنة ورد تشكو النفوس لها لهفا % % بياض من شملها وقبلها ألفا % % PageV01P2392 البحر : - 1(@)(@@) % زحفت بيض الظبا لما رنا % % فتلقاها سريعا مقتلي % % 2(@)(@@) % عامري اللحظ طائي الفم % % 3(@)(@@) % بارز في حسنه كالصنم % % 4(@)(@@) % قلت والقلب اليه ينتمي % % 5(@)(@@) % لك قلبي عبد ود وأنا % % فيك يا أشهل عبد الأشهل % 6(@)(@@) % آه ما أكثر فيك المللا % % 7(@)(@@) % ما دنا شخصك حتى ارتحلا % % 8(@)(@@) % ودعا الحادي وشد الجملا % % 9(@)(@@) % فاستشار البين عندي فتنا % % وغدا يومي يوم الجمل % 10(@)(@@) % أترى يرجع عيشي الناعم % % PageV01P2393 11(@)(@@) % ومقامي بالحميا قائم % % 12(@)(@@) % والحيا بالبرق معط باسم % % 13(@)(@@) % كعماد الدين جماع الثنا % % أفضل الأمة نجل الأفضل % 14(@)(@@) % ملك عم الورى بالمنن % % 15(@)(@@) % وكفاهم مرتبات المحن % % 16(@)(@@) % طاهر الأسرار شهم العلن % % 17(@)(@@) % راقب الله وأسدى المننا % % فهو الوسمي فينا والولي % % 18(@)(@@) % كرم الأخلاق من مذهبه % % 19(@)(@@) % والعلا والجود من مطلبه % % 20(@)(@@) % يا أماني الوفد هنيت به % % PageV01P2394 21(@)(@@) % الندى حيث الهدى حيث الثنا % % فاجتدي أو فاجتني أو فاجتلي % % 22(@)(@@) % وفتاة أتمنى وصلها % % 23(@)(@@) % وهي لا تألف إلا بخلها % % 24(@)(@@) % بهواها يا رسولي قل لها % % 25(@)(@@) % عللي القلب بأرواح المنا % % وعدي الصب ودعي المطل % % PageV01P2395 البحر : - 1(@)(@@) % إلي بكأسك الأشهى إليا % % ولا تبخل بعسجدها عليا % % 2(@)(@@) % معتقة تدار على النداما % % 3(@)(@@) % كأن على ترائبها نظاما % % 4(@)(@@) % من الراح التي محت الظلاما % % 5(@)(@@) % أضاءت وهي صاعدة الحميا % % فقلت عصير عنقود الثريا % % 6(@)(@@) % أدرها بين ألحان وزمر % % 7(@)(@@) % على درين من زهر وقطر % % 8(@)(@@) % كأن حديثه في كل قطر % % 9(@)(@@) % حديث ندى المؤيد في يديا % % يطيب رواية 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms460# ويضوع ريا % % 10(@)(@@) % إلى الملك المؤيد سار مدحي % % PageV01P2396 11(@)(@@) % وخاض إلى حماه كل سمح % % 12(@)(@@) % كما خاض النجوم طلوب صبح % % 13(@)(@@) % فيا لندى طوى الأقطار طيا % % وأنشر حاتما عندي وطيا % % 14(@)(@@) % حلفت ببشرك الوضاح حقا % % 15(@)(@@) % لبق فقت الأنام علا وسبقا % % 16(@)(@@) % فرفقا يا فتى العلياء رفقا % % 17(@)(@@) % شويت جوانح القرناء شيا % % فليتك لو لطفت بهن شيا % % 18(@)(@@) % وغانية يجن بها الجنان % % 19(@)(@@) % يضوع اذا تنفست المكان % % 20(@)(@@) % خلوت بها وقد سمح الزمان % % PageV01P2397 21(@)(@@) % فألقيت الحيا عن منكبيا % % وغافلت الرقيب وقلت هيا % % PageV01P2398 البحر : - 1(@)(@@) % حشى من نار صدك ذائبه % % وتحسبها دموعا ساكبه % % 2(@)(@@) % ولم يفطن لها % % سوى صب أقام % % 3(@)(@@) % على فرش السقام % % 4(@)(@@) % درى ما قصتي فحاكى % % لوعتي وجارى عبرتي % % 5(@)(@@) % و بتنا كالحمائم في الحنين % % و ما يدري الحزين سوى الحزين % % 6(@)(@@) % سباني بالفتور وبالفنون % % 7(@)(@@) % غلام شاهر حد الجفون % % 8(@)(@@) % على وجناته لام ونون % % 9(@)(@@) % يقول وصال مثلي لن يكون % % 10(@)(@@) % فيالك من جفون ضاربه % % بأمثال السيوف القاضبه % % PageV01P2399 11(@)(@@) % اذا ما سلها % % أبادت في الأنام % % 12(@)(@@) % و يالك من غلام % % 13(@)(@@) % كحيل المقلة % % شريف الوجنة % 14(@)(@@) % ضنين العطفة % % 15(@)(@@) % بكيت دما بمرآه الضنين % % كأني فيه من عيني ظعين % % 16(@)(@@) % يعنفني النديم على التصابي % % 17(@)(@@) % و يحلف لا يذوق لمى الحباب % % 18(@)(@@) % رويدك كيف أسلو عن شراب % % 19(@)(@@) % و عن ساق يطوف على الصحاب % % 20(@)(@@) % بكأس للأنامل خاضبه % % تحل عرى النفوس التائبه % % PageV01P2400 21(@)(@@) % و تنقص حبلها % % فدع عنك الملام % % 22(@)(@@) % و بادر بالمدام % % 23(@)(@@) % زمان اللذة % % و خذ يا منيتي % % 24(@)(@@) % خضاب القهوة % % 25(@)(@@) % و لا تمدد إلى حلف يمين % % فما الخصيب كف من يمين % % 26(@)(@@) % لها وصلي ولابن علي قصدي % % 27(@)(@@) % تضيع ثروتي ونداه يجدي % % 28(@)(@@) % مليك طالع في كل حمد % % 29(@)(@@) % تكاد يمينه بالجود تعدي % % 30(@)(@@) % إلى تلك اليمين الواهبه % % تيمم كل نفس طالبه % % PageV01P2401 31(@)(@@) % و تأوي ظلها % % على غيظ الغمام % 32(@)(@@) % لدى عالم المقام % % 33(@)(@@) % رفيع النسبة % نسيب الرفعة % 34(@)(@@) % سعيد الطلعة % % 35(@)(@@) % أغاث ندى يديه المعتفين % % و أودى بأسه بالمعتدين % % 36(@)(@@) % بني أيوب حسبكم عمادا % % 37(@)(@@) % أعاد سناء بيتكم وزادا % % 38(@)(@@) % كريم كم قصدناه فجادا % % 39(@)(@@) % وعدنا قاصدين له فعادا % % 40(@)(@@) % ولاقينا لها متواثبه % % جوئزنا عليها واجبه % % PageV01P2402 41(@)(@@) % ففتحنا اللهى % % بأنواع الكلام % % 42(@)(@@) % كأسجاع الحمام % % 43(@)(@@) % فكم من منحة % محت من نزحة % 44(@)(@@) % وكم من مدحة % % 45(@)(@@) % لها في كل سامعة 
urn:cts:arabLit:0768IbnNubataShacir.Diwan.JK007636-ara1:ms461# رنين % % يكاد بلحنها يشدو الجنين % % 46(@)(@@) % ومشغوف اذا مالليل جنا % % 47(@)(@@) % تذكر وصل من يهوى فجنا % % 48(@)(@@) % كذا من يعشق الأجفان وسنا % % 49(@)(@@) % نهبن منام مقلته فعنا % % 50(@)(@@) % على صحب الجفون الناهبه % % متى تهدى الضلوع اللاهبه % % PageV01P2403 51(@)(@@) % تركتني لأجلها % % إذا جن الظلام % % 52(@)(@@) % جفا عيني المنام % % 53(@)(@@) % وهاجت حسرتي % % على تلك التي % % 54(@)(@@) % أباحت قتلتي % % 55(@)(@@) % وما في دولة الأحباب أمين % % فينظر في قلوب المسلمين % % PageV01P2404